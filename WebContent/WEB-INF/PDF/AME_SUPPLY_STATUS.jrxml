	<queryString><![CDATA[SELECT val.*,
  sch.SCH_NAME,supply.suupy_qty
FROM
  (SELECT YEAR_OF_COMMISSION ,
    DESIGN_PUMPING_HOURS,
    TOTAL_NO_HABITATION,
    sch_sno
  FROM PMS_AME_MST_SCH_DETAILS
  )val
JOIN
  ( SELECT SCH_SNO, SCH_NAME FROM PMS_SCH_MASTER
  ) sch
ON sch.sch_sno=val.sch_sno
left outer JOIN
  (SELECT SUM(QTY_OR_AMOUNT) / 1000 AS suupy_qty ,
    sch_sno
  FROM PMS_AME_TRN_SCH_PERFORM_MTH where PERFORM_DESC_SNO=2 and month=4 and year=2012
  GROUP BY sch_sno
  )supply
ON supply.sch_sno=val.sch_sno]]></queryString>

