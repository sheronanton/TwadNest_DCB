	<queryString><![CDATA[SELECT x.PMS_ASSET_GEN_SNO,
  x.OFFICE_ID,
  x.PROJECT_ID,
  x.SCH_SNO,
  x.ADMIN_SANCTION_SNO,
  x.YEAR_COMMISION,
  x.POP_PRESENT,
  x.POP_INTER,
  x.POP_ULTIMATE,
  x.SUPPLY_TP,
  x.SUPPLY_HAB,
  x.DAILY_REQ_INTER,
  x.DAILY_REQ_ULTIMATE,
  x.RATE_PUMP_INTER,
  x.RATE_PUMP_ULTIMATE,
  x.REMARKS,
  x.DAILY_REQ_PRESENT,
  x.PRESENT_YR,
  x.INTER_YR,
  x.ULTIMATE_YR,
  x.RATE_PUMP_PRESENT,
  x.SUPPLY_CORP,
  x.SUPPLY_MUN,
  x.HRS_PUMPING,
  y.ADMIN_SANCTION_SNO,
  y.SANCTION_REFNO,
  y.SCH_SNO,m.SCH_SNO,m.SCH_NAME,y.SANCTION_DATE,y.SANCTION_AMOUNT 
FROM
  (SELECT PMS_ASSET_GEN_SNO,
    OFFICE_ID,
    PROJECT_ID,
    SCH_SNO,
    ADMIN_SANCTION_SNO,
    YEAR_COMMISION,
    POP_PRESENT,
    POP_INTER,
    POP_ULTIMATE,
    SUPPLY_TP,
    SUPPLY_HAB,
    DAILY_REQ_INTER,
    DAILY_REQ_ULTIMATE,
    RATE_PUMP_INTER,
    RATE_PUMP_ULTIMATE,
    REMARKS,
    DAILY_REQ_PRESENT,
    PRESENT_YR,
    INTER_YR,
    ULTIMATE_YR,
    RATE_PUMP_PRESENT,
    SUPPLY_CORP,
    SUPPLY_MUN,
    HRS_PUMPING
  FROM pms_sch_asset_general
  WHERE SCH_SNO=$P{sch_no}
  AND OFFICE_ID=$P{off_id}
  )x
left JOIN
  (SELECT ADMIN_SANCTION_SNO,SANCTION_AMOUNT / 100000 as SANCTION_AMOUNT ,to_char(SANCTION_DATE,'dd/MM/YYYY') as SANCTION_DATE, SCH_SNO,SANCTION_REFNO FROM pms_sch_admin_sanction
  where SCH_SNO=$P{sch_no})
  y
ON x.ADMIN_SANCTION_SNO=y.ADMIN_SANCTION_SNO
 and
x.SCH_SNO=y.SCH_SNO
left join
 (SELECT SCH_SNO,SCH_NAME FROM pms_sch_master
  where SCH_SNO=$P{sch_no})m
ON 
x.SCH_SNO=m.SCH_SNO]]></queryString>

