	<queryString><![CDATA[select  
  fyear1.sch_name,fyear1.supply1,fyear1.dmd1,fyear1.collect1,fyear2.supply2,fyear2.dmd2,fyear2.collect2
from
(
select 
sch_name,sch_sno,trunc(supplied/1000,3) as supply1,trunc(dmd/10000000,2) as dmd1,trunc(collection/10000000,2) as collect1
 
from
(
select  sch_sno, sch_name,sum(coln_include_charges) as collection,sum(DMD_FOR_MTH_WC) as  dmd from pms_dcb_ledger_actual where (to_date((01 || '-' || month || '-' || year),'dd-MM-yyyy') >= to_date('1-4-2012','dd-mm-yyyy')) and    (to_date((01 || '-' || month || '-' || year),'dd-MM-yyyy') <= to_date('31-3-2013','dd-mm-yyyy'))
group by sch_sno,sch_name
)fin
join
(
  select sum(QTY_CONSUMED) as supplied ,SCHEME_SNO from PMS_DCB_WC_BILLING where (to_date((01 || '-' || month || '-' || year),'dd-MM-yyyy') >= to_date('1-4-2012','dd-mm-yyyy')) and    (to_date((01 || '-' || month || '-' || year),'dd-MM-yyyy') <= to_date('31-3-2013','dd-mm-yyyy'))
  group by SCHEME_SNO 
)wc1
on wc1.SCHEME_SNO=fin.sch_sno
 order by  sch_name ,sch_sno
) fyear1
join
(
select 
sch_name,sch_sno,trunc(supplied/1000,3) as supply2,trunc(dmd/10000000,2) as dmd2,trunc(collection/10000000,2) as collect2
 
from
(
select  sch_sno, sch_name,sum(coln_include_charges) as collection,sum(DMD_FOR_MTH_WC) as  dmd from pms_dcb_ledger_actual where (to_date((01 || '-' || month || '-' || year),'dd-MM-yyyy') >= to_date('1-4-2013','dd-mm-yyyy')) and    (to_date((01 || '-' || month || '-' || year),'dd-MM-yyyy') <= to_date('31-3-2014','dd-mm-yyyy'))
group by sch_sno,sch_name
)fin
join
(
  select sum(QTY_CONSUMED) as supplied ,SCHEME_SNO from PMS_DCB_WC_BILLING where (to_date((01 || '-' || month || '-' || year),'dd-MM-yyyy') >= to_date('1-4-2013','dd-mm-yyyy')) and    (to_date((01 || '-' || month || '-' || year),'dd-MM-yyyy') <= to_date('31-3-2014','dd-mm-yyyy'))
  group by SCHEME_SNO 
)wc1
on wc1.SCHEME_SNO=fin.sch_sno
 order by  sch_name ,sch_sno
)fyear2

on fyear1.sch_sno=fyear2.sch_sno]]></queryString>

