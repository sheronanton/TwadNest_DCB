	<queryString><![CDATA[SELECT bud.BUDGET_SNO,
  bud.SCH_SNO,
  bud.FIN_YEAR,case when bud.ORG_REV='O' then 'Orignal'
                    else 'Revised'    end  as status,
  bud.ADMIN_REF_NO, bud.TECH_REF_NO,
  bud.BUDGET_EST_AMT,    bud.OFFICE_ID,   sn.SCH_NAME,  TO_CHAR(bud.ADMIN_REF_DATE , 'dd/MM/yyyy') AS ADMIN_REF_DATE,
  TO_CHAR(bud.TECH_REF_DATE , 'dd/MM/yyyy') AS TECH_REF_DATE
FROM PMS_AME_TRN_BUDGET bud,
  PMS_SCH_MASTER sn
WHERE bud.sch_sno=sn.sch_sno
AND bud.FIN_YEAR     =$P{fin_yr} and bud.OFFICE_ID=$P{office_id}]]></queryString>

