	<queryString><![CDATA[SELECT DISTINCT m.sch_sno,
  M.Sch_Name,
  m.office_id,
  Mitem.Main_Item_Sno,
  mitem.main_item_desc,
  sitem.SUB_ITEM_DESC,
  sitem.SUB_ITEM_SNO
From Pms_Sch_Master M
inner JOIN Pms_Ame_Trn_Abstract T 
On M.Sch_Sno   =T.Sch_Sno and  t.fin_year='2012-2013'
And M.Office_Id=5982
left Join Pms_Ame_Mst_Main_Item Mitem
On Mitem.Main_Item_Sno=T.Main_Item_Sno
And Mitem.Group_Sno=T.Group_Sno
left Join Pms_Ame_Mst_Sub_Item Sitem
On Sitem.Sub_Item_Sno=T.Sub_Item_Sno
order by mitem.main_item_sno,sitem.sub_item_sno]]></queryString>

