	<parameter name="office_id" isForPrompting="true" class="java.lang.String"/>
	<parameter name="fin_year" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT sch.sch_name,
  sch.sch_year,decode(ame.ameamt,null,0,ame.ameamt) as amt,off.office_name
FROM
  (SELECT *
  FROM PMS_SCH_MASTER
  WHERE office_id=$P{office_id}
  AND sch_sno   IN
    (SELECT scheme_sno
    FROM PMS_DCB_MST_BENEFICIARY_METRE
    WHERE office_id =$P{office_id}
    AND METER_STATUS='L'
    )
  )sch
  
 left join 
  (
  select 
  sum(AM_EST_AMT) as ameamt,OFFICE_ID,sch_sno
  from   
  PMS_AME_TRN_ABSTRACT where office_id =$P{office_id} and fin_year=$P{fin_year}
  group by OFFICE_ID,sch_sno
  )ame
  on ame.sch_sno=sch.sch_sno
 left   join 
  (
    select 
    office_name,office_id
    from 
    com_mst_all_offices_view where  office_id =$P{office_id}
    
  )off
  on (1=1)]]></queryString>

