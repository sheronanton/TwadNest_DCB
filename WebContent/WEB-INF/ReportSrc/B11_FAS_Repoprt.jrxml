<!DOCTYPE jasperReport PUBLIC "//JasperReports//DTD Report Design//EN" "http://jasperreports.sourceforge.net/dtds/jasperreport.dtd">
<jasperReport
		 name="B11_FAS_Repoprt"
		 columnCount="1"
		 printOrder="Vertical"
		 orientation="Landscape"
		 pageWidth="900"
		 pageHeight="595"
		 columnWidth="850"
		 columnSpacing="0"
		 leftMargin="30"
		 rightMargin="20"
		 topMargin="20"
		 bottomMargin="20"
		 whenNoDataType="NoPages"
		 isTitleNewPage="false"
		 isSummaryNewPage="false">
	<property name="ireport.scriptlethandling" value="0" />
	<property name="ireport.encoding" value="UTF-8" />
	<import value="java.util.*" />
	<import value="net.sf.jasperreports.engine.*" />
	<import value="net.sf.jasperreports.engine.data.*" />

	<parameter name="year" isForPrompting="true" class="java.lang.Integer"/>
	<parameter name="month" isForPrompting="true" class="java.lang.Integer"/>
	<parameter name="office_id" isForPrompting="true" class="java.lang.Integer"/>
	<parameter name="Demand_month" isForPrompting="true" class="java.lang.Integer"/>
	<parameter name="Demand_year" isForPrompting="true" class="java.lang.Integer"/>
	<parameter name="monthstr" isForPrompting="false" class="java.lang.String"/>
	<queryString><![CDATA[SELECT
ROW_NUMBER() OVER () AS "sl_no", now() AS "CURRENT_DATE",
DIVISION_CODE as "division_code",
DIVISION_NAME as "division_name",
	FIN.DISTRICT_NAME as "district_name", 
	BLOCK_NAME, 
--	BILL_MONTH,
	
	PANCHAYAT_NAME as "panchayat_name", 
	BENEFICIARY_SNO as "beneficiary_sno",
	BENEFICIARY_NAME as "beneficiary_name", 
	BALANCE_WC as "balance_wc" ,
	BALANCE_MAINT  as "balance_maint", 
	BALANCE_INT as "balance_int" ,
		BALANCE_WC +
	BALANCE_MAINT + 
	BALANCE_INT AS "balance",
	
		case 	BILL_MONTH 
 			when 1 then 'JANUARY' 
 			when 2 then 'FEBRUARY' 
 			when 3 then 'MARCH' 
			when 4 then 'APRIL' 
			when 5 then 'MAY' 
 			when 6 then 'JUNE' 
 			when 7 then 'JULY' 
 			when 8 then 'AUGUST' 
 			when 9 then 'SEPTEMBER' 
 			when 10 then 'OCTOBER' 
 			when 11 then 'NOVEMBER' 
 			when 12 then 'DECEMBER' 
 		END AS MONTH ,
		BILL_YEAR
--	month,year
	
	
	
FROM
	(
	SELECT ROW_NUMBER
		( ) OVER ( ORDER BY REG.REG, REG.CIR, REG.DIV, DEMAND.BENEFICIARY_TYPE_ID_SUB ) AS SL_NO,
		REG.REG AS REGION_NAME,
		REG.CIR AS CIRCLE_NAME,
		REG.DIV AS DIVISION_NAME,
		BILL_YEAR, 
		BILL_MONTH, 
		
			CASE WHEN BILL_MONTH = 12 THEN 1 ELSE BILL_MONTH +1 END AS MONTH , 
			CASE WHEN BILL_MONTH = 12 THEN BILL_YEAR+1 ELSE BILL_YEAR  END AS YEAR , 

 	case 	BILL_MONTH 
 			when 1 then 'JANUARY' 
 			when 2 then 'FEBRUARY' 
 			when 3 then 'MARCH' 
			when 4 then 'APRIL' 
			when 5 then 'MAY' 
 			when 6 then 'JUNE' 
 			when 7 then 'JULY' 
 			when 8 then 'AUGUST' 
 			when 9 then 'SEPTEMBER' 
 			when 10 then 'OCTOBER' 
 			when 11 then 'NOVEMBER' 
 			when 12 then 'DECEMBER' 
 		END AS MONTH ,
		
		
		DEMAND.OFFICE_ID AS DIVISION_CODE,
		DISTRICT.DISTRICT_NAME,
		BLOCK.BLOCK_NAME,
		PANCHAYAT.PANCH_NAME AS PANCHAYAT_NAME,
		DEMAND.BENEFICIARY_SNO,
		DEMAND.BENEFICIARY_NAME,
		DEMAND.BENEFICIARY_TYPE_ID_SUB,
		TYP.BEN_TYPE_DESC AS BENEFICIARY_TYPE,
		
		
-- 		DEMAND.OB_WC,
-- 		DEMAND.DMD_WC,
-- 		COALESCE ( FAS_COLLECTION_WC.AMOUNT, 0 ) AS COLN_WC,
		DEMAND.OB_WC + DEMAND.DMD_WC - COALESCE ( FAS_COLLECTION_WC.AMOUNT, 0 ) AS BALANCE_WC,
		
-- 		DEMAND.OB_MAINT,
-- 		COALESCE ( FAS_COLLECTION_MAINT.AMOUNT, 0 ) AS COLN_MAINT,
		DEMAND.OB_MAINT  - COALESCE ( FAS_COLLECTION_MAINT.AMOUNT, 0 ) AS BALANCE_MAINT,
		
		
-- 		DEMAND.OB_INT,
-- 		DEMAND.DMD_INT,
-- 		COALESCE ( FAS_COLLECTION_INT.AMOUNT, 0 ) AS COLN_INT,
		DEMAND.OB_INT + DEMAND.DMD_INT - COALESCE ( FAS_COLLECTION_INT.AMOUNT, 0 ) AS BALANCE_INT,
		
		
		
-- 		DEMAND.OB_WC + DEMAND.OB_MAINT + DEMAND.OB_INT AS OB,
-- 		DEMAND.DMD_WC + DEMAND.DMD_INT AS DMD,
-- 		COALESCE ( FAS_COLLECTION_WC.AMOUNT, 0 ) + COALESCE ( FAS_COLLECTION_INT.AMOUNT, 0 )  + COALESCE ( FAS_COLLECTION_INT.AMOUNT, 0 ) AS COLN,
		DEMAND.OB_WC + DEMAND.DMD_WC - COALESCE ( FAS_COLLECTION_WC.AMOUNT, 0 ) + DEMAND.OB_INT + DEMAND.DMD_INT - COALESCE ( FAS_COLLECTION_INT.AMOUNT, 0 )+
		DEMAND.OB_MAINT - COALESCE ( FAS_COLLECTION_MAINT.AMOUNT, 0 )   AS BALANCE 
	FROM
		(
		SELECT
			DMD.OFFICE_ID,
			DMD.BENEFICIARY_SNO,
			BEN2.BENEFICIARY_NAME,
			DMD.BENEFICIARY_TYPE_ID_SUB,
			DMD.BILL_YEAR, 
			DMD.BILL_MONTH,
			SUM ( OUTSTANDING_DUE_WC ) AS OUTSTANDING_DUE_WC,
			SUM ( COLN_WC ) AS COLN_WC,
			SUM ( OUTSTANDING_DUE_WC ) - SUM ( COLN_WC ) AS OB_WC,
			SUM ( OUTSTANDING_DUE_MAINT ) AS OUTSTANDING_DUE_MAINT,
			SUM ( COLN_MAINT ) AS COLN_MAINT,
			SUM ( OUTSTANDING_DUE_MAINT ) - SUM ( COLN_MAINT ) AS OB_MAINT ,
			SUM ( DMD_WC ) AS DMD_WC,
			SUM ( OUTSTANDING_DUE_INT ) AS OUTSTANDING_DUE_INT,
			SUM ( COLN_INT ) AS COLN_INT,
			SUM ( OUTSTANDING_DUE_INT ) - SUM ( COLN_INT ) AS OB_INT,
			SUM ( DMD_INT ) AS DMD_INT 
		FROM
			(
			SELECT
				DMD.OFFICE_ID,
					DMD.BILL_YEAR, 
			DMD.BILL_MONTH,
				BEN.BENEFICIARY_TYPE_ID_SUB,
			CASE
					
					WHEN BEN.BENEFICIARY_TYPE_ID_SUB = 1 THEN
					BEN.ADDED_TO_BEN_SNO ELSE BEN.BENEFICIARY_SNO 
				END AS BENEFICIARY_SNO,
				COALESCE ( DMD.OB_YESTER_YR_WC, 0 ) + COALESCE ( DMD.OB_CUR_YR_WC, 0 )  AS OUTSTANDING_DUE_WC,
				COALESCE ( DMD.OB_MAINT_CHARGES, 0 ) AS OUTSTANDING_DUE_MAINT , 
				COALESCE ( DMD.COLN_YESTER_YR_WC, 0 ) + COALESCE ( DMD.COLN_CUR_YR_WC, 0 )  - COALESCE ( DMD.ADD_CHARGES_WC, 0 ) + COALESCE ( DMD.MINUS_CHARGES_WC, 0 ) - COALESCE ( DMD.ADD_CHARGES_MAINT, 0 ) + COALESCE ( DMD.MINUS_CHARGES_MAINT, 0 ) AS COLN_WC,
				COALESCE ( DMD.COLN_MAINT, 0 ) - COALESCE ( DMD.ADD_CHARGES_MAINT, 0 ) + COALESCE ( DMD.MINUS_CHARGES_MAINT, 0 ) AS COLN_MAINT , 
				COALESCE ( DMD.WC_MTH_TOTAL, 0 ) AS DMD_WC,
				COALESCE ( DMD.OB_INT_AMT_WC, 0 ) AS OUTSTANDING_DUE_INT,
				COALESCE ( DMD.COLN_INT_WC, 0 ) - COALESCE ( DMD.ADD_CHARGES_INT, 0 ) + COALESCE ( DMD.MINUS_CHARGES_INT, 0 ) AS COLN_INT,
				COALESCE ( DMD.DMD_INT_FOR_MTH_WC, 0 ) AS DMD_INT 
			FROM
				PMS_DCB_TRN_BILL_DMD DMD
				LEFT JOIN PMS_DCB_MST_BENEFICIARY BEN ON BEN.BENEFICIARY_SNO = DMD.BENEFICIARY_SNO 
			WHERE
		---		BILL_YEAR = [$DEMAND_YEAR ] 
				BILL_YEAR = $P{Demand_year}::numeric 
				AND BILL_MONTH =$P{Demand_month}::numeric
			) AS DMD
			LEFT JOIN PMS_DCB_MST_BENEFICIARY BEN2 ON BEN2.BENEFICIARY_SNO = DMD.BENEFICIARY_SNO 
		GROUP BY
			DMD.OFFICE_ID,
			DMD.BENEFICIARY_SNO,
			DMD.BENEFICIARY_TYPE_ID_SUB,
			BEN2.BENEFICIARY_NAME ,
			DMD.BILL_YEAR,
			DMD.BILL_MONTH
		) AS DEMAND -----------------------------------------------------------------------COLLECTION WC FROM FAS -----------------------------------------------
		LEFT JOIN (
		SELECT
			OFFICE_ID,
			BENEFICIARY_SNO,
			BENEFICIARY_TYPE_ID_SUB,
			SUM ( AMOUNT ) AS AMOUNT 
		FROM
			(
			SELECT
				TOTAL.OFFICE_ID,
				BEN.BENEFICIARY_TYPE_ID_SUB,
			CASE
					
					WHEN BEN.BENEFICIARY_TYPE_ID_SUB = 1 THEN
					BEN.ADDED_TO_BEN_SNO ELSE BEN.BENEFICIARY_SNO 
				END AS BENEFICIARY_SNO,
				AMOUNT 
			FROM
				(
				SELECT
					* 
				FROM
					(
					SELECT
						OFFICE_ID,
						BENEFICIARY_SNO,
						SUM ( AMOUNT ) AS AMOUNT 
					FROM
						(
						SELECT
							PMS_DCB_FAS_RECEIPT_VIEW.RECEIPT_NO,
							PMS_DCB_FAS_RECEIPT_VIEW.ACCOUNTING_FOR_OFFICE_ID AS OFFICE_ID,
							PMS_DCB_FAS_RECEIPT_VIEW.CASHBOOK_YEAR AS YEAR,
							PMS_DCB_FAS_RECEIPT_VIEW.CASHBOOK_MONTH AS MONTH,
							PMS_DCB_FAS_RECEIPT_VIEW.SUB_LEDGER_TYPE_CODE,
							PMS_DCB_FAS_RECEIPT_VIEW.SUB_LEDGER_CODE,
							PMS_DCB_FAS_RECEIPT_VIEW.SCH_TYPE_ID,
							PMS_DCB_FAS_RECEIPT_VIEW.SCH_SNO,
							PMS_DCB_FAS_RECEIPT_VIEW.BENEFICIARY_NAME,
							PMS_DCB_FAS_RECEIPT_VIEW.BENEFICIARY_SNO,
							PMS_DCB_FAS_RECEIPT_VIEW.BENEFICIARY_TYPE_ID_SUB,
							PMS_DCB_FAS_RECEIPT_VIEW.AMOUNT 
						FROM
							PMS_DCB_FAS_RECEIPT_VIEW 
						WHERE
							(
								PMS_DCB_FAS_RECEIPT_VIEW.ACCOUNT_HEAD_CODE = ANY (
									ARRAY [ 782401 :: NUMERIC,
									782402 :: NUMERIC,
									782403 :: NUMERIC,
									782404 :: NUMERIC,
									782405 :: NUMERIC,
									782406 :: NUMERIC,
									782407 :: NUMERIC,
									900108 :: NUMERIC,
									900109 :: NUMERIC ] 
								) 
							) 
							AND CASHBOOK_YEAR = $P{year}::numeric
							AND CASHBOOK_MONTH = $P{month}::numeric
						) OPT1 
					GROUP BY
						OFFICE_ID,
						BENEFICIARY_SNO 
					) RECEIPT UNION ALL
				SELECT
					* 
				FROM
					(
					SELECT
						OPT4.OFFICE_ID :: NUMERIC AS OFFICE_ID,
						OPT4.BENEFICIARY_SNO,
						SUM ( OPT4.AMOUNT ) AS AMOUNT 
					FROM
						(
						SELECT
							PMS_DCB_OTHER_CHARGES.OFFICE_ID,
							PMS_DCB_OTHER_CHARGES.BENEFICIARY_SNO,
							PMS_DCB_OTHER_CHARGES.CASHBOOK_YEAR AS YEAR,
							PMS_DCB_OTHER_CHARGES.CASHBOOK_MONTH AS MONTH,
							SUM (
							COALESCE ( CASE WHEN PMS_DCB_OTHER_CHARGES.CR_DR_INDICATOR :: TEXT = 'CR' :: TEXT THEN PMS_DCB_OTHER_CHARGES.AMOUNT ELSE NULL :: NUMERIC END, 0 :: NUMERIC ) 
			) AS AMOUNT 
		FROM
			PMS_DCB_OTHER_CHARGES 
		WHERE
			(
				PMS_DCB_OTHER_CHARGES.ACCOUNT_HEAD_CODE IN ( SELECT DISTINCT PMS_DCB_RECEIPT_ACCOUNT_MAP.ACCOUNT_HEAD_CODE FROM PMS_DCB_RECEIPT_ACCOUNT_MAP WHERE PMS_DCB_RECEIPT_ACCOUNT_MAP.COLLECTION_TYPE = 7 :: NUMERIC ) ---ONLY FOR EDAPADI
				
				AND PMS_DCB_OTHER_CHARGES.ACCOUNT_HEAD_CODE NOT IN ( SELECT DISTINCT PMS_DCB_RECEIPT_ACCOUNT_MAP.ACCOUNT_HEAD_CODE FROM PMS_DCB_RECEIPT_ACCOUNT_MAP WHERE PMS_DCB_RECEIPT_ACCOUNT_MAP.ACCOUNT_HEAD_CODE :: VARCHAR LIKE'780%' AND OFFICE_ID = '7210' ) ---ONLY FOR EDAPADI
				
			) 
		GROUP BY
			PMS_DCB_OTHER_CHARGES.OFFICE_ID,
			PMS_DCB_OTHER_CHARGES.BENEFICIARY_SNO,
			PMS_DCB_OTHER_CHARGES.CASHBOOK_YEAR,
			PMS_DCB_OTHER_CHARGES.CASHBOOK_MONTH 
		ORDER BY
			PMS_DCB_OTHER_CHARGES.OFFICE_ID,
			PMS_DCB_OTHER_CHARGES.BENEFICIARY_SNO 
		) AS OPT4 
	WHERE
		OPT4.YEAR =$P{year}::NUMERIC

		AND OPT4.MONTH=$P{month}::numeric

	GROUP BY
		OPT4.OFFICE_ID,
		OPT4.BENEFICIARY_SNO 
	) ADJ 
	) TOTAL
	LEFT JOIN PMS_DCB_MST_BENEFICIARY BEN ON BEN.BENEFICIARY_SNO = TOTAL.BENEFICIARY_SNO 
	) AS OPT1 
GROUP BY
	OFFICE_ID,
	BENEFICIARY_SNO,
	BENEFICIARY_TYPE_ID_SUB 
	) FAS_COLLECTION_WC ON FAS_COLLECTION_WC.OFFICE_ID = DEMAND.OFFICE_ID 
	AND FAS_COLLECTION_WC.BENEFICIARY_SNO = DEMAND.BENEFICIARY_SNO --------------------------------------------------------CURRENT MONTH COLLECTION INTEREST --------------------------------------------------------
	LEFT JOIN (
	SELECT
		OFFICE_ID,
		BENEFICIARY_SNO,
		BENEFICIARY_TYPE_ID_SUB,
		SUM ( AMOUNT ) AS AMOUNT 
	FROM
		(
		SELECT
			TOTAL.OFFICE_ID,
			BEN.BENEFICIARY_TYPE_ID_SUB,
		CASE
				
				WHEN BEN.BENEFICIARY_TYPE_ID_SUB = 1 THEN
				BEN.ADDED_TO_BEN_SNO ELSE BEN.BENEFICIARY_SNO 
			END AS BENEFICIARY_SNO,
			AMOUNT 
		FROM
			(
			SELECT
				* 
			FROM
				(
				SELECT
					OFFICE_ID,
					BENEFICIARY_SNO,
					SUM ( AMOUNT ) AS AMOUNT 
				FROM
					(
					SELECT
						PMS_DCB_FAS_RECEIPT_VIEW.RECEIPT_NO,
						PMS_DCB_FAS_RECEIPT_VIEW.ACCOUNTING_FOR_OFFICE_ID AS OFFICE_ID,
						PMS_DCB_FAS_RECEIPT_VIEW.CASHBOOK_YEAR AS YEAR,
						PMS_DCB_FAS_RECEIPT_VIEW.CASHBOOK_MONTH AS MONTH,
						PMS_DCB_FAS_RECEIPT_VIEW.SUB_LEDGER_TYPE_CODE,
						PMS_DCB_FAS_RECEIPT_VIEW.SUB_LEDGER_CODE,
						PMS_DCB_FAS_RECEIPT_VIEW.SCH_TYPE_ID,
						PMS_DCB_FAS_RECEIPT_VIEW.SCH_SNO,
						PMS_DCB_FAS_RECEIPT_VIEW.BENEFICIARY_NAME,
						PMS_DCB_FAS_RECEIPT_VIEW.BENEFICIARY_SNO,
						PMS_DCB_FAS_RECEIPT_VIEW.BENEFICIARY_TYPE_ID_SUB,
						PMS_DCB_FAS_RECEIPT_VIEW.AMOUNT 
					FROM
						PMS_DCB_FAS_RECEIPT_VIEW 
					WHERE
						( PMS_DCB_FAS_RECEIPT_VIEW.ACCOUNT_HEAD_CODE = ANY ( ARRAY [ 120601 :: NUMERIC ] ) ) 
						AND CASHBOOK_YEAR =  $P{year}::numeric
 
						AND CASHBOOK_MONTH = $P{month}::numeric 
					) OPT1 
				GROUP BY
					OFFICE_ID,
					BENEFICIARY_SNO 
				) RECEIPT UNION ALL
			SELECT
				* 
			FROM
				(
				SELECT
					OPT4.OFFICE_ID :: NUMERIC AS OFFICE_ID,
					OPT4.BENEFICIARY_SNO,
					SUM ( OPT4.AMOUNT ) AS AMOUNT 
				FROM
					(
					SELECT
						PMS_DCB_OTHER_CHARGES.OFFICE_ID,
						PMS_DCB_OTHER_CHARGES.BENEFICIARY_SNO,
						PMS_DCB_OTHER_CHARGES.CASHBOOK_YEAR AS YEAR,
						PMS_DCB_OTHER_CHARGES.CASHBOOK_MONTH AS MONTH,
						SUM (
						COALESCE ( CASE WHEN PMS_DCB_OTHER_CHARGES.CR_DR_INDICATOR :: TEXT = 'CR' :: TEXT THEN PMS_DCB_OTHER_CHARGES.AMOUNT ELSE NULL :: NUMERIC END, 0 :: NUMERIC ) 
		) AS AMOUNT 
	FROM
		PMS_DCB_OTHER_CHARGES 
	WHERE
		( PMS_DCB_OTHER_CHARGES.ACCOUNT_HEAD_CODE IN ( SELECT DISTINCT PMS_DCB_RECEIPT_ACCOUNT_MAP.ACCOUNT_HEAD_CODE FROM PMS_DCB_RECEIPT_ACCOUNT_MAP WHERE PMS_DCB_RECEIPT_ACCOUNT_MAP.COLLECTION_TYPE = 9 :: NUMERIC ) ) 
	GROUP BY
		PMS_DCB_OTHER_CHARGES.OFFICE_ID,
		PMS_DCB_OTHER_CHARGES.BENEFICIARY_SNO,
		PMS_DCB_OTHER_CHARGES.CASHBOOK_YEAR,
		PMS_DCB_OTHER_CHARGES.CASHBOOK_MONTH 
	ORDER BY
		PMS_DCB_OTHER_CHARGES.OFFICE_ID,
		PMS_DCB_OTHER_CHARGES.BENEFICIARY_SNO 
	) AS OPT4 
WHERE
	OPT4.YEAR =  $P{year}::numeric
 
	AND OPT4.MONTH = $P{month}::numeric
 
GROUP BY
	OPT4.OFFICE_ID,
	OPT4.BENEFICIARY_SNO 
	) ADJ 
	) TOTAL
	LEFT JOIN PMS_DCB_MST_BENEFICIARY BEN ON BEN.BENEFICIARY_SNO = TOTAL.BENEFICIARY_SNO 
	) AS OPT1 
GROUP BY
	OFFICE_ID,
	BENEFICIARY_SNO,
	BENEFICIARY_TYPE_ID_SUB 
	) FAS_COLLECTION_INT ON FAS_COLLECTION_INT.OFFICE_ID = DEMAND.OFFICE_ID 
	AND FAS_COLLECTION_INT.BENEFICIARY_SNO = DEMAND.BENEFICIARY_SNO 
	
		LEFT JOIN 
											(
											select TOTAL.office_id , TOTAL.beneficiary_sno , beneficiary_type_id_sub , sum(amount) as AMOUNT  from (

										SELECT
														* 
											FROM
														(
														SELECT
															office_id,
															beneficiary_sno,
															SUM ( amount ) AS amount 
														FROM
															(
															SELECT
																pms_dcb_fas_receipt_view.receipt_no,
																pms_dcb_fas_receipt_view.accounting_for_office_id AS office_id,
																pms_dcb_fas_receipt_view.cashbook_year AS YEAR,
																pms_dcb_fas_receipt_view.cashbook_month AS MONTH,
																pms_dcb_fas_receipt_view.sub_ledger_type_code,
																pms_dcb_fas_receipt_view.sub_ledger_code,
																pms_dcb_fas_receipt_view.sch_type_id,
																pms_dcb_fas_receipt_view.sch_sno,
																pms_dcb_fas_receipt_view.beneficiary_name,
																pms_dcb_fas_receipt_view.beneficiary_sno,
																pms_dcb_fas_receipt_view.beneficiary_type_id_sub,
																pms_dcb_fas_receipt_view.amount 
															FROM
																pms_dcb_fas_receipt_view 
															WHERE
																( pms_dcb_fas_receipt_view.account_head_code in (
											select
												ACCOUNT_HEAD_CODE
											from
												PMS_DCB_RECEIPT_ACCOUNT_MAP
											where
												sch_type_id = 12
												and ACTIVE_STATUS = 'L'
												and collection_type = 8 ) ) 
																AND cashbook_year =  $P{year}::numeric
 
																AND cashbook_month =  $P{month}::numeric
 
															) opt1 
														GROUP BY
															office_id,
															beneficiary_sno
													) as RECEIPT
														
														union all 
														
											SELECT
														* 
													FROM
														(
														SELECT
															opt4.office_id :: NUMERIC AS office_id,
															opt4.beneficiary_sno,
															SUM ( opt4.amount ) AS amount 
														FROM
															(
															SELECT
																pms_dcb_other_charges.office_id,
																pms_dcb_other_charges.beneficiary_sno,
																pms_dcb_other_charges.cashbook_year AS YEAR,
																pms_dcb_other_charges.cashbook_month AS MONTH,
																SUM (
																COALESCE ( CASE WHEN pms_dcb_other_charges.cr_dr_indicator :: TEXT = 'CR' :: TEXT THEN pms_dcb_other_charges.amount ELSE NULL :: NUMERIC END, 0 :: NUMERIC ) 
												) AS amount 
											FROM
												pms_dcb_other_charges 
											WHERE
												( pms_dcb_other_charges.account_head_code IN ( SELECT DISTINCT pms_dcb_receipt_account_map.account_head_code FROM pms_dcb_receipt_account_map WHERE pms_dcb_receipt_account_map.collection_type = 8 :: NUMERIC ) ) 
											GROUP BY
												pms_dcb_other_charges.office_id,
												pms_dcb_other_charges.beneficiary_sno,
												pms_dcb_other_charges.cashbook_year,
												pms_dcb_other_charges.cashbook_month 
											ORDER BY
												pms_dcb_other_charges.office_id,
												pms_dcb_other_charges.beneficiary_sno 
											) AS opt4 
										WHERE
											opt4.YEAR = $P{year}::numeric

											AND opt4.MONTH = $P{month}::numeric
 
										GROUP BY
											opt4.office_id,
											opt4.beneficiary_sno 
											) as ADJUSTMENT
											
											) as TOTAL
											lEFT JOIN pms_dcb_mst_beneficiary BEN ON BEN.BENEFICIARY_SNO = TOTAL.BENEFICIARY_SNO
											group by TOTAL.office_id , TOTAL.beneficiary_sno , BEN.beneficiary_type_id_sub 
											
											) FAS_COLLECTION_MAINT ON  FAS_COLLECTION_MAINT.OFFICE_ID = DEMAND.OFFICE_ID 
	AND FAS_COLLECTION_MAINT.BENEFICIARY_SNO = DEMAND.BENEFICIARY_SNO 
	
	-----------------------------------------------------MASTERS ---
	LEFT JOIN PMS_DCB_MST_BENEFICIARY BEN ON BEN.BENEFICIARY_SNO = DEMAND.BENEFICIARY_SNO
	LEFT JOIN PMS_DCB_BEN_TYPE TYP ON TYP.BEN_TYPE_ID = BEN.BENEFICIARY_TYPE_ID_SUB
	LEFT JOIN COM_MST_DISTRICTS DISTRICT ON DISTRICT.DISTRICT_CODE = BEN.DISTRICT_CODE
