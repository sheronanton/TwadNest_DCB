	<queryString><![CDATA[select b.BENEFICIARY_NAME,c.SCH_NAME,a.TARIFF_RATE,a.Qty,a.dmd from 
(select BENEFICIARY_SNO,
sum(WC_SLAB_AMT) as dmd,TARIFF_RATE,
SCH_SNO,
Qty
from PMS_DCB_WC_SLAB where month=$P{month} and year=$P{year} AND Qty  >0   and TARIFF_RATE=$P{Tariff}
group by BENEFICIARY_SNO, TARIFF_RATE, SCH_SNO, Qty 
) a
left outer join
(
select BENEFICIARY_SNO,
BENEFICIARY_NAME from PMS_DCB_MST_BENEFICIARY  
)b
on a.BENEFICIARY_SNO=b.BENEFICIARY_SNO
left outer join
(
select SCH_SNO,
SCH_NAME from PMS_SCH_MASTER  
)c
on c.SCH_SNO=a.SCH_SNO
order by b.BENEFICIARY_NAME,c.SCH_NAME]]></queryString>

