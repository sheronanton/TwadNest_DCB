	<queryString><![CDATA[select 
BENEFICIARY_SNO,
 BENEFICIARY_NAME,
BENEFICIARY_TYPE_ID_SUB,
BEN_TYPE_DESC,
district_name,
OFFICE_ID,dmd,ob,collection,arrear_collection,bal,cb,OB_MAINT_CHARGES,ob_int,COLN_INT_WC,
(OB_MAINT_CHARGES+ob+dmd+ob_int)-(collection+arrear_collection+COLN_INT_WC) as net_balance,
CASE
    WHEN (collection) < 0
    THEN '-'     
    ELSE case when dmd > 0 then to_char(TRUNC((collection) /dmd * 100,3),'990.99')	else '-' end 
  END AS avg1,
CASE
    WHEN (arrear_collection) < 0
    THEN '-'     
    ELSE case when Ob > 0 then to_char(TRUNC((arrear_collection) /ob * 100,3),'990.99')	else '-' end 
  END AS avg2
from 

(


SELECT
 	ben.BENEFICIARY_SNO,
	ben.BENEFICIARY_TYPE_ID_SUB,
	btype.BEN_TYPE_DESC,
	dis.district_name,
 (
    SELECT
      CASE
        WHEN ADDED_AREA='y'
        OR ADDED_AREA  ='Y'
        THEN BENEFICIARY_NAME || ' (Added )' 
        ELSE BENEFICIARY_NAME
      END AS BENEFICIARY_NAME
    FROM PMS_DCB_MST_BENEFICIARY
    WHERE BENEFICIARY_SNO= ben.BENEFICIARY_SNO
   ) AS BENEFICIARY_NAME,
	--(select  case when ADDED_AREA='y' or  ADDED_AREA='Y'  then ADDED_TO_BEN_NAME else BENEFICIARY_NAME end as BENEFICIARY_NAME from PMS_DCB_MST_BENEFICIARY where BENEFICIARY_SNO= ben.BENEFICIARY_SNO) as BENEFICIARY_NAME, 
	--(select  case when ADDED_AREA='y' or  ADDED_AREA='Y'  then ADDED_TO_BEN_SNO else BENEFICIARY_SNO end as BENEFICIARY_SNO from PMS_DCB_MST_BENEFICIARY where BENEFICIARY_SNO= ben.BENEFICIARY_SNO) as BENEFICIARY_SNO, 

  coll.OFFICE_ID ,
  dmd, 
  ob,  
  case when collection < dmd then
      collection
  else 
      dmd
    end
      as collection,
   case when  collection > dmd   then
    (collection-dmd) 
    else
    0
    end as arrear_collection,
     case when collection > dmd then
     0 
     else 
        (dmd-collection) end  AS bal,
  (ob +(dmd-collection)) AS cb,OB_MAINT_CHARGES,ob_int,COLN_INT_WC
 
FROM (
  (SELECT OFFICE_ID,
    BENEFICIARY_SNO,
    (SUM(OB_CUR_YR_WC)+SUM(OB_YESTER_YR_WC))/ 100000 AS ob,sum(OB_MAINT_CHARGES)/ 100000 as OB_MAINT_CHARGES 
  FROM PMS_DCB_OB_YEARLY
  WHERE  MONTH=4   AND FIn_YEAR   =$P{arr_year}  GROUP BY OFFICE_ID,
    BENEFICIARY_SNO
  )ob
LEFT OUTER JOIN
  (SELECT BENEFICIARY_SNO ,
    OFFICE_ID,
    ((SUM(COLN_CUR_YR_WC) +sum(COLN_MAINT) + SUM(COLN_YESTER_YR_WC) +SUM(ADD_CHARGES_WC)) -SUM(MINUS_CHARGES_WC) )/ 100000 AS collection,
    sum(CB_INT_AMT_WC) / 100000 as ob_int,sum(COLN_INT_WC) / 100000 as COLN_INT_WC
  FROM PMS_DCB_TRN_BILL_DMD
 WHERE   (to_date((01||'-'||BILL_MONTH||'-'||BILL_YEAR),'dd-MM-yyyy') >=to_date('1-4-'||$P{arr_year},'dd-mm-yyyy'))
  AND (to_date((01||'-'||BILL_MONTH||'-'||BILL_YEAR),'dd-MM-yyyy') <=to_date($P{enddate_cur}||'-'||$P{month}||'-'||$P{year},'dd-mm-yyyy')) 
 and BENEFICIARY_SNO in (SELECT     BENEFICIARY_SNO FROM PMS_DCB_MST_BENEFICIARY  WHERE status='L')
  GROUP BY OFFICE_ID,
    BENEFICIARY_SNO
  )coll
ON coll.OFFICE_ID       =ob.OFFICE_ID
AND coll.BENEFICIARY_SNO=ob.BENEFICIARY_SNO
LEFT OUTER JOIN
  (SELECT OFFICE_ID,
    BENEFICIARY_SNO,
    SUM(MONTH_BILL_AMT) / 100000 AS dmd
  FROM PMS_DCB_TRN_BILL_DMD
  WHERE   (to_date((01||'-'||BILL_MONTH||'-'||BILL_YEAR),'dd-MM-yyyy') >=to_date('1-4-'||$P{arr_year},'dd-mm-yyyy'))
   AND (to_date((01||'-'||BILL_MONTH||'-'||BILL_YEAR),'dd-MM-yyyy') <=to_date($P{enddate_prv}||'-'||$P{prv_month}||'-'||$P{prv_year},'dd-mm-yyyy')) 
  GROUP BY OFFICE_ID,
    BENEFICIARY_SNO
  )dmd
ON coll.OFFICE_ID       =dmd.OFFICE_ID
AND coll.BENEFICIARY_SNO=dmd.BENEFICIARY_SNO
AND dmd.OFFICE_ID       =ob.OFFICE_ID
AND dmd.BENEFICIARY_SNO =ob.BENEFICIARY_SNO
LEFT OUTER JOIN
  (SELECT DISTRICT_CODE ,
    BENEFICIARY_NAME,
    BENEFICIARY_SNO,
    BENEFICIARY_TYPE_ID_SUB
  FROM PMS_DCB_MST_BENEFICIARY
  WHERE status='L'
  )ben
ON ben.BENEFICIARY_SNO =dmd.BENEFICIARY_SNO
AND ben.BENEFICIARY_SNO=coll.BENEFICIARY_SNO
AND ben.BENEFICIARY_SNO=ob.BENEFICIARY_SNO
LEFT OUTER JOIN
  ( SELECT district_code,district_name FROM com_mst_districts
  )dis
ON dis.district_code=ben.district_code
LEFT OUTER JOIN
  ( SELECT BEN_TYPE_ID,BEN_TYPE_DESC FROM PMS_DCB_BEN_TYPE
  )btype
ON btype.BEN_TYPE_ID=ben.BENEFICIARY_TYPE_ID_SUB )
where BENEFICIARY_SNO is not null
ORDER BY BENEFICIARY_TYPE_ID_SUB,
  district_name,
  BENEFICIARY_SNO
  )]]></queryString>

