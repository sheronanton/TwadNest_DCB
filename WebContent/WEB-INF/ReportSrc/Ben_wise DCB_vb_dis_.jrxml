	<queryString><![CDATA[select 
 DISTRICT_CODE,
district_name,
 
  SUM(dmd)                                                                                                       AS dmd,
  SUM(ob)                                                                                                        AS ob,
  SUM(collection)                                                                                                AS collection,
  SUM(arrear_collection)                                                                                         AS arrear_collection,
  SUM(bal)                                                                                                       AS bal,
  SUM(cb)                                                                                                        AS cb,
  SUM(OB_MAINT_CHARGES)                                                                                          AS OB_MAINT_CHARGES,
  SUM(ob_int)                                                                                                    AS ob_int,
  SUM(COLN_INT_WC)                                                                                               AS COLN_INT_WC,
  (SUM(OB_MAINT_CHARGES)+SUM(ob)+SUM(dmd)+SUM(ob_int))-(SUM(collection)+SUM(arrear_collection)+SUM(COLN_INT_WC)) AS net_balance
from 

(


SELECT 
  ben.BENEFICIARY_TYPE_ID,
  btype.BEN_TYPE_DESC,
 district_name,DISTRICT_CODE,
  ben.BENEFICIARY_NAME,
  coll.OFFICE_ID ,
  dmd, 
  ob,  
  case when collection < dmd then
      collection
  else 
      dmd
    end
      as collection,
   case when  collection > dmd   then
    (collection-dmd) 
    else
    0
    end as arrear_collection,
     case when collection > dmd then
     0 
     else 
        (dmd-collection) end  AS bal,
  (ob +(dmd-collection)) AS cb,OB_MAINT_CHARGES,ob_int,COLN_INT_WC
 
FROM (
  (SELECT OFFICE_ID,
    BENEFICIARY_SNO,
    (SUM(OB_CUR_YR_WC)+SUM(OB_YESTER_YR_WC))/ 100000 AS ob,sum(OB_MAINT_CHARGES)/ 100000 as OB_MAINT_CHARGES 
  FROM PMS_DCB_OB_YEARLY
  WHERE  MONTH=4   AND FIn_YEAR   =$P{arr_year}  GROUP BY OFFICE_ID,
    BENEFICIARY_SNO
  )ob
LEFT OUTER JOIN
  (SELECT BENEFICIARY_SNO ,
    OFFICE_ID,
    ((SUM(COLN_CUR_YR_WC) +sum(COLN_MAINT) + SUM(COLN_YESTER_YR_WC) +SUM(ADD_CHARGES_WC)) -SUM(MINUS_CHARGES_WC) )/ 100000 AS collection,
    sum(CB_INT_AMT_WC) / 100000 as ob_int,sum(COLN_INT_WC) / 100000 as COLN_INT_WC
  FROM PMS_DCB_TRN_BILL_DMD  
  WHERE   (to_date((01||'-'||BILL_MONTH||'-'||BILL_YEAR),'dd-MM-yyyy') >=to_date('1-4-'||$P{arr_year},'dd-mm-yyyy'))
  AND (to_date((01||'-'||BILL_MONTH||'-'||BILL_YEAR),'dd-MM-yyyy') <=to_date($P{enddate_cur}||'-'||$P{month}||'-'||$P{year},'dd-mm-yyyy')) 
  and BENEFICIARY_SNO in (SELECT     BENEFICIARY_SNO FROM PMS_DCB_MST_BENEFICIARY  WHERE status='L')
  GROUP BY OFFICE_ID,
    BENEFICIARY_SNO
  )coll
ON coll.OFFICE_ID       =ob.OFFICE_ID
AND coll.BENEFICIARY_SNO=ob.BENEFICIARY_SNO
LEFT OUTER JOIN
  (SELECT OFFICE_ID,
    BENEFICIARY_SNO,
    SUM(MONTH_BILL_AMT) / 100000 AS dmd
  FROM PMS_DCB_TRN_BILL_DMD
  WHERE   (to_date((01||'-'||BILL_MONTH||'-'||BILL_YEAR),'dd-MM-yyyy') >=to_date('1-4-'||$P{arr_year},'dd-mm-yyyy'))
  AND (to_date((01||'-'||BILL_MONTH||'-'||BILL_YEAR),'dd-MM-yyyy') <=to_date($P{enddate_prv}||'-'||$P{prv_month}||'-'||$P{prv_year},'dd-mm-yyyy')) 
  GROUP BY OFFICE_ID,
    BENEFICIARY_SNO
  )dmd
ON coll.OFFICE_ID       =dmd.OFFICE_ID
AND coll.BENEFICIARY_SNO=dmd.BENEFICIARY_SNO
AND dmd.OFFICE_ID       =ob.OFFICE_ID
AND dmd.BENEFICIARY_SNO =ob.BENEFICIARY_SNO
LEFT OUTER JOIN
  (SELECT DISTRICT_CODE ,
    BENEFICIARY_NAME,
    BENEFICIARY_SNO,
    BENEFICIARY_TYPE_ID
  FROM PMS_DCB_MST_BENEFICIARY 
  WHERE status='L' and BENEFICIARY_TYPE_ID_SUB=6
  )ben
ON ben.BENEFICIARY_SNO =dmd.BENEFICIARY_SNO
AND ben.BENEFICIARY_SNO=coll.BENEFICIARY_SNO
AND ben.BENEFICIARY_SNO=ob.BENEFICIARY_SNO
LEFT OUTER JOIN
  ( SELECT district_code,district_name FROM com_mst_districts
  )dis
ON dis.district_code=ben.district_code
LEFT OUTER JOIN
  ( SELECT BEN_TYPE_ID,BEN_TYPE_DESC FROM PMS_DCB_BEN_TYPE
  )btype
ON btype.BEN_TYPE_ID=ben.BENEFICIARY_TYPE_ID )
where BENEFICIARY_SNO is not null
ORDER BY BENEFICIARY_TYPE_ID,
  district_name,
  BENEFICIARY_SNO
  )
  GROUP BY DISTRICT_CODE,
  district_name
ORDER BY DISTRICT_CODE]]></queryString>

