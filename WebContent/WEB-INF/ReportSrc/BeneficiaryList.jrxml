	<queryString><![CDATA[SELECT
  BENEFICIARY_SNO,
  BENEFICIARY_NAME,
  BEN_TYPE_DESC,
  DECODE(DISTRICT_NAME,null,'  ',DISTRICT_NAME) AS DISTRICT_NAME,
  DECODE(BLOCK_NAME,null,'   ',BLOCK_NAME) AS BLOCK_NAME,
  DECODE(BILLING_ADDRESS1,null,'  ',BILLING_ADDRESS1) AS BILLING_ADDRESS1 ,
  DECODE(BILLING_ADDRESS2,null,'  ',BILLING_ADDRESS2) AS BILLING_ADDRESS2 ,
  DECODE(BILLING_ADDRESS3,null,'  ',BILLING_ADDRESS3) AS BILLING_ADDRESS3 
  
FROM
(
  (
    SELECT 
      BENEFICIARY_SNO,
      BENEFICIARY_NAME,
      BENEFICIARY_TYPE_ID, 
      DISTRICT_CODE,
      BLOCK_SNO,
      BILLING_ADDRESS1,
      BILLING_ADDRESS2,
      BILLING_ADDRESS3
    FROM PMS_DCB_MST_BENEFICIARY
    WHERE STATUS='L' and OFFICE_ID=$P{Off}
    ORDER BY BENEFICIARY_TYPE_ID
  )a
  
  LEFT OUTER JOIN
  
  (
    SELECT
      BEN_TYPE_ID,
      BEN_TYPE_DESC
    FROM PMS_DCB_BEN_TYPE
  )b
  
  ON a.BENEFICIARY_TYPE_ID = b.BEN_TYPE_ID
  LEFT OUTER JOIN
  
  (
    SELECT
      DISTRICT_CODE,
      DISTRICT_NAME
    FROM COM_MST_DISTRICTS
  )c
  
  ON a.DISTRICT_CODE = c.DISTRICT_CODE
  LEFT OUTER JOIN
  
  (
    SELECT
      BLOCK_SNO,
      BLOCK_NAME
    FROM COM_MST_BLOCKS
  )d
  
  ON a.BLOCK_SNO = d.BLOCK_SNO
  
)

order by BEN_TYPE_ID,BEN_TYPE_DESC,BENEFICIARY_NAME]]></queryString>

