	</variable>
	<variable name="blk" class="java.lang.String" resetType="Report" calculation="Nothing">
	</variable>
	<variable name="row1_sum" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[$F{MONTH_BILL_AMT}.add( $F{DMD_FOR_MTH_MC}).add($F{INT_CALC_WC})]]></variableExpression>
	</variable>
	<variable name="net_sum_1_new" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
