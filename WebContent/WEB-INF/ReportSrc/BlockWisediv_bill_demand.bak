	</variable>
	<variable name="int_display" class="java.lang.String" resetType="None" calculation="Nothing">
		<variableExpression><![CDATA["<<Penalty @"+$F{INT_RATE}+"% for Rs."]]></variableExpression>
	</variable>
	<variable name="int_amt" class="java.math.BigDecimal" resetType="None" calculation="Nothing">
		<variableExpression><![CDATA[$F{INT_CALC_WC}.multiply(new java.math.BigDecimal("100")).divide( $F{INT_RATE})]]></variableExpression>
	</variable>
	<variable name="netotheramt_int" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[($F{ADD_CHARGES_INT}).subtract( $F{MINUS_CHARGES_INT})]]></variableExpression>
	</variable>
	<variable name="correction_amt" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[($F{ADD_CHARGES_WC}).subtract($F{MINUS_CHARGES_WC}).add($V{netotheramt_int})]]></variableExpression>
	</variable>
