	<queryString><![CDATA[SELECT 
  sm.sch_name,
  tb.tech_ref_no,
  tb.tech_ref_date,
  tb.budget_est_amt,
  tb.ORG_REV,
  name.office_name,
  name.office_id,
  d.office_id as region_id,
  e.office_id as circle_id,
  d.office_name as region,
  e.office_name as circle
FROM pms_sch_master sm,
  com_mst_all_offices_view name,
  pms_ame_trn_budget tb, 
  com_mst_all_offices_view d,
  com_mst_all_offices_view e
WHERE
    sm.sch_sno=tb.sch_sno
    and name.office_id=tb.office_id
    and d.office_level_id='RN'
    and e.office_level_id='CL'
    and name.region_office_id=d.office_id
    and name.circle_office_id=e.office_id
    and tb.fin_year =$P{fin_year}
order by  region_id,circle_id,office_id]]></queryString>

