DISTRICT_CODE,DISTRICT_NAME,BENEFICIARY_SNO,BENEFICIARY_NAME,BEN_TYPE_ID,YEAR,
decode(MONTH,'07','June')AS MONTHDESC,
MONTH,
BEN_TYPE_DESC
from PMS_DCB_LEDGER_ACTUAL
group by DISTRICT_CODE,DISTRICT_NAME,BENEFICIARY_SNO,BENEFICIARY_NAME,BEN_TYPE_ID,YEAR,MONTH,BEN_TYPE_DESC
