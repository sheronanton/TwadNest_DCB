	<queryString><![CDATA[SELECT b.*,
  a.OFFICE_NAME,w.*,a.reg_name,a.circle
FROM
  (SELECT SUM(amount) AS COUNT,
    ACCOUNTING_FOR_OFFICE_ID
  FROM FAS_RECEIPT_TRANSACTION
  WHERE ACCOUNTING_FOR_OFFICE_ID IN
    (SELECT office_id FROM PMS_DCB_DIV_DIST_MAP
    )
  AND cashbook_month =$P{month}
  AND cashbook_year  =$P{year}
  and account_head_code IN (SELECT ACCOUNT_HEAD_CODE  FROM PMS_DCB_RECEIPT_ACCOUNT_MAP  WHERE COLLECTION_TYPE IN (6,7,8))
  AND receipt_no    IN
    (SELECT receipt_no
    FROM FAS_RECEIPT_MASTER
    WHERE SUB_LEDGER_TYPE_CODE    = 14
    AND ACCOUNTING_FOR_OFFICE_ID IN
      (SELECT office_id FROM PMS_DCB_DIV_DIST_MAP
      )
    AND cashbook_month=$P{month}
    AND cashbook_year =$P{year}
    AND RECEIPT_STATUS='L'
    )
  GROUP BY ACCOUNTING_FOR_OFFICE_ID
  ) b

JOIN
  (  SELECT a1.OFFICE_NAME,a1.office_id,b1.OFFICE_NAME as reg_name,c1.OFFICE_NAME as circle  FROM COM_MST_ALL_OFFICES_VIEW a1,COM_MST_ALL_OFFICES_VIEW b1 ,COM_MST_ALL_OFFICES_VIEW c1 
  where a1.OFFICE_LEVEL_ID='DN'  and b1.office_level_id='RN'
  and a1.REGION_OFFICE_ID=b1.OFFICE_ID 
  and a1.REGION_OFFICE_ID=c1.region_office_id  
  and a1.circle_office_id=c1.office_id and a1.office_id in (select office_id from pms_dcb_div_dist_map)
    
  ) a
ON a.office_id=b.ACCOUNTING_FOR_OFFICE_ID

left outer JOIN
  (SELECT  DECODE(SUM(TOTAL_AMT),NULL,0.0,SUM(TOTAL_AMT)) as TOTAL_AMT   ,
    OFFICE_ID
  FROM PMS_DCB_WC_BILLING  where month=$P{month} and year=$P{year}
  GROUP BY OFFICE_ID
  )w
ON w.OFFICE_ID=a.office_id 

order by a.reg_name,a.circle, a.office_id]]></queryString>

