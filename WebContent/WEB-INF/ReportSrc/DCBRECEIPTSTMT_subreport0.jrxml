		<defaultValueExpression ><![CDATA[$P{off_name}]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT SUM(amount)
FROM FAS_RECEIPT_TRANSACTION
WHERE accounting_for_office_id=$P{office_id}
AND cashbook_month            =$P{month}
AND cashbook_year             =$P{year}
and ACCOUNTING_UNIT_ID=$P{a_id}
AND sub_ledger_type_code      =10 and ACCOUNT_HEAD_CODE in (SELECT ACCOUNT_HEAD_CODE  from PMS_DCB_RECEIPT_ACCOUNT_MAP)
AND receipt_no               IN
  (SELECT receipt_no
  FROM FAS_RECEIPT_MASTER
  WHERE accounting_for_office_id=$P{office_id}
  AND cashbook_month            =$P{month}
  AND cashbook_year             =$P{year}
  and ACCOUNTING_UNIT_ID=$P{a_id}  and SUB_LEDGER_CODE  in (select BENEFICIARY_SNO from PMS_DCB_MST_BENEFICIARY  where   OFFICE_ID=$P{office_id} and status='L')
  AND sub_ledger_type_code      =14
  AND receipt_status = 'L' 
  )]]></queryString>

