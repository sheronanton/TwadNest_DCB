  rec.BALANCE_18 ,rec.NET_DUE_19 as "BALANCE_18 ,rec.NET_DUE_19"
FROM




  ( 

  (
  select 
        distinct BEN_TYPE_GROUP 
  from 
  
        PMS_DCB_BEN_TYPE
        
  )bt  
     join
  (
  select 
         BEN_TYPE_ID,
         BEN_TYPE_SDESC,BEN_TYPE_DESC ,BEN_TYPE_GROUP
  from 
  
        PMS_DCB_BEN_TYPE 
 order by BEN_TYPE_GROUP
  )bt2
 on bt2. BEN_TYPE_GROUP=bt.BEN_TYPE_GROUP
       join 
  (
 SELECT OFFICE_ID,
    OFFICE_NAME ,       
---    DECODE(MONTH,1,'JANUARY', 2,'February', 3,'March', 4,'April', 5,'May', 6,'June', 7,'July', 8,'August', 9,'September', 10,'October', 11,'November', 12,'December', 'N/A')MONTH,
 case
		when (MONTH=1) then 'Jan'
 when (MONTH=2) then 'February'
when (MONTH=3) then 'MARCH'
 when (MONTH=4) then 'APRIL'
 when (MONTH=5) then 'MAY'
 when (MONTH=6) then 'JUNE'
 when (MONTH=7) then 'JULY'
 when (MONTH=8) then 'AUGUST'
 when (MONTH=9) then 'SEPTEMBER'
 when (MONTH=10) then 'OCTOBER'
 when (MONTH=11) then 'NOVEMBER'
when (MONTH=12) then 'DECEMBER'  end as "MONTH",
 YEAR as "YEAR",

   
    coalesce(SUM(OPENING_BAL_MC),NULL,0.0,SUM(OPENING_BAL_MC))                             AS OPENING_BAL_MC_3,
    coalesce(SUM(ADDNS_IF_ANY),NULL,0.0,SUM(ADDNS_IF_ANY))                                 AS ADDNS_IF_ANY_3A,
    coalesce(SUM(COL_UTO_PRV_MNH_MC),NULL,0.0,SUM(COL_UTO_PRV_MNH_MC))                     AS COL_UTO_PRV_MNH_MC_4,
    coalesce(SUM(COLL_FOR_MTH_MAINT),NULL,0.0,SUM(COLL_FOR_MTH_MAINT))                     AS COLL_FOR_MTH_MAINT_5,
    coalesce(SUM(TOTAL_COLLN),NULL,0.0,SUM(TOTAL_COLLN))                                   AS TOTAL_COLLN_6,
    coalesce(SUM(BALANCE),NULL,0.0,SUM(BALANCE))                                           AS BALANCE_7,
    coalesce(SUM(OPENING_BAL_WC),NULL,0.0,SUM(OPENING_BAL_WC))                             AS OPENING_BAL_WC_8,
    coalesce(SUM(DMD_UPTO_PRV_MNTH_WC),NULL,0.0,SUM(DMD_UPTO_PRV_MNTH_WC))                 AS DMD_UPTO_PRV_MNTH_WC_9,
    coalesce(SUM(DMD_FOR_MTH_WC),NULL,0.0,SUM(DMD_FOR_MTH_WC))                             AS DMD_FOR_MTH_WC_10,
    coalesce(SUM(TOTAL_DUES),NULL,0.0,SUM(TOTAL_DUES))                             AS TOTAL_DUES_11,
    coalesce(SUM(COLN_UPTO_PRV_MTH_YES_YR_WC),NULL,0.0,SUM(COLN_UPTO_PRV_MTH_YES_YR_WC))   AS COLN_UPTO_PRV_MTH_YES_YR_WC_12,
    coalesce(SUM(COLN_UPTO_PRV_MTH_CR_YR_DMD),NULL,0.0,SUM(COLN_UPTO_PRV_MTH_CR_YR_DMD))   AS COLN_UPTO_PRV_MTH_CR_YR_DMD_13,
    coalesce(SUM(COLN_FOR_MTH_YES_YR_WC),NULL,0.0,SUM(COLN_FOR_MTH_YES_YR_WC))             AS COLN_FOR_MTH_YES_YR_WC_14,
    coalesce(SUM(COLN_FOR_MTH_WC),NULL,0.0,SUM(COLN_FOR_MTH_WC))                           AS COLN_FOR_MTH_WC_15,
    coalesce(SUM(TOT_COLN_YES_YR_DMD),NULL,0.0,SUM(TOT_COLN_YES_YR_DMD))                   AS TOT_COLN_YES_YR_DMD_16,
    coalesce(SUM(TOT_COLN_FOR_YR_DMD),NULL,0.0,SUM(TOT_COLN_FOR_YR_DMD))                   AS TOT_COLN_FOR_YR_DMD_17,
    coalesce(SUM(BALANCE_18),NULL,0.0,SUM(BALANCE_18))                                     AS BALANCE_18,
    coalesce(SUM(NET_DUE),NULL,0.0,SUM(NET_DUE))                                           AS NET_DUE_19 ,BEN_TYPE_ID ,
    coalesce(SUM(COLN_INCLUDE_CHARGES),NULL,0.0,SUM(COLN_INCLUDE_CHARGES))                                           AS 	COLN_INCLUDE_CHARGES  

  FROM PMS_DCB_LEDGER_ACTUAL   
  WHERE   month=$P{month}::int and year=$P{year}::int 
  GROUP BY office_id,     BEN_TYPE_ID,    YEAR,    MONTH,  OFFICE_NAME 
  )rec
   
  on rec.BEN_TYPE_ID=bt2.BEN_TYPE_ID
  
 
  
  )

order by bt.BEN_TYPE_GROUP]]></queryString>

