	<parameter name="divby" isForPrompting="true" class="java.lang.Integer"/>
	<queryString><![CDATA[select 
   BEN_TYPE_ID_SUB,
  round ( sum(NET_DUE_19)/ $P{divby} ,2)  as NET_DUE_19,sum(BALANCE) / $P{divby} as BALANCE,
  BEN_TYPE_DESC_SUB,MONTH,YEAR, sum((NET_DUE_19+BALANCE)) / $P{divby} as total
from 
(
select 
    NET_DUE_19,BALANCE,
    case when BEN_TYPE_ID_SUB > 6 then 
    7
    else
    BEN_TYPE_ID_SUB
    end as BEN_TYPE_ID_SUB,
    case when BEN_TYPE_ID_SUB > 6 then 
    'Private'
    else
    BEN_TYPE_DESC_SUB
    end as BEN_TYPE_DESC_SUB,YEAR,MONTH
from
(
SELECT BEN_TYPE_ID_SUB,BEN_TYPE_DESC_SUB,DECODE(MONTH,1,'JANUARY', 2,'February', 3,'March', 4,'April', 5,'May', 6,'June', 7,'July', 8,'August', 9,'September', 10,'October', 11,'November', 12,'December', 'N/A')MONTH,
  YEAR, DECODE(SUM(BALANCE_18)+sum(CB_INT_AMT_WC),NULL,0.0,SUM(BALANCE_18)+sum(CB_INT_AMT_WC))   AS NET_DUE_19,
 DECODE(SUM(BALANCE),NULL,0.0,SUM(BALANCE))  as BALANCE
FROM PMS_DCB_LEDGER_ACTUAL a
WHERE MONTH      =$P{month} and NET_DUE > 0 
AND YEAR         =$P{year}  
GROUP BY YEAR,
  MONTH,
 BEN_TYPE_ID_SUB  ,
  BEN_TYPE_DESC_SUB
  order by BEN_TYPE_ID_SUB

)
)
where BEN_TYPE_DESC_SUB is not null group by BEN_TYPE_ID_SUB, BEN_TYPE_DESC_SUB,MONTH,YEAR
order by BEN_TYPE_ID_SUB]]></queryString>

