	<queryString><![CDATA[select BENEFICIARY_NAME,BEN_TYPE_DESC,SCH_NAME,QTY_SUPPLIED,NET_DUE,COLN_INCLUDE_CHARGES,DMD_FOR_MTH_WC,month,year,
(select OFFICE_NAME from COM_MST_OFFICES where office_id=$P{office_id})as OFFICE_NAME
from PMS_DCB_LEDGER_ACTUAL  where 

TO_DATE((MONTH ||'-'
                         || YEAR),'mm-yyyy') BETWEEN TO_DATE($P{Frm_month}
                         ||'-'
                         ||$P{year},'mm-yyyy')
                          AND to_date($P{month}
                         ||'-'
                         ||$P{to_year},'mm-yyyy')


and old_office_id=$P{office_id} and BENEFICIARY_SNO=$P{ben_name} order by year,month,BEN_TYPE_DESC,SCH_NAME]]></queryString>

