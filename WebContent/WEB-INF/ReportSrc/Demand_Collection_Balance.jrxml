	<parameter name="year1" isForPrompting="true" class="java.lang.String"/>
	<parameter name="year2" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT sch.SCH_NAME,
  dmd,
  collection,
  arcol,
  arcol + collection          AS tot,
  (dmd) - ( collection+arcol) AS bal
FROM (
  (SELECT (SUM(coln_for_mth_yes_yr_wc) + SUM(COLN_INCLUDE_CHARGES))-(SUM(DMD_PRV_MTH_WC)) AS arcol,
    SUM(DMD_FOR_MTH_WC)                                                              AS dmd,
    SUM(COLN_INCLUDE_CHARGES)                                                        AS collection,
    sch_sno
  FROM PMS_DCB_LEDGER_ACTUAL
  WHERE ( (MONTH >=4
  AND YEAR       <=$P{year1} )
  OR (MONTH      <=3
  AND YEAR       >=$P{year2}) )
  GROUP BY sch_sno
  )act
JOIN
  ( SELECT SCH_SNO,SCH_NAME FROM PMS_SCH_MASTER
  )sch
ON sch.SCH_SNO=act.sch_sno )

ORDER BY SCH_NAME]]></queryString>

