	<queryString><![CDATA[SELECT *
FROM
  (SELECT a1.office_name ,
    a1.office_id,
    b1.office_name AS regname,
    c1.office_name AS circle
  FROM com_mst_all_offices_view a1,
    com_mst_all_offices_view b1,
    com_mst_all_offices_view c1
  WHERE a1.office_level_id='DN'
  AND b1.office_level_id  ='RN'
    --   AND a1.office_id        =5082
  AND a1.region_office_id =b1.office_id
  AND a1.region_office_id =c1.region_office_id
  AND a1.circle_office_id =c1.office_id
  AND EXISTS
    (SELECT office_id FROM pms_dcb_div_dist_map c WHERE c.office_id=a1.office_id
    )
  )OFF
JOIN
  (
    SELECT SUM(a.QTY_CONSUMED) AS qty, SUM(a.QTY_CONSUMED) * 150 as amt2, SUM(a.TOTAL_AMT) AS amt, a.office_id  FROM PMS_DCB_WC_BILLING a  
    WHERE ( (a.month>=4 AND a.YEAR=2013 ) OR (a.month<=3 AND a.YEAR=2014) ) 
    AND a.beneficiary_sno IN (SELECT b.beneficiary_sno FROM pms_dcb_mst_beneficiary b WHERE b.beneficiary_type_id_sub NOT IN (1,2,3,4,6,5)  AND status='L' AND b.office_id= a.office_id )
    GROUP BY a.office_id 
  )wc
on  wc.office_id=off.office_id]]></queryString>

