  sitem.SUB_ITEM_DESC,SCH_TYPE_ID,(select SCH_TYPE_DESC from   PMS_SCH_LKP_TYPE   where SCH_TYPE_ID=ac.SCH_TYPE_ID) as sch_type_desc
FROM
  (SELECT ACC_CODE_SNO,
    CODE_W_E_F,SCH_TYPE_ID,
    MAIN_ITEM_SNO,
    SUB_ITEM_SNO,
    ACCOUNT_HEAD_CODE,
    ACTIVE_STATUS
  FROM PMS_AME_MST_ITEM_ACC_CODE
  WHERE ACTIVE_STATUS='A'
