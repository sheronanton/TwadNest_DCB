	<parameter name="year" isForPrompting="true" class="java.lang.Integer"/>
	<parameter name="month" isForPrompting="true" class="java.lang.Integer"/>
	<parameter name="divby" isForPrompting="true" class="java.lang.Integer"/>
	<queryString><![CDATA[select K.* from
(select d.OFFICE_NAME,b.ACCOUNTING_FOR_OFFICE_ID as office_id,a.fas,c.dcb from
(SELECT t.for_accounting_unit_id as offic,
   sum(t.amount)/$P{divby} as fas
FROM fas_adjust_memo_mst m,
  fas_adjust_memo_trn t
WHERE m.accounting_unit_id    =t.accounting_unit_id
AND m.accounting_for_office_id=t.accounting_for_office_id
AND m.cashbook_year           =t.cashbook_year
AND m.cashbook_month          =t.cashbook_month
AND m.voucher_no              =t.voucher_no
AND m.memo_status             ='L'
AND m.cashbook_year           =$P{year}
and m.cashbook_month          =$P{month}
and t.SUB_LEDGER_TYPE_CODE =15
and t.ACCOUNT_HEAD_CODE=610101
and t.REMARKS like '%WATER%'
--AND t.acceptance_status      IS NULL
group by t.for_accounting_unit_id
)a
left outer join
(
select ACCOUNTING_UNIT_ID, ACCOUNTING_FOR_OFFICE_ID from FAS_MST_ACCT_UNIT_OFFICES
where CLOSED IS NULL 
)b
on a.offic=b.ACCOUNTING_UNIT_ID
left outer join
(
select OFFICE_ID,sum(AMOUNT)/$P{divby} as dcb,CASHBOOK_MONTH,CASHBOOK_YEAR from PMS_DCB_OTHER_CHARGES a
where  CR_DR_INDICATOR='CR' and INCLUDE_CB='Y'
and VOUCHER_NO in (select VOUCHER_NO from FAS_JOURNAL_MASTER b where JOURNAL_TYPE_CODE in (70,113) and JOURNAL_STATUS='L' and
a.VOUCHER_YEAR=$P{year}  and a.VOUCHER_MONTH=$P{month} and a.OFFICE_ID=b.ACCOUNTING_FOR_OFFICE_ID
)group by OFFICE_ID, CASHBOOK_MONTH, CASHBOOK_YEAR
)c
on b.ACCOUNTING_FOR_OFFICE_ID=c.OFFICE_ID 
left outer join
(
SELECT OFFICE_ID,OFFICE_NAME FROM COM_MST_OFFICES
)d
on b.ACCOUNTING_FOR_OFFICE_ID=d.OFFICE_ID 
)k where office_id in (select OFFICE_ID from PMS_DCB_MST_BENEFICIARY where STATUS='L' ) 
order by OFFICE_NAME]]></queryString>

