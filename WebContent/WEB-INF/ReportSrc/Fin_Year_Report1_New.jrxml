	<queryString><![CDATA[SELECT  sum(ac.OPENING_BAL_MC_3)         AS OPENING_BAL_MC_3,
	 sum(  ac.ADDNS_IF_ANY_3A)                AS ADDNS_IF_ANY_3A ,
 	sum(  ac.COL_UTO_PRV_MNH_MC_4)           AS COL_UTO_PRV_MNH_MC_4 ,
 	sum(  ac.COLL_FOR_MTH_MAINT_5)                 AS COLL_FOR_MTH_MAINT_5,
 	sum(  ac.TOTAL_COLLN_6)                 AS TOTAL_COLLN_6,
	sum(  ac.BALANCE_7)                      AS BALANCE_7,
 	sum(  ac.OPENING_BAL_WC_8)               AS OPENING_BAL_WC_8,
 	sum(  ac.DMD_UPTO_PRV_MNTH_WC_9)        AS DMD_UPTO_PRV_MNTH_WC_9,
 	sum(  ac.DMD_FOR_MTH_WC_10)              AS DMD_FOR_MTH_WC_10,
 	sum(  ac.TOTAL_DUES_11)                 AS TOTAL_DUES_11,
	sum(ac.COLN_UPTO_PRV_MTH_YES_YR_WC_12) AS COLN_UPTO_PRV_MTH_YES_YR_WC_12,
   	sum(ac.COLN_UPTO_PRV_MTH_CR_YR_DMD_13) AS COLN_UPTO_PRV_MTH_CR_YR_DMD_13,
   	sum(ac.COLN_FOR_MTH_YES_YR_WC_14)      AS COLN_FOR_MTH_YES_YR_WC_14,
 --  	trunc(ac.COLN_FOR_MTH_WC_15)              AS COLN_FOR_MTH_WC_15,
  	sum( ac.TOT_COLN_YES_YR_DMD_16)        AS TOT_COLN_YES_YR_DMD_16,
 	sum(  ac.TOT_COLN_FOR_YR_DMD_17)         AS TOT_COLN_FOR_YR_DMD_17,
	sum(  ac.BALANCE_18)                     AS BALANCE_18,
  	sum( ac.NET_DUE_19 )                   AS NET_DUE_19,
		dis.DISTRICT_NAME                 AS DISTRICT_NAME,DISTRICT_CODE,
  sum(rpt2.COLN_FOR_MTH_WC_15_new)   as COLN_FOR_MTH_WC_15
FROM
  (SELECT DECODE(SUM(OPENING_BAL_MC),NULL,0.0,SUM(OPENING_BAL_MC))                      AS OPENING_BAL_MC_3,
    DECODE(SUM(ADDNS_IF_ANY),NULL,0.0,SUM(ADDNS_IF_ANY))                                AS ADDNS_IF_ANY_3A,
    DECODE(SUM(COL_UTO_PRV_MNH_MC),NULL,0.0,SUM(COL_UTO_PRV_MNH_MC))                    AS COL_UTO_PRV_MNH_MC_4,
    DECODE(SUM(COLL_FOR_MTH_MAINT),NULL,0.0,SUM(COLL_FOR_MTH_MAINT))                    AS COLL_FOR_MTH_MAINT_5,
    DECODE(SUM(TOTAL_COLLN),NULL,0.0,SUM(TOTAL_COLLN))                                  AS TOTAL_COLLN_6,
    DECODE(SUM(BALANCE),NULL,0.0,SUM(BALANCE))                                          AS BALANCE_7,
    DECODE(SUM(OPENING_BAL_WC),NULL,0.0,SUM(OPENING_BAL_WC))                            AS OPENING_BAL_WC_8,
    DECODE(SUM(DMD_UPTO_PRV_MNTH_WC),NULL,0.0,SUM(DMD_UPTO_PRV_MNTH_WC))                AS DMD_UPTO_PRV_MNTH_WC_9,
    DECODE(SUM(DMD_FOR_MTH_WC),NULL,0.0,SUM(DMD_FOR_MTH_WC))                            AS DMD_FOR_MTH_WC_10,
    DECODE(SUM(TOTAL_DUES),NULL,0.0,SUM(TOTAL_DUES))                                    AS TOTAL_DUES_11,
    DECODE(SUM(COLN_UPTO_PRV_MTH_YES_YR_WC),NULL,0.0,SUM(COLN_UPTO_PRV_MTH_YES_YR_WC))  AS COLN_UPTO_PRV_MTH_YES_YR_WC_12,
    DECODE(SUM(COLN_UPTO_PRV_MTH_CR_YR_DMD),NULL,0.0,SUM(COLN_UPTO_PRV_MTH_CR_YR_DMD))  AS COLN_UPTO_PRV_MTH_CR_YR_DMD_13,
    DECODE(SUM(COLN_FOR_MTH_YES_YR_WC),NULL,0.0,SUM(COLN_FOR_MTH_YES_YR_WC))            AS COLN_FOR_MTH_YES_YR_WC_14,
    DECODE(SUM(COLN_INCLUDE_CHARGES),NULL,0.0,SUM(COLN_INCLUDE_CHARGES))                          AS COLN_FOR_MTH_WC_15,
    DECODE(SUM(TOT_COLN_YES_YR_DMD),NULL,0.0,SUM(TOT_COLN_YES_YR_DMD))                  AS TOT_COLN_YES_YR_DMD_16,
    DECODE(SUM(TOT_COLN_FOR_YR_DMD),NULL,0.0,SUM(TOT_COLN_FOR_YR_DMD))                  AS TOT_COLN_FOR_YR_DMD_17,
    DECODE(SUM(BALANCE_18),NULL,0.0,SUM(BALANCE_18))                                    AS BALANCE_18,
    DECODE(SUM(NET_DUE),NULL,0.0,SUM(NET_DUE))                                          AS NET_DUE_19,
 
    DISTRICT_CODE,  
    BENEFICIARY_SNO 
    
  FROM PMS_DCB_LEDGER_ACTUAL 
  WHERE YEAR   =$P{year}
  AND MONTH    =4
 
  GROUP BY   
    DISTRICT_CODE ,
    BENEFICIARY_SNO 
  )ac
 
  
LEFT JOIN
  ( SELECT DISTINCT DISTRICT_NAME, district_code FROM PMS_DCB_LEDGER_ACTUAL  
  )dis
ON dis.district_code=ac.district_code
 
LEFT  join
  (
  SELECT  DECODE(SUM(COLN_INCLUDE_CHARGES),NULL,0.0,SUM(COLN_INCLUDE_CHARGES))  AS COLN_FOR_MTH_WC_15_new,  DISTRICT_CODE,    BENEFICIARY_SNO    
  FROM PMS_DCB_LEDGER_ACTUAL
  WHERE  (to_date((01 || '-' || month || '-' || year),'dd-MM-yyyy') >= to_date('1-4-' || $P{year},'dd-mm-yyyy')) and    (to_date((01 || '-' || month || '-' || year),'dd-MM-yyyy') <= to_date('31-3-'||$P{year1},'dd-mm-yyyy'))	
  GROUP BY DISTRICT_CODE ,BENEFICIARY_SNO
  )rpt2
  on     
    rpt2.BENEFICIARY_SNO=ac.BENEFICIARY_SNO   
    and rpt2.DISTRICT_CODE=ac.DISTRICT_CODE
    group by DISTRICT_CODE,DISTRICT_NAME
ORDER BY   DISTRICT_CODE]]></queryString>

