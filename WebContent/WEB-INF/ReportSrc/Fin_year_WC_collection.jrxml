	<queryString><![CDATA[SELECT act.office_id,act.office_name,dmd.wcdemand as DEMAND,act.collection,apr.op,act.SCH_TYPE_ID_SUB,typedesc.SCH_TYPE_SUB_DESC
FROM
  (SELECT office_id,office_name,SCH_TYPE_ID_SUB,SUM(COLN_INCLUDE_CHARGES)   AS collection  FROM PMS_DCB_LEDGER_ACTUAL
  WHERE ( MONTH >=4 AND YEAR=2013) AND (MONTH <=12 AND YEAR=2013)
  GROUP BY office_id,office_name,SCH_TYPE_ID_SUB
  )act
JOIN
  (SELECT SCH_TYPE_ID_SUB,SUM(DMD_FOR_MTH_WC)     AS wcdemand , office_id FROM PMS_DCB_LEDGER_ACTUAL
  WHERE ( MONTH >=4 AND YEAR=2013) AND (MONTH <=11 AND YEAR =2013)
  GROUP BY office_id,SCH_TYPE_ID_SUB
  )dmd
ON dmd.office_id=act.office_id AND dmd.SCH_TYPE_ID_SUB=act.SCH_TYPE_ID_SUB
JOIN
  (SELECT SUM(OPENING_BAL_WC)   AS op,office_id,SCH_TYPE_ID_SUB FROM PMS_DCB_LEDGER_ACTUAL
  WHERE YEAR=2013 AND MONTH =12 GROUP BY office_id,SCH_TYPE_ID_SUB
  )apr
ON apr.office_id=act.office_id AND apr.SCH_TYPE_ID_SUB=act.SCH_TYPE_ID_SUB
LEFT JOIN
  (SELECT DISTINCT SCH_TYPE_ID_SUB,SCH_TYPE_SUB_DESC FROM PMS_DCB_APPLICABLE_SCH_TYPE
  )typedesc
ON typedesc.SCH_TYPE_ID_SUB =act.SCH_TYPE_ID_SUB AND typedesc.SCH_TYPE_ID_SUB=apr.SCH_TYPE_ID_SUB
AND act.SCH_TYPE_ID_SUB     =apr.SCH_TYPE_ID_SUB 
ORDER BY typedesc.SCH_TYPE_ID_SUB,act.SCH_TYPE_ID_SUB]]></queryString>

