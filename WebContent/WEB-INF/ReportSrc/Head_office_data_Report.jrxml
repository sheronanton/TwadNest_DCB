	<parameter name="year" isForPrompting="true" class="java.lang.String"/>
	<parameter name="month" isForPrompting="true" class="java.lang.String"/>
	<parameter name="mvalue" isForPrompting="true" class="java.lang.String"/>
	<parameter name="april_year" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[select 
mas.*,
decode (april_ob.OB_WC,null,0,april_ob.OB_WC) as OB_WC,
decode (april_ob.OB_MANIT,null,0,april_ob.OB_MANIT) as OB_MANIT,
decode (april_ob.OB_INT,null,0,april_ob.OB_INT) as OB_INT,
decode (april_ob.OB_MAINT_INT,null,0,april_ob.OB_MAINT_INT) as OB_MAINT_INT,
decode (QTY_CONSUMED,null,0,QTY_CONSUMED) as QTY_CONSUMED
from
(
SELECT a.office_id ,
  a.office_name,
  a.BENEFICIARY_SNO,
    (select     case when ADDED_AREA='y' or  ADDED_AREA='Y'  then BENEFICIARY_NAME || '(Added)' else BENEFICIARY_NAME end as BENEFICIARY_NAME from PMS_DCB_MST_BENEFICIARY where BENEFICIARY_SNO= a.BENEFICIARY_SNO) as BENEFICIARY_NAME,
  --a.BENEFICIARY_NAME, 
  decode(a.BLOCK_SNO,null,'',a.BLOCK_SNO) as BLOCK_SNO,
  decode(a.BEN_TYPE_DESC,null,'',a.BEN_TYPE_DESC) as BTYPE,
  decode(a.block_name,null,'',a.block_name) as  block_name,
  a.BEN_TYPE_ID as BENEFICIARY_TYPE_ID,
  decode(a.BEN_TYPE_ID_SUB,null,'',BEN_TYPE_ID_SUB) as BENEFICIARY_TYPE_ID_SUB,
  decode(a.BEN_TYPE_DESC_SUB,null,'',a.BEN_TYPE_DESC_SUB) as BTYPE_SUB,
  a.DISTRICT_CODE,(select DIST_DISPLAY_SNO from PMS_DCB_MST_CENSUS_DIST where DISTRICT_CODE=a.DISTRICT_CODE) as dis_order,
  a.DISTRICT_NAME as DISTNAME,
  a.SCH_SNO,
  a.SCH_NAME,
 (SELECT b.BEN_TYPE_GROUP
    FROM PMS_DCB_BEN_TYPE b
    WHERE b.BEN_TYPE_ID=a.BEN_TYPE_ID_SUB ) as BEN_TYPE_GROUP ,
    (SELECT b.BEN_TYPE_GROUP_NAME 
    FROM PMS_DCB_BEN_TYPE b
    WHERE b.BEN_TYPE_ID=a.BEN_TYPE_ID_SUB ) as BEN_TYPE_GROUP_NAME ,
    ADD_CHARGES_WC as amountdr,
MINUS_CHARGES_WC as amountcr,
decode (COLL_FOR_MTH_MAINT,null,0,COLL_FOR_MTH_MAINT) as COLL_FOR_MTH_MAINT,
(COLN_INCLUDE_CHARGES +decode (COLN_FOR_MTH_YES_YR_WC,null,0,COLN_FOR_MTH_YES_YR_WC)+decode (COLL_FOR_MTH_MAINT,null,0,COLL_FOR_MTH_MAINT) )  as Amount,DMD_FOR_MTH_WC as TOTAL_AMT,ADD_DMD

FROM pms_dcb_ledger_actual a
where month =$P{month} and year=$P{year}  

order by   a.office_id,
  a.BEN_TYPE_ID_SUB,
  a.BENEFICIARY_Name
  )mas
   
  LEFT JOIN
  (SELECT a.BENEFICIARY_SNO,
    a.FIN_YEAR,
    a.MONTH,
    a.OFFICE_ID ,a.sch_sno,
    sum(OB_CUR_YR_WC)+sum(OB_YESTER_YR_WC)            AS OB_WC,
    sum(OB_MAINT_CHARGES)                        AS OB_MANIT,
    sum(OB_INT_AMT_WC)                           AS OB_INT,
    sum(OB_INT_PRV_YR_MAINT)+sum(OB_INT_CUR_YR_MAINT) AS OB_MAINT_INT
  FROM PMS_DCB_OB_YEARLY a
  WHERE a.fin_year=$P{april_year}
  AND a.month     =4
  group by OFFICE_ID,FIN_YEAR,MONTH,BENEFICIARY_SNO,sch_sno
  )april_ob
ON april_ob.BENEFICIARY_SNO=mas.BENEFICIARY_SNO
AND april_ob.OFFICE_ID     =mas.OFFICE_ID
AND april_ob.sch_sno     =mas.sch_sno
LEFT JOIN
  (SELECT c.BENEFICIARY_SNO,
    c.OFFICE_ID,
    c.MONTH,
    c.YEAR,c.SCHEME_SNO,        
    SUM(c.QTY_CONSUMED) AS QTY_CONSUMED,
    SUM(c.TOTAL_AMT)    AS TOTAL_AMT
  FROM PMS_DCB_WC_BILLING c
  WHERE c.month=$P{month}
    AND c.year   =$P{year}
  GROUP BY OFFICE_ID,
    YEAR,
    MONTH,
    BENEFICIARY_SNO ,SCHEME_SNO 
     
  ) water_charges
ON water_charges.BENEFICIARY_SNO =mas.BENEFICIARY_SNO
AND water_charges.OFFICE_ID      =mas.OFFICE_ID
and water_charges.SCHEME_SNO      =april_ob.sch_sno
and water_charges.SCHEME_SNO      =mas.sch_sno 
AND water_charges.BENEFICIARY_SNO=april_ob.BENEFICIARY_SNO
AND water_charges.OFFICE_ID      =april_ob.OFFICE_ID
order by dis_order,OFFICE_ID,BENEFICIARY_TYPE_ID]]></queryString>

