	<parameter name="year" isForPrompting="true" class="java.lang.String"/>
	<parameter name="month" isForPrompting="true" class="java.lang.String"/>
	<parameter name="mvalue" isForPrompting="true" class="java.lang.String"/>
	<parameter name="april_year" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT
      rec.CASHBOOK_MONTH,
      rec.CASHBOOK_YEAR,
      rec.ACCOUNTING_FOR_OFFICE_ID,
      off.office_name,
      rec.DISTRICT_CODE,
      dis.DISTRICT_NAME,
      ben.BLOCK_SNO,
      block.BLOCK_NAME,
      ben.BENEFICIARY_TYPE_ID,
      bentype.BEN_TYPE_DESC,
      rec.BENEFICIARY_TYPE_ID_SUB,
      bensubtype.BEN_TYPE_DESC as BEN_TYPE_Sub_DESC,
      groupname.BEN_TYPE_GROUP,
      groupname. BEN_TYPE_GROUP_NAME,
      rec.BENEFICIARY_SNO,
      rec.BENEFICIARY_NAME,
      rec.SCH_SNO,
      sch.sch_name,
      april_ob.OB_WC,
      april_ob.OB_MANIT,
      april_ob.OB_INT,
      april_ob.OB_MAINT_INT,
      rec.AMOUNT   as Collection,
      dem.TOTAL_AMT,
      dem.QTY_CONSUMED,
      dem.CR,dem.DR,dem.ARR
      
          
    FROM
        (
            SELECT
               ACCOUNTING_FOR_OFFICE_ID,
               AMOUNT   ,
               DISTRICT_CODE,
               SUB_LEDGER_CODE,
               SCH_SNO,
               BENEFICIARY_NAME,
               BENEFICIARY_SNO,
               CASHBOOK_MONTH,
               CASHBOOK_YEAR,
               BENEFICIARY_TYPE_ID_SUB
               
            FROM
                PMS_DCB_FAS_RECEIPT_VIEW_ALL
                
  --      where CASHBOOK_MONTH=2 and   CASHBOOK_YEAR=2019
  where  CASHBOOK_MONTH=$P{month}   AND CASHBOOK_YEAR=$P{year}
        )rec
      left JOIN (
           SELECT
                office_id,
                office_name
            FROM
                com_mst_all_offices_view
        ) off ON off.office_id = rec.ACCOUNTING_FOR_OFFICE_ID
         left JOIN (
            SELECT
            DISTRICT_CODE,
            OFFICE_ID,
            BLOCK_SNO,
            beneficiary_sno,
            BENEFICIARY_TYPE_ID
          FROM
                pms_dcb_mst_beneficiary
            WHERE
                status = 'L'
        ) ben ON ben.beneficiary_sno = rec.BENEFICIARY_SNO and
         ben.OFFICE_ID=rec.ACCOUNTING_FOR_OFFICE_ID
        left JOIN (
            SELECT
           DISTRICT_CODE,
           DISTRICT_NAME
            FROM
                COM_MST_DISTRICTS
         ) dis ON dis.DISTRICT_CODE = ben.DISTRICT_CODE 
          left JOIN (
            SELECT
          BLOCK_sno,
          block_name
            FROM
                COM_MST_BLOCKS
         ) block ON block.BLOCK_sno = ben.BLOCK_SNO
           left JOIN (
            SELECT
           BEN_TYPE_ID,
           BEN_TYPE_DESC
            FROM
                PMS_DCB_BEN_TYPE
         ) bentype ON bentype.BEN_TYPE_ID = ben.BENEFICIARY_TYPE_ID 
         
          left JOIN (
            SELECT
           BEN_TYPE_ID,
           BEN_TYPE_DESC
            FROM
                PMS_DCB_BEN_TYPE
         ) bensubtype ON bensubtype.BEN_TYPE_ID = rec.BENEFICIARY_TYPE_ID_SUB 
         
         left Join (
         SELECT  BEN_TYPE_ID,
         BEN_TYPE_GROUP,
         BEN_TYPE_GROUP_NAME
    FROM PMS_DCB_BEN_TYPE 
    )groupname on groupname.BEN_TYPE_ID=rec.BENEFICIARY_TYPE_ID_SUB
    
         
         LEFT JOIN (
            SELECT
                sch_sno,
                sch_name
            FROM
                pms_sch_master
        ) sch ON sch.sch_sno = rec.SCH_SNO
        
        
        
        LEFT JOIN (
            SELECT  
              
              MONTH ,YEAR,SCHEME_SNO,OFFICE_ID,BENEFICIARY_SNO,QTY_CONSUMED,TOTAL_AMT,CR,DR,ARR
            FROM
                PMS_DCB_REPORT
        ) Dem ON Dem.SCHEME_SNO = rec.SCH_SNO and
        Dem.OFFICE_ID=rec.ACCOUNTING_FOR_OFFICE_ID      and
        Dem.BENEFICIARY_SNO=rec.BENEFICIARY_SNO       and
        Dem.MONTH=rec.CASHBOOK_MONTH        and
        Dem.year=rec.CASHBOOK_YEAR
        
        
        
        LEFT JOIN
  (SELECT a.BENEFICIARY_SNO,
    a.FIN_YEAR,
    a.MONTH,
    a.OFFICE_ID ,a.sch_sno,
    sum(OB_CUR_YR_WC)+sum(OB_YESTER_YR_WC)            AS OB_WC,
    sum(OB_MAINT_CHARGES)                        AS OB_MANIT,
    sum(OB_INT_AMT_WC)                           AS OB_INT,
    sum(OB_INT_PRV_YR_MAINT)+sum(OB_INT_CUR_YR_MAINT) AS OB_MAINT_INT
  FROM PMS_DCB_OB_YEARLY a
 WHERE a.fin_year=$P{april_year}
--   WHERE a.fin_year=2018
  AND a.month     =4
  group by OFFICE_ID,FIN_YEAR,MONTH,BENEFICIARY_SNO,sch_sno
  )april_ob
ON april_ob.BENEFICIARY_SNO=rec.BENEFICIARY_SNO
AND april_ob.OFFICE_ID     =rec.ACCOUNTING_FOR_OFFICE_ID
AND april_ob.sch_sno     =rec.sch_sno]]></queryString>

