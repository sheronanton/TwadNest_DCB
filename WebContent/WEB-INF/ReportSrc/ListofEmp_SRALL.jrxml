	<queryString><![CDATA[select * from
(
select b.office_id,d.department_id,b.OFFICE_NAME,c.EMPLOYEE_ID,c.EMPLOYEE_NAME||'.'||c.EMPLOYEE_INITIAL EMPLOYEE_NAME,
decode(e.DESIGNATION,null,' ',e.DESIGNATION) DESIGNATION, f.EMPLOYEE_STATUS_DESC,h.HIERARCHICAL_SEQUENCE
from HRM_EMP_CONTROLLING_OFFICE a
inner join COM_MST_OFFICES b on b.OFFICE_ID=a.CONTROLLING_OFFICE_ID
inner join HRM_MST_EMPLOYEES c on c.EMPLOYEE_ID=a.EMPLOYEE_ID
left outer join HRM_EMP_CURRENT_POSTING d on d.EMPLOYEE_ID = a.EMPLOYEE_ID
left outer join HRM_MST_DESIGNATIONS e on e.DESIGNATION_ID = d.DESIGNATION_ID
left outer join hrm_mst_employee_status f on f.EMPLOYEE_STATUS_ID=d.EMPLOYEE_STATUS_ID	
inner join com_mst_office_levels h on h.office_level_id = b.Office_level_id
) x
left outer join (
    select a.OTHER_DEPT_ID,a.OTHER_DEPT_OFFICE_ID,decode(a.OTHER_DEPT_OFFICE_NAME,null,' ',a.OTHER_DEPT_OFFICE_NAME) current_office1 from hrm_mst_other_dept_offices a
) y on y.OTHER_DEPT_OFFICE_ID = x.office_id and y.OTHER_DEPT_ID = x.department_id
left outer join (
  select a.office_id,decode(a.office_name,null,' ',a.office_name) current_office2 from com_mst_offices a
) z on z.office_id =x.office_id
order by HIERARCHICAL_SEQUENCE,OFFICE_NAME,EMPLOYEE_NAME]]></queryString>

