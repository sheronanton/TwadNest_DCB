	<queryString><![CDATA[SELECT DISTRICT_NAME,
  CASE
    WHEN NET_DUE_19 < 0
    THEN 0
    ELSE round ( NET_DUE_19,2)  
  END AS NET_DUE_19,
  DISTRICT_NAME 
FROM
  (SELECT SUM(NET_DUE)   AS NET_DUE_19,
    DISTRICT_NAME 
  FROM
    (SELECT NET_DUE_LAKH as NET_DUE,
      DISTRICT_NAME 
    FROM PMS_DCB_LEDGER_ACTUAL
    WHERE MONTH        =$P{month}
    AND YEAR           =$P{year}
    AND BEN_TYPE_ID=1
    AND net_due        >=0
    )
  GROUP BY DISTRICT_NAME 
  ORDER BY DISTRICT_NAME 
  )]]></queryString>

