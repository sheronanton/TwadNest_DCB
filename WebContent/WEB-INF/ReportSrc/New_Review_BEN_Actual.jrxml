	<queryString><![CDATA[SELECT BENEFICIARY_NAME,
  SCH_TYPE_ID_SUB      AS SCH_TYPE_ID,
  SCH_TYPE_DESC_REPORT AS SCH_TYPE_DESC ,
  BEN_TYPE_ID_SUB,
  BEN_TYPE_DESC_SUB,
  BLOCK_NAME,
  ROUND(TRUNC(net_due19 ,3),2) AS net_due19,
  BLOCK_SNO,
  ROUND(TRUNC(NET_DUE,3),2)                       AS NET_DUE,
  ROUND(TRUNC((net_due19-col10) ,3),2)            AS DMD_ARR_UPTO_MTH_WC ,
  ROUND(TRUNC(d2        +COLL_FOR_MTH_MAINT,3),2) AS COLN_ARR_MTH_WC ,
  ROUND(TRUNC(col10 ,3),2)                        AS DMD_PRV_MTH_WC,
  ROUND(TRUNC(col10_cur,3),2)                     AS col10_cur,
  ROUND(TRUNC(COLN_ADJUSTED ,3),2)                AS COLN_FOR_MTH_WC,
  CASE
    WHEN (ROUND(TRUNC(((net_due19 -col10))+dmd ,3),2)             > 0
    AND ROUND(TRUNC( (d2         +COLL_FOR_MTH_MAINT),3),2) > 0 )
    THEN (ROUND(TRUNC( (d2       +COLL_FOR_MTH_MAINT) ,3),2) /
ROUND(TRUNC(((net_due19-col10)+dmd) ,3),2))* 100
    ELSE 0
  END AS avg1,
  CASE
    WHEN ROUND(TRUNC(col10 ,3),2) <= 0
    THEN 0
    ELSE ROUND(ROUND(TRUNC(COLN_ADJUSTED ,3),2)/ ROUND(TRUNC(col10
,3),2),2) * 100
  END AS AVG2,
  cb,
  dmd
FROM
  (SELECT BENEFICIARY_NAME,
    SUM(DMD_ARR_UPTO_MTH_WC)    AS DMD_ARR_UPTO_MTH_WC,
    SUM(dmd_for_mth_wc)         AS col10_cur,
    SUM(COLN_INCLUDE_CHARGES)   AS col15,
    SUM(coln_for_mth_yes_yr_wc) AS col14,
    SUM(NET_DUE_PRV)            AS net_due19,
    SUM(DMD_PRV_MTH_WC)         AS COL10,
    SUM(ADD_DMD_UPTO)           AS dmd,
    SUM(COLL_FOR_MTH_MAINT)     AS COLL_FOR_MTH_MAINT,
    CASE
      WHEN SUM(COLN_INCLUDE_CHARGES) > SUM(DMD_PRV_MTH_WC)
      THEN SUM(DMD_PRV_MTH_WC)
      ELSE SUM(COLN_INCLUDE_CHARGES)
    END                  AS COLN_ADJUSTED,
    SUM(COLN_FOR_MTH_WC) AS COLN_FOR_MTH_WC,
    SUM(NET_DUE)         AS NET_DUE,
    SUM(coln_for_mth_yes_yr_wc)+(
    CASE
      WHEN (( SUM(COLN_INCLUDE_CHARGES)) < SUM(DMD_PRV_MTH_WC))
      THEN 0
      ELSE ( SUM(COLN_INCLUDE_CHARGES))- SUM(DMD_PRV_MTH_WC)
    END ) AS d2,
    BLOCK_NAME,
    BLOCK_SNO,
    SCH_TYPE_ID_SUB,
    SCH_TYPE_DESC_REPORT ,
    BEN_TYPE_ID_SUB,
    BEN_TYPE_DESC_SUB,
    (SUM(NET_DUE_PRV)-SUM(DMD_PRV_MTH_WC)-SUM(D1))+(SUM(DMD_PRV_MTH_WC)-SUM(COLN_FOR_MTH_WC))
AS CB

  FROM
    (SELECT
      CASE
        WHEN (( (act.COLN_INCLUDE_CHARGES)) < (act.DMD_PRV_MTH_WC))
        THEN 0
        ELSE ( (act.COLN_INCLUDE_CHARGES))- (act.DMD_PRV_MTH_WC)
      END AS d1,
      act.BENEFICIARY_SNO,
      (SELECT
        CASE
          WHEN ADDED_AREA='y'
          OR ADDED_AREA  ='Y'
          THEN ADDED_TO_BEN_NAME
          ELSE BENEFICIARY_NAME
        END AS BENEFICIARY_NAME
      FROM PMS_DCB_MST_BENEFICIARY
      WHERE BENEFICIARY_SNO= act.BENEFICIARY_SNO
      )                            AS BENEFICIARY_NAME,
      (act.DMD_ARR_UPTO_MTH_WC)    AS DMD_ARR_UPTO_MTH_WC,
      (act.dmd_for_mth_wc)         AS dmd_for_mth_wc,
      (act.COLN_INCLUDE_CHARGES)   AS COLN_INCLUDE_CHARGES,
      (act.coln_for_mth_yes_yr_wc) AS coln_for_mth_yes_yr_wc,
      (act.NET_DUE_PRV)            AS NET_DUE_PRV,
      (act.DMD_PRV_MTH_WC)         AS DMD_PRV_MTH_WC,
      (act.COLN_ADJUSTED)          AS COLN_ADJUSTED ,
      (act.coln_for_mth_wc)        AS coln_for_mth_wc,
      (act.NET_DUE)                AS NET_DUE,
      act.COLL_FOR_MTH_MAINT       AS COLL_FOR_MTH_MAINT,
      act.BLOCK_NAME,
      act.BLOCK_SNO,
      act.SCH_TYPE_ID_SUB,
      act.SCH_TYPE_DESC_REPORT ,
      act.BEN_TYPE_ID_SUB,
      ACT.BEN_TYPE_DESC_SUB,
      ACT.ADD_DMD_UPTO
    FROM PMS_DCB_LEDGER_ACTUAL act
    WHERE act.office_id=$P{office_id}
    AND act.MONTH      =$P{month}
    AND act.YEAR       =$P{year}

    )
  GROUP BY BENEFICIARY_NAME, BLOCK_NAME, BLOCK_SNO, SCH_TYPE_ID_SUB,
SCH_TYPE_DESC_REPORT, BEN_TYPE_ID_SUB, BEN_TYPE_DESC_SUB
  )
ORDER BY SCH_TYPE_ID_SUB,
  BEN_TYPE_ID_SUB,
  BLOCK_SNO,
  BENEFICIARY_NAME]]></queryString>

