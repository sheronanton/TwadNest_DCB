	<queryString><![CDATA[SELECT BENEFICIARY_NAME,
  SCH_TYPE_ID,
  SCH_TYPE_DESC,
  BEN_TYPE_ID_SUB,
  BEN_TYPE_DESC_SUB,
  BLOCK_NAME,
  BLOCK_SNO,
  ROUND(TRUNC(NET_DUE      ,3),2)        AS NET_DUE,
  ROUND(TRUNC((net_due19   -col10),3),2) AS DMD_ARR_UPTO_MTH_WC ,
  ROUND(TRUNC(d2+COLL_FOR_MTH_MAINT           ,3),2)        AS COLN_ARR_MTH_WC ,
  ROUND(TRUNC(col10        ,3),2)        AS DMD_PRV_MTH_WC,
  ROUND(TRUNC(col10_cur    ,3),2)        AS col10_cur,
  ROUND(TRUNC(COLN_ADJUSTED,3),2)        AS COLN_FOR_MTH_WC,
  CASE
    WHEN (ROUND(TRUNC((net_due19 -col10),3),2) > 0
    AND ROUND(TRUNC(d2+COLL_FOR_MTH_MAINT           ,3),2)        > 0 )
    THEN (ROUND(TRUNC(d2+COLL_FOR_MTH_MAINT         ,3),2) / ROUND(TRUNC((net_due19-col10),3),2))* 100
    ELSE 0
  END AS avg1,
  CASE
    WHEN ROUND(TRUNC(col10,3),2) <= 0
    THEN 0
    ELSE ROUND(ROUND(TRUNC(COLN_ADJUSTED,3),2)/ ROUND(TRUNC(col10,3),2),2) * 100
  END AS avg2
FROM
  (SELECT BENEFICIARY_SNO ,
    BENEFICIARY_NAME,
    SUM(dmd_for_mth_wc)         AS col10_cur,
    SUM(COLN_INCLUDE_CHARGES)   AS col15,
    SUM(coln_for_mth_yes_yr_wc) AS col14,
    SUM(NET_DUE_PRV)            AS net_due19,
    SUM(DMD_PRV_MTH_WC)         AS col10,
    SUM(COLN_ADJUSTED)          AS COLN_ADJUSTED,
    SUM(COLN_FOR_MTH_WC)        AS COLN_FOR_MTH_WC,
    SUM(NET_DUE)                AS NET_DUE,
    SUM(coln_for_mth_yes_yr_wc)+SUM(d1)                     AS d2,
   sum(COLL_FOR_MTH_MAINT) as COLL_FOR_MTH_MAINT,
    BLOCK_NAME,
    BLOCK_SNO,
    SCH_TYPE_ID,
    SCH_TYPE_DESC ,
    BEN_TYPE_ID_SUB,
    BEN_TYPE_DESC_SUB,
    ((NET_DUE_PRV)-(DMD_PRV_MTH_WC)-(d1))+((DMD_PRV_MTH_WC)-(COLN_FOR_MTH_WC)) AS cb
  FROM
    (SELECT
      CASE
        WHEN (( (COLN_INCLUDE_CHARGES)) < (DMD_PRV_MTH_WC))
        THEN 0
        ELSE ((COLN_INCLUDE_CHARGES))- (DMD_PRV_MTH_WC)
      END AS d1,
      BENEFICIARY_SNO,
      BENEFICIARY_NAME,
      (dmd_for_mth_wc)         AS dmd_for_mth_wc,
      (COLN_INCLUDE_CHARGES)   AS COLN_INCLUDE_CHARGES,
      (coln_for_mth_yes_yr_wc) AS coln_for_mth_yes_yr_wc,
      (NET_DUE_PRV)            AS NET_DUE_PRV,
      (DMD_PRV_MTH_WC)         AS DMD_PRV_MTH_WC,
      (COLN_ADJUSTED)          AS COLN_ADJUSTED ,
      (coln_for_mth_wc)        AS coln_for_mth_wc,
      (NET_DUE)                AS NET_DUE,
      BLOCK_NAME,
      BLOCK_SNO,
      SCH_TYPE_ID_SUB as SCH_TYPE_ID,
      SCH_TYPE_DESC_REPORT as SCH_TYPE_DESC ,
      BEN_TYPE_ID_SUB,
      BEN_TYPE_DESC_SUB,COLL_FOR_MTH_MAINT
    FROM PMS_DCB_LEDGER_ACTUAL_TEST
    WHERE office_id    =$P{office_id}
    AND MONTH          =$P{month}
    AND YEAR           =$P{year}
    AND BEN_TYPE_ID_SUB=6

    )
  GROUP BY SCH_TYPE_ID,
    SCH_TYPE_DESC,
    BEN_TYPE_ID_SUB,
    BEN_TYPE_DESC_SUB ,
    BLOCK_NAME,
    BLOCK_SNO ,
    BENEFICIARY_SNO,
    BENEFICIARY_NAME
  )
ORDER BY SCH_TYPE_ID,
  BEN_TYPE_ID_SUB,
  BLOCK_NAME,
  BENEFICIARY_NAME]]></queryString>

