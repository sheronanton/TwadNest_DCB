	<queryString><![CDATA[SELECT rec.*,
  ty.*
FROM
  (SELECT beneficiary_sno,
    BENEFICIARY_NAME,
    BENEFICIARY_TYPE_ID
  FROM PMS_DCB_MST_BENEFICIARY
  WHERE   status='L' and  office_id      =$P{office_id}
  AND beneficiary_sno IN
    (SELECT beneficiary_sno
    FROM PMS_DCB_MST_BENEFICIARY_METRE
    WHERE office_id =$P{office_id} and METER_STATUS='L'
    )
  AND beneficiary_sno NOT IN
    ( SELECT DISTINCT beneficiary_sno
    FROM PMS_DCB_OB_YEARLY
    WHERE office_id=$P{office_id}
    AND MONTH      =$P{month}
    AND fin_year   =$P{year}
    )
  )rec
JOIN
  ( SELECT BEN_TYPE_DESC,BEN_TYPE_ID FROM PMS_DCB_BEN_TYPE
  )ty
ON ty.BEN_TYPE_ID=rec.BENEFICIARY_TYPE_ID

ORDER BY BEN_TYPE_ID]]></queryString>

