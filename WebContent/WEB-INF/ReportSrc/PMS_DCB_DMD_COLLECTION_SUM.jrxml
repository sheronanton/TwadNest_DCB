	<parameter name="office_id" isForPrompting="true" class="java.lang.String"/>
	<parameter name="mvalue" isForPrompting="true" class="java.lang.String"/>
	<parameter name="office_name" isForPrompting="true" class="java.lang.String"/>
	<parameter name="year2" isForPrompting="true" class="java.lang.String"/>
	<parameter name="year1" isForPrompting="true" class="java.lang.String"/>
	<parameter name="month2" isForPrompting="true" class="java.lang.String"/>
	<parameter name="month1" isForPrompting="true" class="java.lang.String"/>
	<parameter name="days" isForPrompting="true" class="java.lang.Integer"/>
	<parameter name="count" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT bens.BILL_MONTH,
  bens.BILL_YEAR,
  coalesce ( bens.TOTAL_AMT,NULL,0,TOTAL_AMT) AS TOTAL_AMT ,
  bens.ADD_CHARGES_WC,
  bens.MINUS_CHARGES_WC ,
  dmd.*,
  (bens.TOTAL_AMT+bens.MINUS_CHARGES_WC-(bens.ADD_CHARGES_WC)) AS tot,
  CASE
    WHEN dmd.MONTH =1
    THEN 'January'
    WHEN dmd.MONTH =2
    THEN 'February'
    WHEN dmd.MONTH =3
    THEN 'March'
    WHEN dmd.MONTH =4
    THEN 'April'
    WHEN dmd.MONTH =5
    THEN 'May'
    WHEN dmd.MONTH =6
    THEN 'June'
    WHEN dmd.MONTH =7
    THEN 'July'
    WHEN dmd.MONTH =8
    THEN 'August'
    WHEN dmd.MONTH =9
    THEN 'September'
    WHEN dmd.MONTH =10
    THEN 'October'
    WHEN dmd.MONTH =11
    THEN 'November'
    WHEN dmd.MONTH =12
    THEN 'December'
  END AS mval
FROM
  (SELECT MONTH,
    YEAR,    
    SUM(TOTAL_AMT)    AS dd,
    SUM(QTY_CONSUMED) AS qty
  FROM PMS_DCB_WC_BILLING
  WHERE OFFICE_ID=$P{office_id}::numeric
   and (to_date((01||'-'||month||'-'||year),'dd-MM-yyyy') >= to_date('1-'||$P{month1}||'-'||$P{year1},'dd-mm-yyyy')) and
   (to_date((01||'-'||month||'-'||year),'dd-MM-yyyy') <= to_date($P{days}||'-'||$P{month2}||'-'||$P{year2},'dd-mm-yyyy'))   
  GROUP BY YEAR,
    MONTH
  ORDER BY MONTH
  )dmd
LEFT JOIN
  (SELECT 
    BILL_MONTH,
    BILL_YEAR,
    OFFICE_ID,
    SUM(COLN_CUR_YR_WC)   AS TOTAL_AMT,
    SUM(ADD_CHARGES_WC)   AS ADD_CHARGES_WC,
    SUM(MINUS_CHARGES_WC) AS MINUS_CHARGES_WC
  FROM PMS_DCB_TRN_BILL_DMD
  WHERE office_id=$P{office_id}::numeric
  and (to_date((01||'-'||BILL_MONTH||'-'||BILL_YEAR),'dd-MM-yyyy') >= to_date('1-'||$P{month1}||'-'||$P{year1},'dd-mm-yyyy')) and
   (to_date((01||'-'||BILL_MONTH||'-'||BILL_YEAR),'dd-MM-yyyy') <= to_date($P{days}||'-'||$P{month2}||'-'||$P{year2},'dd-mm-yyyy'))     GROUP BY OFFICE_ID,
    BILL_YEAR,
    BILL_MONTH
  )bens
on dmd.MONTH         =bens.BILL_MONTH
ORDER BY 
  MONTH,
  YEAR,
  BILL_MONTH,
  bens.BILL_YEAR]]></queryString>

