	<queryString><![CDATA[SELECT b.OFFICE_ID,
  b.PROJECT_ID,
  b.PROJECT_NAME ,
  b.status ,
  b.SCH_SNO AS view_sch_sno,
  c.MONTH,
  c.YEAR,
  d.office_name,
  a.meter_status ,
  sch.sch_sno AS sch_sch_sno,
  st.SCH_STATUS_DESC,
  sch.sch_name,
  sch.SCH_STATUS_ID
FROM
  ( SELECT DISTINCT scheme_sno,
    office_id,
    meter_status
  FROM PMS_DCB_MST_BENEFICIARY_METRE
  WHERE meter_status='L'
  )a
JOIN
  (SELECT *
  FROM PMS_MST_PROJECTS_VIEW
  WHERE sch_sno IS NOT NULL
  AND sch_sno   <> 0
  AND status     ='C'
  )b
ON a.office_id  =b.office_id
AND a.scheme_sno=b.sch_sno
JOIN
  (SELECT DISTINCT SCH_SNO,
    OFFICE_ID,
    MONTH,
    YEAR
  FROM PMS_DCB_TRN_MONTHLY_PR
  WHERE MONTH=11
  AND YEAR   =2012
  )c
ON a.office_id=c.OFFICE_ID
AND c.SCH_SNO =a.scheme_sno
AND c.SCH_SNO =b.sch_sno
JOIN
  (SELECT OFFICE_ID,
    office_name
  FROM com_mst_offices_view
  WHERE office_level_id='DN'
  )d
ON d.office_id=a.OFFICE_ID
JOIN
  ( SELECT SCH_SNO,SCH_NAME,SCH_STATUS_ID FROM PMS_SCH_MASTER
  )sch
ON sch.sch_sno =c.SCH_SNO
AND sch.sch_sno=a.scheme_sno
AND sch.sch_sno=b.sch_sno
JOIN
  ( SELECT SCH_STATUS_ID,SCH_STATUS_DESC FROM PMS_SCH_LKP_STATUS
  )st
ON st.SCH_STATUS_ID=sch.SCH_STATUS_ID
order by OFFICE_ID]]></queryString>

