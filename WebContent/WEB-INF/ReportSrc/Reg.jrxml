	<queryString><![CDATA[SELECT
     x.*,
     dia.office_name AS Region,
     j.office_name   AS circle,
     DECODE(M.amt,NULL,0,M.amt)AS AMT
 FROM
     (
         SELECT
             beneficiary_sno,
             ben_type_desc,
             office_id,
             sch_sno,
             beneficiary_name,
             sch_name,
             sch_type_desc,
             ( balance + opening_bal_wc ) / $P{divby} AS opening,
             ( dmd_upto_prv_mnth_wc + add_dmd_upto +dmd_for_mth_wc ) / $P{divby} AS demandpre,
             ( tot_coln_yes_yr_dmd + coln_upto_prv_mth_cr_yr_dmd + coln_include_charges ) / $P{divby} AS collection
           
         FROM
             pms_dcb_ledger_actual
         WHERE
             month = $P{month1}
             AND year = $P{year1}
     ) x
     LEFT OUTER JOIN (
         SELECT
             region_office_id,
             office_id,
             circle_office_id
         FROM
             com_mst_all_offices_view
         WHERE
             office_level_id = 'DN'
             AND office_id IN (
                 SELECT
                     office_id
                 FROM
                     pms_dcb_div_dist_map
             )
     ) dik ON dik.office_id = x.office_id
     LEFT OUTER JOIN (
         SELECT
             region_office_id,
             office_name
         FROM
             com_mst_all_offices_view
         WHERE
             office_level_id = 'RN'
     ) dia ON dia.region_office_id = dik.region_office_id
     LEFT OUTER JOIN (
         SELECT
             circle_office_id,
             office_name
         FROM
             com_mst_all_offices_view
         WHERE
             office_level_id = 'CL' and circle_office_id not in (7322,6486)
     ) j ON j.circle_office_id = dik.circle_office_id
      LEFT OUTER JOIN (
        SELECT SUM(AMOUNT) / $P{divby}  AS amt
        ,beneficiary_sno,SCH_SNO
          FROM PMS_DCB_FAS_REC_VIEW_ALL_MOD
          WHERE ACCOUNT_HEAD_CODE NOT IN (120601)
          AND CASHBOOK_MONTH           =$P{month}
          AND CASHBOOK_YEAR            =$P{year}
          group by beneficiary_sno,SCH_SNO
     ) M ON M.beneficiary_sno = x.beneficiary_sno and M.SCH_SNO=x.sch_sno
     order by Region,circle,sch_type_desc,beneficiary_name]]></queryString>

