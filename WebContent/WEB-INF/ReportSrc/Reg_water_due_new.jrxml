	<queryString><![CDATA[select k.*,(k.Dem+k.D1+k.D2+k.D3+k.D4+k.D5) as Demand,(k.PDem+k.PD1+k.PD2+k.PD3+k.PD4+k.PD5) as PDemand ,(k.Pcol+k.PC1+k.PC2+k.PC3+k.PC4+k.PC5) as Pcoll, (k.Pamt+k.Pa1+k.Pa2+k.Pa3+k.Pa4+k.Pa5) as Pamount 
from (select a1.OFFICE_NAME,a1.Region,(a1.Demand) as Dem,(a1.am+a1.cr-a1.dr) as Pamt ,(a1.dmd2) as PDem,(a1.Pcol) as Pcol
,((a1.ob+a1.DMD+a1.dmd2-(a1.COLLECTION))-(a1.am+a1.cr-a1.dr)) as VP,
DECODE(b1.TP_cb ,NULL,0,b1.TP_cb)as TP,
DECODE(b1.D1 ,NULL,0,b1.D1)as D1,
DECODE(b1.PD1 ,NULL,0,b1.PD1)as PD1,
DECODE(b1.PC1 ,NULL,0,b1.PC1)as PC1,
DECODE(b1.amt ,NULL,0,b1.amt)as Pa1,
DECODE(c1.mul_cb ,NULL,0,c1.mul_cb)as MUL,
DECODE(c1.D2 ,NULL,0,c1.D2)as D2,
DECODE(c1.PD2 ,NULL,0,c1.PD2)as PD2,
DECODE(c1.PC2 ,NULL,0,c1.PC2)as PC2,
DECODE(c1.amt ,NULL,0,c1.amt)as Pa2,
DECODE(d1.CP_cb ,NULL,0,d1.CP_cb)as COP,
DECODE(d1.D3 ,NULL,0,d1.D3)as D3,
DECODE(d1.PD3 ,NULL,0,d1.PD3)as PD3,
DECODE(d1.PC3 ,NULL,0,d1.PC3)as PC3,
DECODE(d1.amt ,NULL,0,d1.amt)as Pa3,
DECODE(e1.mul3 ,NULL,0,e1.mul3)as MUL3,
DECODE(e1.D4 ,NULL,0,e1.D4)as D4,
DECODE(e1.PD4 ,NULL,0,e1.PD4)as PD4,
DECODE(e1.PC4 ,NULL,0,e1.PC4)as PC4,
DECODE(e1.amt ,NULL,0,e1.amt)as Pa4,
DECODE(f1.Priv ,NULL,0,f1.Priv)as Privat,
DECODE(f1.D5 ,NULL,0,f1.D5)as D5,
DECODE(f1.PD5 ,NULL,0,f1.PD5)as PD5,
DECODE(f1.amt ,NULL,0,f1.amt)as Pa5,
DECODE(f1.PC5 ,NULL,0,f1.PC5)as PC5

from (select y.* ,decode(x.am ,null,0,x.am)as am ,decode(g.dr,null,0,g.dr)as dr,
DECODE(z.cr,NULL,0,z.cr)AS CR

from
(select 
 OFFICE_NAME,Region,OFFICE_ID,
 (SUM(dmd)-sum(dmd2))  AS dmd,  SUM(ob) AS ob,sum(dmd2) as dmd2,sum(col) as Pcol,((sum(dmd)))/$P{count} as Demand , SUM(collection) AS collection,
SUM(arrear_collection) AS arrear_collection,SUM(bal)  AS bal,SUM(cb)   AS cb,SUM(OB_MAINT_CHARGES)  AS OB_MAINT_CHARGES,SUM(ob_int) AS ob_int,
SUM(COLN_INT_WC)     AS COLN_INT_WC, SUM(ob)+(SUM(dmd)-SUM(dmd2))-( SUM(collection)  ) AS net_balance
from 

(

SELECT 
y.amt,renam.OFFICE_NAME as Region,reg.OFFICE_NAME,
  ben.BENEFICIARY_TYPE_ID,
  btype.BEN_TYPE_DESC,
 district_name,DISTRICT_CODE,
  ben.BENEFICIARY_NAME,
  coll.OFFICE_ID ,
  decode(dmd,null,0,dmd) as dmd,
  decode(dmd2,null,0,dmd2) as dmd2,
    decode(col,null,0,col) as col,
  ob,  
    collection,
   case when  collection > dmd   then
    (collection-dmd) 
    else
    0
    end as arrear_collection,
     case when collection > dmd then
     0 
     else 
        (dmd-collection) end  AS bal,
  (ob +(dmd-collection)) AS cb,OB_MAINT_CHARGES,ob_int,COLN_INT_WC

FROM (
  (SELECT OFFICE_ID,
    BENEFICIARY_SNO,
    (SUM(OB_CUR_YR_WC)+sum(OB_MAINT_CHARGES)+SUM(OB_YESTER_YR_WC))/ $P{divby} AS ob,sum(OB_MAINT_CHARGES)/ $P{divby} as OB_MAINT_CHARGES 
  FROM PMS_DCB_OB_YEARLY
  WHERE  MONTH=4   AND FIn_YEAR   = $P{app_year}  GROUP BY OFFICE_ID,
    BENEFICIARY_SNO
  )ob
LEFT OUTER JOIN
  (SELECT BENEFICIARY_SNO ,
    OFFICE_ID,
    ((SUM(COLN_CUR_YR_WC) +sum(COLN_MAINT) + SUM(COLN_YESTER_YR_WC) +SUM(MINUS_CHARGES_WC)) -SUM(ADD_CHARGES_WC) )/ $P{divby} AS collection,
    sum(CB_INT_AMT_WC) / $P{divby} as ob_int,sum(COLN_INT_WC) / $P{divby} as COLN_INT_WC
  FROM PMS_DCB_TRN_BILL_DMD  
  WHERE   (to_date((01||'-'||BILL_MONTH||'-'||BILL_YEAR),'dd-MM-yyyy') >=to_date('1-4-'||$P{app_year},'dd-mm-yyyy'))
  AND (to_date((01||'-'||BILL_MONTH||'-'||BILL_YEAR),'dd-MM-yyyy') <=to_date($P{endofday1}||'-'||$P{month1}||'-'||$P{year1},'dd-mm-yyyy')) 
  and BENEFICIARY_SNO in (SELECT     BENEFICIARY_SNO FROM PMS_DCB_MST_BENEFICIARY  )
  GROUP BY OFFICE_ID,
    BENEFICIARY_SNO
  )coll
ON coll.OFFICE_ID       =ob.OFFICE_ID
AND coll.BENEFICIARY_SNO=ob.BENEFICIARY_SNO
LEFT OUTER JOIN
  (SELECT OFFICE_ID,
    BENEFICIARY_SNO,
    SUM(MONTH_BILL_AMT) / $P{divby} AS dmd
  FROM PMS_DCB_TRN_BILL_DMD
  WHERE   (to_date((01||'-'||BILL_MONTH||'-'||BILL_YEAR),'dd-MM-yyyy') >=to_date('1-4-'||$P{app_year},'dd-mm-yyyy'))
  AND (to_date((01||'-'||BILL_MONTH||'-'||BILL_YEAR),'dd-MM-yyyy') <=to_date($P{endofday1}||'-'||$P{month1}||'-'||$P{year1},'dd-mm-yyyy')) 
  GROUP BY OFFICE_ID,
    BENEFICIARY_SNO
  )dmd
ON coll.OFFICE_ID       =dmd.OFFICE_ID
AND coll.BENEFICIARY_SNO=dmd.BENEFICIARY_SNO
AND dmd.OFFICE_ID       =ob.OFFICE_ID
AND dmd.BENEFICIARY_SNO =ob.BENEFICIARY_SNO
LEFT OUTER JOIN  (SELECT OFFICE_ID, BENEFICIARY_SNO, SUM(MONTH_BILL_AMT) / $P{divby} AS dmd2 ,((SUM(COLN_CUR_YR_WC) +sum(COLN_MAINT) + SUM(COLN_YESTER_YR_WC) +SUM(MINUS_CHARGES_WC)) -SUM(ADD_CHARGES_WC) )/ $P{divby} AS col FROM PMS_DCB_TRN_BILL_DMD
  WHERE BILL_MONTH=$P{month1} and BILL_YEAR=$P{year1}
  GROUP BY OFFICE_ID, BENEFICIARY_SNO  )dmd2
ON coll.OFFICE_ID =dmd2.OFFICE_ID
AND coll.BENEFICIARY_SNO=dmd2.BENEFICIARY_SNO
AND dmd2.OFFICE_ID =ob.OFFICE_ID
AND dmd2.BENEFICIARY_SNO =ob.BENEFICIARY_SNO
LEFT OUTER JOIN
  (SELECT DISTRICT_CODE ,office_id,
    BENEFICIARY_NAME,
    beneficiary_sno,
    BENEFICIARY_TYPE_ID,BENEFICIARY_TYPE_ID_SUB
  FROM PMS_DCB_MST_BENEFICIARY 
  WHERE  BENEFICIARY_TYPE_ID_SUB=6
  )ben
ON ben.BENEFICIARY_SNO =dmd.BENEFICIARY_SNO
AND ben.BENEFICIARY_SNO=coll.BENEFICIARY_SNO
AND ben.BENEFICIARY_SNO=ob.BENEFICIARY_SNO
LEFT OUTER JOIN
  ( SELECT district_code,district_name FROM com_mst_districts
  )dis
ON dis.district_code=ben.district_code
LEFT OUTER JOIN
  ( SELECT OFFICE_ID,OFFICE_NAME,REGION_OFFICE_ID FROM COM_MST_ALL_OFFICES_VIEW where OFFICE_LEVEL_ID='DN' and OFFICE_ID in (select OFFICE_ID from PMS_DCB_DIV_DIST_MAP)
  )reg
ON reg.OFFICE_ID=ben.OFFICE_ID
LEFT OUTER JOIN
  ( SELECT OFFICE_ID,OFFICE_NAME FROM COM_MST_ALL_OFFICES_VIEW
  )renam
ON reg.REGION_OFFICE_ID=renam.OFFICE_ID

LEFT OUTER JOIN
  ( SELECT BEN_TYPE_ID,BEN_TYPE_DESC FROM PMS_DCB_BEN_TYPE
  )btype
ON btype.BEN_TYPE_ID=ben.BENEFICIARY_TYPE_ID_SUB )
where BENEFICIARY_SNO is not null
ORDER BY BENEFICIARY_TYPE_ID,
  district_name,
  BENEFICIARY_SNO
  )
  GROUP BY OFFICE_NAME,Region,OFFICE_ID
ORDER BY Region,OFFICE_NAME
)Y
left OUTER JOIN
  (SELECT  ACCOUNTING_FOR_OFFICE_ID,Sum(Amount) / $P{divby} AS am

    FROM PMS_DCB_FAS_REC_VIEW_ALL_MOD
    WHERE  CASHBOOK_MONTH        = $P{month}
    AND CASHBOOK_YEAR            = $P{year}
    AND BENEFICIARY_TYPE_ID_SUB     IN (6)
    GROUP BY ACCOUNTING_FOR_OFFICE_ID
  )x
ON  y.office_id=x.ACCOUNTING_FOR_OFFICE_ID

LEFT OUTER JOIN
  (SELECT  ACCOUNTING_FOR_OFFICE_ID ,SUM(AMOUNT)/ $P{divby} AS cr
     FROM PMS_DCB_FAS_OTHER_CHARGES_USE
    WHERE  CASHBOOK_MONTH           =$P{month}
    AND CASHBOOK_YEAR            =$P{year}
    and CR_DR_INDICATOR='CR'
    AND BENEFICIARY_TYPE_ID_SUB    IN (6)
     group by  ACCOUNTING_FOR_OFFICE_ID 
)z
ON y.OFFICE_ID=z.ACCOUNTING_FOR_OFFICE_ID
LEFT OUTER JOIN
  (SELECT  ACCOUNTING_FOR_OFFICE_ID ,SUM(AMOUNT)/ $P{divby} AS dr
     FROM PMS_DCB_FAS_OTHER_CHARGES_USE
    WHERE  CASHBOOK_MONTH           =$P{month}
    AND CASHBOOK_YEAR            =$P{year}
    and CR_DR_INDICATOR='DR'
    AND BENEFICIARY_TYPE_ID_SUB    IN (6)
     group by  ACCOUNTING_FOR_OFFICE_ID 
)g
ON y.OFFICE_ID=g.ACCOUNTING_FOR_OFFICE_ID




)a1
-------------------------------------------------------------------TP------------------------
left outer join
(
select a1.office_name,a1.region,(a1.demand) as d1,(a1.dmd2) as pd1,(a1.col) as pc1,((a1.ob+a1.dmd+a1.dmd2-(a1.collection))-(a1.am+a1.cr-a1.dr)) as tp_cb,(a1.am+a1.cr-a1.dr) as amt
from (select y.* ,decode(x.am ,null,0,x.am)as am,decode(g.dr,null,0,g.dr)as dr,
DECODE(z.cr,NULL,0,z.cr)AS CR  from
(select 
 OFFICE_NAME,Region,OFFICE_ID,
 (SUM(dmd)-sum(dmd2))  AS dmd,  SUM(ob) AS ob,sum(dmd2) as dmd2,sum(col) as col,((sum(dmd)))/$P{count} as Demand , SUM(collection) AS collection,
SUM(arrear_collection) AS arrear_collection,SUM(bal)  AS bal,SUM(cb)   AS cb,SUM(OB_MAINT_CHARGES)  AS OB_MAINT_CHARGES,SUM(ob_int) AS ob_int,
SUM(COLN_INT_WC)     AS COLN_INT_WC, SUM(ob)+(SUM(dmd)-SUM(dmd2))-( SUM(collection)  ) AS net_balance
from 

(

SELECT 
y.amt,renam.OFFICE_NAME as Region,reg.OFFICE_NAME,
