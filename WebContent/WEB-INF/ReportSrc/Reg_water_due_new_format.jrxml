	<queryString><![CDATA[select OFFICE_NAME,Region,Demand,PDEMAND,COLL,COP,MUL,TP,VP,Privat,
(COP+MUL+TP+VP+Privat-PDEMAND+COLL) as arrdmd ,
case when (COP+MUL+TP+VP+Privat-PDEMAND+COLL) >= COLL 
    then
        COLL
      when   (COP+MUL+TP+VP+Privat-PDEMAND+COLL) < Coll and (COP+MUL+TP+VP+Privat-PDEMAND+COLL ) > 0
      then (COP+MUL+TP+VP+Privat-PDEMAND+COLL)
    else
     0
    end as arrear,
    
    
     case when (COP+MUL+TP+VP+Privat-PDEMAND+COLL ) >= COLL 
     then
     0 
     when   (COP+MUL+TP+VP+Privat-PDEMAND+COLL ) < Coll and (COP+MUL+TP+VP+Privat-PDEMAND+COLL ) > 0
      then (COLL-(COP+MUL+TP+VP+Privat-PDEMAND+COLL ))
     else 
       Coll
       end  AS bal


from
(
select k.*,(k.Dem+k.D1+k.D2+k.D3+k.D5) as Demand ,(k.PDem+k.PD1+k.PD2+k.PD3+k.PD5) as PDemand ,
(k.ccoll+k.c1+k.c2+k.c3+k.c5) as Coll 
from (select a1.OFFICE_NAME,a1.Region,(a1.Demand) as Dem ,(a1.dmd2) as PDem,(a1.ob+a1.DMD-(a1.COLLECTION)) as VP,
(a1.coll2) as ccoll,(a1.arrear_collection) as aarr,(a1.bal) as bbal,
coalesce(b1.TP_cb ,NULL,0,b1.TP_cb)as TP,
coalesce(b1.D1 ,NULL,0,b1.D1)as D1,
coalesce(b1.PD1 ,NULL,0,b1.PD1)as PD1,
coalesce(b1.c1 ,NULL,0,b1.c1)as c1,
coalesce(b1.a1 ,NULL,0,b1.a1)as a1,
coalesce(b1.b1 ,NULL,0,b1.b1)as b1,
coalesce(c1.mul_cb ,NULL,0,c1.mul_cb)as MUL,
coalesce(c1.D2 ,NULL,0,c1.D2)as D2,
coalesce(c1.PD2 ,NULL,0,c1.PD2)as PD2,
coalesce(c1.c2 ,NULL,0,c1.c2)as c2,
coalesce(c1.a2 ,NULL,0,c1.a2)as a2,
coalesce(c1.b2 ,NULL,0,c1.b2)as b2,
coalesce(d1.CP_cb ,NULL,0,d1.CP_cb)as COP,
coalesce(d1.D3 ,NULL,0,d1.D3)as D3,
coalesce(d1.PD3 ,NULL,0,d1.PD3)as PD3,
coalesce(d1.a3 ,NULL,0,d1.a3)as a3,
coalesce(d1.b3 ,NULL,0,d1.b3)as b3,
coalesce(d1.c3 ,NULL,0,d1.c3)as c3,
coalesce(f1.Priv ,NULL,0,f1.Priv)as Privat,
coalesce(f1.D5 ,NULL,0,f1.D5)as D5,
coalesce(f1.PD5 ,NULL,0,f1.PD5)as PD5,
coalesce(f1.a5 ,NULL,0,f1.a5)as a5,
coalesce(f1.b5 ,NULL,0,f1.b5)as b5,
coalesce(f1.c5 ,NULL,0,f1.c5)as c5

from (select y.* ,coalesce(x.amt ,NULL,0,x.amt)as amt from
(select 
 OFFICE_NAME,Region,OFFICE_ID,
 (SUM(dmd))  AS dmd,  SUM(ob) AS ob,sum(dmd2) as dmd2,(sum(dmd))/$P{count}::int as Demand , SUM(collection) AS collection,sum(coll2) as coll2,
SUM(arrear_collection) AS arrear_collection,SUM(bal)  AS bal,SUM(cb)   AS cb,SUM(OB_MAINT_CHARGES)  AS OB_MAINT_CHARGES,SUM(ob_int) AS ob_int,
SUM(COLN_INT_WC)     AS COLN_INT_WC, SUM(ob)+(SUM(dmd)-SUM(dmd2))-( SUM(collection)  ) AS net_balance
from 

(

SELECT 
renam.OFFICE_NAME as Region,reg.OFFICE_NAME,
 ben.BENEFICIARY_TYPE_ID,
  btype.BEN_TYPE_DESC,
 district_name,ben.DISTRICT_CODE,
  ben.BENEFICIARY_NAME,
  coll.OFFICE_ID ,
  coalesce(dmd,null,0,dmd) as dmd,
  coalesce(dmd2,null,0,dmd2) as dmd2,
  coalesce(coll2,null,0,coll2) as coll2,
  ob,  
    collection,
    case when (ob+dmd-dmd2-collection+coll2) >= coll2 
    then
        coll2
    else
     (ob+dmd-dmd2-collection+coll2)
    end as arrear_collection,
     case when (ob+dmd-dmd2-collection+coll2) >= coll2 
     then
     0 
     else 
        (coll2-(ob+dmd-dmd2-collection+coll2)) end  AS bal,
  (ob +(dmd-collection)) AS cb,OB_MAINT_CHARGES,ob_int,COLN_INT_WC

FROM (
  (SELECT OFFICE_ID,
    BENEFICIARY_SNO,
    (SUM(OB_CUR_YR_WC)+sum(OB_MAINT_CHARGES)+SUM(OB_YESTER_YR_WC))/ $P{divby}::int AS ob,sum(OB_MAINT_CHARGES)/ $P{divby}::int as OB_MAINT_CHARGES 
  FROM PMS_DCB_OB_YEARLY
  WHERE  MONTH=4   AND FIn_YEAR   = $P{app_year}::int 
  and BENEFICIARY_SNO in (SELECT     BENEFICIARY_SNO FROM PMS_DCB_MST_BENEFICIARY where BENEFICIARY_TYPE_ID_SUB=6 )
  
  GROUP BY OFFICE_ID,
    BENEFICIARY_SNO
  )ob
LEFT OUTER JOIN
  (SELECT BENEFICIARY_SNO ,
    OFFICE_ID,
    ((SUM(COLN_CUR_YR_WC) +sum(COLN_MAINT) + SUM(COLN_YESTER_YR_WC) +SUM(MINUS_CHARGES_WC)) -SUM(ADD_CHARGES_WC) )/ $P{divby}::int AS collection,
    sum(CB_INT_AMT_WC) / $P{divby}::int as ob_int,sum(COLN_INT_WC) / $P{divby}::int as COLN_INT_WC
  FROM PMS_DCB_TRN_BILL_DMD  
  WHERE   (to_date((01||'-'||BILL_MONTH||'-'||BILL_YEAR),'dd-MM-yyyy') >=to_date('1-4-'||$P{app_year}::int,'dd-mm-yyyy'))
  AND (to_date((01||'-'||BILL_MONTH||'-'||BILL_YEAR),'dd-MM-yyyy') <=to_date($P{endofday}::int||'-'||$P{month}::int||'-'||$P{year}::int,'dd-mm-yyyy')) 
    GROUP BY OFFICE_ID,
    BENEFICIARY_SNO
  )coll
ON coll.OFFICE_ID       =ob.OFFICE_ID
AND coll.BENEFICIARY_SNO=ob.BENEFICIARY_SNO
LEFT OUTER JOIN
  (SELECT OFFICE_ID,
    BENEFICIARY_SNO,
    SUM(MONTH_BILL_AMT) / $P{divby}::int AS dmd
  FROM PMS_DCB_TRN_BILL_DMD
  WHERE   (to_date((01||'-'||BILL_MONTH||'-'||BILL_YEAR),'dd-MM-yyyy') >=to_date('1-4-'||$P{app_year}::int,'dd-mm-yyyy'))
  AND (to_date((01||'-'||BILL_MONTH||'-'||BILL_YEAR),'dd-MM-yyyy') <=to_date($P{endofday1}::int||'-'||$P{month1}::int||'-'||$P{year1}::int,'dd-mm-yyyy')) 
  GROUP BY OFFICE_ID,
    BENEFICIARY_SNO
  )dmd
ON coll.OFFICE_ID       =dmd.OFFICE_ID
AND coll.BENEFICIARY_SNO=dmd.BENEFICIARY_SNO
AND dmd.OFFICE_ID       =ob.OFFICE_ID
AND dmd.BENEFICIARY_SNO =ob.BENEFICIARY_SNO
LEFT OUTER JOIN  (SELECT OFFICE_ID, BENEFICIARY_SNO, SUM(MONTH_BILL_AMT) / $P{divby}::int AS dmd2  FROM PMS_DCB_TRN_BILL_DMD
  WHERE BILL_MONTH=$P{month1}::int and BILL_YEAR=$P{app_year}::int
  GROUP BY OFFICE_ID, BENEFICIARY_SNO  )dmd2
ON coll.OFFICE_ID =dmd2.OFFICE_ID
AND coll.BENEFICIARY_SNO=dmd2.BENEFICIARY_SNO
AND dmd2.OFFICE_ID =ob.OFFICE_ID
AND dmd2.BENEFICIARY_SNO =ob.BENEFICIARY_SNO

LEFT OUTER JOIN  (SELECT OFFICE_ID, BENEFICIARY_SNO,
((SUM(COLN_CUR_YR_WC) +sum(COLN_MAINT) + SUM(COLN_YESTER_YR_WC) +SUM(MINUS_CHARGES_WC)) -SUM(ADD_CHARGES_WC) )/ $P{divby}::int AS coll2
FROM PMS_DCB_TRN_BILL_DMD
  WHERE BILL_MONTH=$P{month}::int and BILL_YEAR=$P{year}::int
  GROUP BY OFFICE_ID, BENEFICIARY_SNO  )coll2
ON coll.OFFICE_ID =coll2.OFFICE_ID
AND coll.BENEFICIARY_SNO=coll2.BENEFICIARY_SNO
AND coll2.OFFICE_ID =ob.OFFICE_ID
AND coll2.BENEFICIARY_SNO =ob.BENEFICIARY_SNO


LEFT OUTER JOIN
  (SELECT DISTRICT_CODE ,office_id,
    BENEFICIARY_NAME,
    BENEFICIARY_SNO,
    BENEFICIARY_TYPE_ID
  FROM PMS_DCB_MST_BENEFICIARY 
  WHERE  BENEFICIARY_TYPE_ID_SUB=6
  )ben
ON ben.BENEFICIARY_SNO =dmd.BENEFICIARY_SNO
AND ben.BENEFICIARY_SNO=coll.BENEFICIARY_SNO
AND ben.BENEFICIARY_SNO=ob.BENEFICIARY_SNO
LEFT OUTER JOIN
  ( SELECT district_code,district_name FROM com_mst_districts
  )dis
ON dis.district_code=ben.district_code
LEFT OUTER JOIN
  ( SELECT OFFICE_ID,OFFICE_NAME,REGION_OFFICE_ID FROM COM_MST_ALL_OFFICES_VIEW where OFFICE_LEVEL_ID='DN' and OFFICE_ID in (select OFFICE_ID from PMS_DCB_DIV_DIST_MAP)
  )reg
ON reg.OFFICE_ID=ben.OFFICE_ID
LEFT OUTER JOIN
  ( SELECT OFFICE_ID,OFFICE_NAME FROM COM_MST_ALL_OFFICES_VIEW
  )renam
ON reg.REGION_OFFICE_ID=renam.OFFICE_ID

LEFT OUTER JOIN
  ( SELECT BEN_TYPE_ID,BEN_TYPE_DESC FROM PMS_DCB_BEN_TYPE
  )btype
ON btype.BEN_TYPE_ID=ben.BENEFICIARY_TYPE_ID )
where ben.BENEFICIARY_SNO is not null
ORDER BY BENEFICIARY_TYPE_ID,
  district_name,
  ben.BENEFICIARY_SNO
  )ab
  GROUP BY OFFICE_NAME,Region,OFFICE_ID
ORDER BY Region,OFFICE_NAME
)Y
left OUTER JOIN
  (SELECT  ACCOUNTING_FOR_OFFICE_ID,Sum(Amount) / $P{divby}::int AS amt

    FROM PMS_DCB_FAS_REC_VIEW_ALL_MOD
    WHERE  CASHBOOK_MONTH        = $P{month}::int
    AND CASHBOOK_YEAR            = $P{year}::int
    AND BENEFICIARY_TYPE_ID_sub     IN (6)
    GROUP BY ACCOUNTING_FOR_OFFICE_ID
  )x
ON  y.office_id=x.ACCOUNTING_FOR_OFFICE_ID
)a1
-------------------------------------------------------------------TP------------------------
left outer join
(
select a1.OFFICE_NAME,a1.Region,(a1.Demand) as D1,(a1.dmd2) as PD1,(a1.ob+a1.DMD-(a1.COLLECTION)) as TP_cb,
(a1.coll2) as c1,(a1.arrear_collection) as a1,(a1.bal) as b1
from (select y.* ,coalesce(x.amt ,NULL,0,x.amt)as amt from
(select 
 OFFICE_NAME,Region,OFFICE_ID,
 (SUM(dmd))  AS dmd,  SUM(ob) AS ob,sum(dmd2) as dmd2,(sum(dmd))/$P{count}::int as Demand , SUM(collection) AS collection,
SUM(arrear_collection) AS arrear_collection,SUM(bal)  AS bal,SUM(cb)   AS cb,SUM(OB_MAINT_CHARGES)  AS OB_MAINT_CHARGES,SUM(ob_int) AS ob_int,
SUM(COLN_INT_WC)     AS COLN_INT_WC, SUM(ob)+(SUM(dmd)-SUM(dmd2))-( SUM(collection)  ) AS net_balance,sum(coll2) as coll2
from 

(

SELECT 
renam.OFFICE_NAME as Region,reg.OFFICE_NAME,
  ben.BENEFICIARY_TYPE_ID,
  btype.BEN_TYPE_DESC,
 district_name,ben.DISTRICT_CODE,
  ben.BENEFICIARY_NAME,
  coll.OFFICE_ID ,
  coalesce(dmd,null,0,dmd) as dmd,
  coalesce(dmd2,null,0,dmd2) as dmd2,
 coalesce(coll2,null,0,coll2) as coll2,
  ob,  
    collection,
    case when (ob+dmd-dmd2-collection+coll2) >= coll2 
    then
        coll2
    else
     (ob+dmd-dmd2-collection+coll2)
    end as arrear_collection,
     case when (ob+dmd-dmd2-collection+coll2) >= coll2 
     then
     0 
     else 
        (coll2-(ob+dmd-dmd2-collection+coll2)) end  AS bal,
  (ob +(dmd-collection)) AS cb,OB_MAINT_CHARGES,ob_int,COLN_INT_WC

FROM (
  (SELECT OFFICE_ID,
    BENEFICIARY_SNO,
    (SUM(OB_CUR_YR_WC)+sum(OB_MAINT_CHARGES)+SUM(OB_YESTER_YR_WC))/ $P{divby}::int AS ob,sum(OB_MAINT_CHARGES)/ $P{divby}::int as OB_MAINT_CHARGES 
  FROM PMS_DCB_OB_YEARLY
  WHERE  MONTH=4   AND FIn_YEAR   = $P{app_year}::int 
   and BENEFICIARY_SNO in (SELECT     BENEFICIARY_SNO FROM PMS_DCB_MST_BENEFICIARY where BENEFICIARY_TYPE_ID_SUB in (4,5) )
  
  GROUP BY OFFICE_ID,
    BENEFICIARY_SNO
  )ob
LEFT OUTER JOIN
  (SELECT BENEFICIARY_SNO ,
    OFFICE_ID,
    ((SUM(COLN_CUR_YR_WC) +sum(COLN_MAINT) + SUM(COLN_YESTER_YR_WC) +SUM(MINUS_CHARGES_WC)) -SUM(ADD_CHARGES_WC) )/ $P{divby}::int AS collection,
    sum(CB_INT_AMT_WC) / $P{divby}::int as ob_int,sum(COLN_INT_WC) / $P{divby}::int as COLN_INT_WC
  FROM PMS_DCB_TRN_BILL_DMD  
  WHERE   (to_date((01||'-'||BILL_MONTH||'-'||BILL_YEAR),'dd-MM-yyyy') >=to_date('1-4-'||$P{app_year}::int,'dd-mm-yyyy'))
  AND (to_date((01||'-'||BILL_MONTH||'-'||BILL_YEAR),'dd-MM-yyyy') <=to_date($P{endofday}::int||'-'||$P{month}::int||'-'||$P{year}::int,'dd-mm-yyyy')) 
 
  GROUP BY OFFICE_ID,
    BENEFICIARY_SNO
  )coll
ON coll.OFFICE_ID       =ob.OFFICE_ID
AND coll.BENEFICIARY_SNO=ob.BENEFICIARY_SNO
LEFT OUTER JOIN
  (SELECT OFFICE_ID,
    BENEFICIARY_SNO,
    SUM(MONTH_BILL_AMT) / $P{divby}::int AS dmd
  FROM PMS_DCB_TRN_BILL_DMD
  WHERE   (to_date((01||'-'||BILL_MONTH||'-'||BILL_YEAR),'dd-MM-yyyy') >=to_date('1-4-'||$P{app_year}::int,'dd-mm-yyyy'))
  AND (to_date((01||'-'||BILL_MONTH||'-'||BILL_YEAR),'dd-MM-yyyy') <=to_date($P{endofday1}::int||'-'||$P{month1}::int||'-'||$P{year1}::int,'dd-mm-yyyy')) 
  GROUP BY OFFICE_ID,
    BENEFICIARY_SNO
  )dmd
ON coll.OFFICE_ID       =dmd.OFFICE_ID
AND coll.BENEFICIARY_SNO=dmd.BENEFICIARY_SNO
AND dmd.OFFICE_ID       =ob.OFFICE_ID
AND dmd.BENEFICIARY_SNO =ob.BENEFICIARY_SNO
LEFT OUTER JOIN  (SELECT OFFICE_ID, BENEFICIARY_SNO, SUM(MONTH_BILL_AMT) / $P{divby}::int AS dmd2  FROM PMS_DCB_TRN_BILL_DMD
  WHERE BILL_MONTH=$P{month1}::int and BILL_YEAR=$P{year1}::int
  GROUP BY OFFICE_ID, BENEFICIARY_SNO  )dmd2
ON coll.OFFICE_ID =dmd2.OFFICE_ID
AND coll.BENEFICIARY_SNO=dmd2.BENEFICIARY_SNO
AND dmd2.OFFICE_ID =ob.OFFICE_ID
AND dmd2.BENEFICIARY_SNO =ob.BENEFICIARY_SNO

LEFT OUTER JOIN  (SELECT OFFICE_ID, BENEFICIARY_SNO,
((SUM(COLN_CUR_YR_WC) +sum(COLN_MAINT) + SUM(COLN_YESTER_YR_WC) +SUM(MINUS_CHARGES_WC)) -SUM(ADD_CHARGES_WC) )/ $P{divby}::int AS coll2
FROM PMS_DCB_TRN_BILL_DMD
  WHERE BILL_MONTH=$P{month}::int and BILL_YEAR=$P{year}::int
  GROUP BY OFFICE_ID, BENEFICIARY_SNO  )coll2
ON coll.OFFICE_ID =coll2.OFFICE_ID
AND coll.BENEFICIARY_SNO=coll2.BENEFICIARY_SNO
