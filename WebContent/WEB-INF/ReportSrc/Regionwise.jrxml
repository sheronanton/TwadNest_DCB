	<queryString><![CDATA[SELECT
     x.*,
     dia.office_name AS Region,
     j.office_name   AS circle
 FROM
     (
         SELECT
             beneficiary_sno,
             ben_type_desc,
             BEN_TYPE_ID,
             office_id,
             sch_sno,
             beneficiary_name,
             sch_name,
             sch_type_desc,
             ( balance + opening_bal_wc ) / $P{divby}::int AS opening,
             ( dmd_upto_prv_mnth_wc + add_dmd_upto ) / $P{divby}::int AS demandpre,
             ( tot_coln_yes_yr_dmd + coln_upto_prv_mth_cr_yr_dmd + coln_include_charges ) / $P{divby}::int AS collection,
             dmd_for_mth_wc / $P{divby}::int AS demand
         FROM
             pms_dcb_ledger_actual
         WHERE
             month = $P{month}::int
             AND year = $P{year}::int
     ) x
     LEFT OUTER JOIN (
         SELECT
             region_office_id,
             office_id,
             circle_office_id
         FROM
             com_mst_all_offices_view
         WHERE
             office_level_id = 'DN'
             AND office_id IN (
                 SELECT
                     office_id
                 FROM
                     pms_dcb_div_dist_map
             )
     ) dik ON dik.office_id = x.office_id
     LEFT OUTER JOIN (
         SELECT
             region_office_id,
             office_name
         FROM
             com_mst_all_offices_view
         WHERE
             office_level_id = 'RN'
     ) dia ON dia.region_office_id = dik.region_office_id
     LEFT OUTER JOIN (
         SELECT
             circle_office_id,
             office_name
         FROM
             com_mst_all_offices_view
         WHERE
             office_level_id = 'CL' and circle_office_id not in (7322,6486)
     ) j ON j.circle_office_id = dik.circle_office_id
     order by Region,circle,sch_name,BEN_TYPE_ID,beneficiary_name]]></queryString>

