	<queryString><![CDATA[select renam.OFFICE_NAME,a.SCH_NAME,sum(a.Demand) / $P{divby} as dmd,(sum(a.collection)+sum(a.CR_MINUS)-sum(a.DR_PLUS))/$P{divby} as coll
from
(
select DEMAND,COLLECTION,
NVL(DR_PLUS,0) as DR_PLUS,NVL(CR_MINUS,0) as CR_MINUS
,OFFICE_ID,SCH_NO,SCH_NAME  from PMS_DCB_REPORT_BILL_SCH_FINAL
where bill_month=$P{month} and bill_year=$P{year}
)a
LEFT OUTER JOIN
  ( SELECT OFFICE_ID,OFFICE_NAME,REGION_OFFICE_ID FROM COM_MST_ALL_OFFICES_VIEW where OFFICE_LEVEL_ID='DN' and OFFICE_ID in (select OFFICE_ID from PMS_DCB_DIV_DIST_MAP)
  )reg
ON reg.OFFICE_ID=a.OFFICE_ID
LEFT OUTER JOIN
  ( SELECT OFFICE_ID,OFFICE_NAME FROM COM_MST_ALL_OFFICES_VIEW
  )renam
ON reg.REGION_OFFICE_ID=renam.OFFICE_ID 
group by renam.OFFICE_NAME, a.SCH_NAME
order by renam.OFFICE_NAME, a.SCH_NAME]]></queryString>

