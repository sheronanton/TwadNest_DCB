	<queryString><![CDATA[select 
* from 
(
SELECT beneficiary_type_id_sub ,
    ben_type_desc ,
    SUM(qty) AS qty,
    SUM(amt) AS amt
  FROM PMS_DCB_SUPPLIED_QTY where month=$P{month} and year=$P{year} and office_id=$P{office_id}
  GROUP BY beneficiary_type_id_sub,ben_type_desc 
)
join
(
  select  OFFICE_NAME from COM_MST_ALL_OFFICES_VIEW where office_id=$P{office_id}
)off
on (1=1)
  order by beneficiary_type_id_sub]]></queryString>

