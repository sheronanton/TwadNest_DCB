	<parameter name="ben" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT 
  DECODE(SCH_NAME,NULL,' ',SCH_NAME) AS SCH_NAME,
  DECODE(METRE_LOCATION,NULL,' ',CONCAT('Location: ',METRE_LOCATION)) AS METRE_LOCATION,
  DECODE(METRE_LOCATION,NULL,'Scheme-wise','Location-wise') AS TARIFF_SETTING,
  QTY_FROM,
  QTY_TO,
  TARIFF_RATE
FROM
  (
    SELECT 
      SCH_SNO,
      METRE_SNO,
      TARIFF_FLAG,
      QTY_FROM,
      QTY_TO,
      TARIFF_RATE,
      TARIFF_W_E_F,
      MAX_FLAG
    FROM PMS_DCB_TARIFF_SLAB
    WHERE BENEFICIARY_SNO=$P{ben}
      AND ACTIVE_STATUS='A'
  )a
  
  LEFT OUTER JOIN
  
  (
    SELECT 
      SCH_SNO,
      SCH_NAME
    FROM PMS_SCH_MASTER
  )b
  ON a.SCH_SNO=b.SCH_SNO
    
  LEFT OUTER JOIN
  
  (
    SELECT 
      METRE_SNO,
      METRE_LOCATION
    FROM PMS_DCB_MST_BENEFICIARY_METRE
  )c
  ON a.METRE_SNO=c.METRE_SNO]]></queryString>

