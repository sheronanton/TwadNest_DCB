	<queryString><![CDATA[select 
*
from 
(
SELECT   b.district_code,b.beneficiary_name,b.BENEFICIARY_SNO,year,month,
(select district_name from com_mst_districts where district_code=b.district_code) as dist_name,
(select sch_name from pms_sch_master where sch_sno =a.sch_sno) as sch_name,a.SCH_SNO, 
    SUM(a.QTY_CONSUMED_NET) / 1000 AS tot_QTY_CONSUMED_NET
  FROM PMS_DCB_TRN_MONTHLY_PR a,PMS_DCB_MST_BENEFICIARY b
  where a.month=$P{month} and a.year=$P{year} and 
  b.beneficiary_type_id_sub > 6 and
  b.status='L' and a.beneficiary_sno=b.beneficiary_sno
  and a.office_id=b.office_id
  GROUP BY year,month, b.district_code, a.SCH_SNO,b.BENEFICIARY_SNO,b.beneficiary_name order by year,month,b.district_code,a.SCH_SNO,b.BENEFICIARY_SNO
)mas

 join
(select sch_sno as psch_sno,month as pmonth,year as pyear ,sum(PUMPING_QTY) as pqty from PMS_AME_TRN_SCH_DP_QTY where  year=$P{year} and month=$P{month} group by  sch_sno,year,month
)PUMP
on mas.sch_sno=pump.psch_sno
and mas.month=pump.pmonth
and mas.year=pump.pyear
  
order by year,month,mas.district_code,mas.SCH_SNO,mas.BENEFICIARY_SNO]]></queryString>

