	</variable>
	<variable name="netotheramt" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[( $F{add_charges_wc}).subtract($F{minus_charges_wc})]]></variableExpression>
	</variable>
	<variable name="netotheramt_mc" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[($F{add_charges_maint}).subtract( $F{minus_charges_maint})]]></variableExpression>
	</variable>
	<variable name="int_amt" class="java.math.BigDecimal" resetType="None" calculation="Nothing">
		<variableExpression><![CDATA[$F{int_calc_wc}.multiply(new java.math.BigDecimal("100")).divide( $F{int_rate})]]></variableExpression>
	</variable>
	<variable name="int_display" class="java.lang.String" resetType="Report" calculation="Nothing">
