	<queryString><![CDATA[SELECT DMD.BILL_SNO,
RE.OFFICE_REMARK,
  DMD.OFFICE_ID,
  DMD.BENEFICIARY_SNO, 
  COALESCE(DMD.ADD_CHARGES_MAINT,NULL,'0') AS  ADD_CHARGES_MAINT,
  COALESCE(DMD.MINUS_CHARGES_MAINT,NULL,'0') AS MINUS_CHARGES_MAINT,  
  TO_CHAR(DMD.BILL_PERIOD_FROM,'dd/mm/yyyy') AS BILL_PERIOD_FROM,
  TO_CHAR(DMD.BILL_PERIOD_TO,'dd/mm/yyyy')   AS BILL_PERIOD_TO,
      CASE WHEN (LENGTH(DMD.BILL_MONTH::VARCHAR)<2)
            THEN CONCAT('0',DMD.BILL_MONTH)
            ELSE CONCAT('',DMD.BILL_MONTH) END
            AS BILL_MONTH,
  DMD.BILL_YEAR,
        CASE WHEN (LENGTH(DMD.DIV_BILL_NO::VARCHAR)=1)  THEN CONCAT('00',DMD.DIV_BILL_NO)
           WHEN LENGTH(DMD.DIV_BILL_NO::VARCHAR)=2  THEN CONCAT('0',DMD.DIV_BILL_NO)
            ELSE CONCAT('',DMD.DIV_BILL_NO)           
	     END
            AS   DIV_BILL_NO,
  DMD.NET_CONSUMPTION,
  TO_CHAR(DMD.BILLING_DT,'dd/mm/yyyy') AS BILLING_DT,
  BEN.BENEFICIARY_NAME,
  BEN.BENEFICIARY_TYPE_ID,
  case when (BEN.BILLING_ADDRESS1 is null) then ' ' else BEN.BILLING_ADDRESS1 end AS BILLING_ADDRESS1,
	case when (BEN.BILLING_ADDRESS2 is null) then ' ' else BEN.BILLING_ADDRESS2 end AS BILLING_ADDRESS2,
	case when (BEN.BILLING_ADDRESS3 is null) then ' ' else BEN.BILLING_ADDRESS3 end AS BILLING_ADDRESS3,
	case when (BEN.BILLING_PIN_CODE is null) then ' ' else CONCAT('Pincode: ',BEN.BILLING_PIN_CODE) end  AS BILLING_PIN_CODE,
	DIV.OFFICE_NAME, 	
	
	COALESCE (DMD.COLN_INT_WC, 0) AS COLN_INT_WC,
	
 
 COALESCE(DMD.DMD_FOR_MTH_MC,0) AS DMD_FOR_MTH_MC,
  COALESCE(DMD.OB_YESTER_YR_WC,0) AS  OB_YESTER_YR_WC ,
  COALESCE(DMD.ob_cur_yr_wc,0) AS ob_cur_yr_wc,
  COALESCE(DMD.COLN_YESTER_YR_WC,NULL,'0') AS  COLN_YESTER_YR_WC,
  COALESCE(DMD.coln_cur_yr_wc,NULL,0) AS coln_cur_yr_wc,
  COALESCE(DMD.totob,NULL,0) AS  totob,
  COALESCE(DMD.INT_CALC_WC,NULL,0) AS INT_CALC_WC,
  COALESCE(DMD.coln_maint,NULL,0) AS coln_maint, 
  COALESCE(DMD.OB_MAINT_CHARGES,NULL,0) AS OB_MAINT_CHARGES, 
  COALESCE(INT.INT_RATE,NULL,0) AS INT_RATE, 
  COALESCE(DMD.DMD_INT_FOR_MTH_WC ,NULL,0 ) AS DMD_INT_FOR_MTH_WC , 
  COALESCE(DMD.CB_INT_AMT_WC ,NULL,0 ) AS CB_INT_AMT_WC , 
  COALESCE(DMD.COLN_INT_WC ,NULL,0 ) AS COLN_INT_WC , 
  COALESCE(DMD.OB_INT_AMT_WC ,NULL,0 ) AS OB_INT_AMT_WC ,   
COALESCE(DMD.ADD_CHARGES_WC ,NULL,0 ) AS ADD_CHARGES_WC ,    
  COALESCE(DMD.MINUS_CHARGES_WC ,NULL,0 ) AS MINUS_CHARGES_WC , 
  COALESCE(DMD.ADD_CHARGES_INT ,NULL,0 ) AS ADD_CHARGES_INT ,    
  COALESCE(DMD.MINUS_CHARGES_INT ,NULL,0 ) AS MINUS_CHARGES_INT ,   
  DMD.WC_MTH_TOTAL,DMD.CB_CUR_YR_WC,
  DMD.MONTH_BILL_AMT,
  COALESCE(CONCAT('Bill Value is Rs.',DMD.AMT_WORDS),NULL,' ') AS AMT_WORDS,
  btp.ADDRESS_TO  ,BTP.REMARKS
FROM (
  (SELECT BILL_SNO,ADDRESS_TO,
    OFFICE_ID,
    BENEFICIARY_SNO,
    BILL_PERIOD_FROM,
    BILL_PERIOD_TO,
    BILL_MONTH,
    BILL_YEAR,
    DIV_BILL_NO,
    NET_CONSUMPTION,
    BILLING_DT ,
    COLN_INT_WC,
    COLN_YESTER_YR_WC,
    ob_cur_yr_wc,
    OB_YESTER_YR_WC,
    (ob_cur_yr_wc+OB_YESTER_YR_WC) as totob,
    coln_cur_yr_wc,CB_CUR_YR_WC,
    ob_maint_charges,
    INT_CALC_WC,
    coln_maint,
    WC_MTH_TOTAL,
    MONTH_BILL_AMT,
    AMT_WORDS,DMD_INT_FOR_MTH_WC,
    CB_INT_AMT_WC ,
    OB_INT_AMT_WC,DMD_FOR_MTH_MC  ,ADD_CHARGES_INT,MINUS_CHARGES_INT,  ADD_CHARGES_WC,MINUS_CHARGES_WC  ,
    ADD_CHARGES_MAINT,MINUS_CHARGES_MAINT	   
  FROM PMS_DCB_TRN_BILL_DMD
  WHERE BILL_SNO in (select BILL_SNO from PMS_DCB_TRN_BILL_DMD where  BILL_SNO= $P{billsno}::numeric )  

  ) DMD
JOIN
  (SELECT BENEFICIARY_SNO,
    upper(BENEFICIARY_NAME) as BENEFICIARY_NAME ,
    BENEFICIARY_TYPE_ID,
    DISTRICT_CODE,
    BLOCK_SNO,
    BILLING_ADDRESS1,
    BILLING_ADDRESS2,
    BILLING_ADDRESS3,
    BILLING_PIN_CODE,
    OTHERS_PRIVATE_SNO
  FROM PMS_DCB_MST_BENEFICIARY
  ) BEN
ON BEN.BENEFICIARY_SNO=DMD.BENEFICIARY_SNO
JOIN
  (SELECT OFFICE_NAME, OFFICE_ID FROM com_mst_offices
  )div
ON div.OFFICE_ID=DMD.OFFICE_ID
JOIN
  ( SELECT BENEFICIARY_TYPE,INT_RATE FROM PMS_DCB_MST_INT  where ACTIVE_STATUS='A'
  ) INT
ON INT.BENEFICIARY_TYPE=BEN.BENEFICIARY_TYPE_ID
JOIN
  (
      SELECT BEN_TYPE_ID,ADDRESS_TO,REMARKS FROM PMS_DCB_BEN_TYPE
  ) BTP
 ON BEN.BENEFICIARY_TYPE_ID=BEN_TYPE_ID
 JOIN
  (
      SELECT OFFICE_ID,REMARK As OFFICE_REMARK FROM PMS_DCB_OFFICE_BILL
  ) RE
 ON RE.OFFICE_ID=DMD.OFFICE_ID
 
)order by DIV_BILL_NO]]></queryString>

