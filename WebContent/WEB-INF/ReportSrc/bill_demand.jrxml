	<variable name="balance_wc" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[($F{ob_yester_yr_wc}).add(($F{ob_cur_yr_wc} )).subtract(($F{coln_cur_yr_wc}).add(($F{coln_yester_yr_wc})))]]></variableExpression>
	</variable>
	<variable name="balance_mc" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[$F{ob_maint_charges}.subtract( $F{coln_maint} )]]></variableExpression>
	</variable>
	<variable name="BALANCE" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[$V{balance_wc}.add( $V{balance_mc} ).add($F{ob_int_amt_wc}.subtract($F{coln_int_wc}))]]></variableExpression>
	</variable>
	<variable name="OB" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[( ($F{ob_yester_yr_wc}.add( $F{ob_cur_yr_wc} )) ) .add($F{ob_maint_charges}) .add ($F{ob_int_amt_wc})]]></variableExpression>
	</variable>
	<variable name="COLN" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[$F{coln_cur_yr_wc}.add( $F{coln_maint} ).add ($F{coln_yester_yr_wc}).add($F{coln_int_wc})]]></variableExpression>
	</variable>
	<variable name="out_penalty" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[$V{BALANCE}.add( $F{month_bill_amt} )]]></variableExpression>
	</variable>
	<variable name="tot_ob" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[(  $F{ob_yester_yr_wc}.add( $F{ob_cur_yr_wc} ) )]]></variableExpression>
	</variable>
	<variable name="bal_int" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[($F{ob_int_amt_wc}).subtract(($F{coln_int_wc}))]]></variableExpression>
	</variable>
	<variable name="mon_int" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[$F{int_rate}]]></variableExpression>
	</variable>
	<variable name="month_int_cal" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[$V{balance_wc}.multiply( $V{mon_int} )]]></variableExpression>
		<initialValueExpression><![CDATA[new java.math.BigDecimal("0")]]></initialValueExpression>
	</variable>
	<variable name="month_bal" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[($F{month_bill_amt}).add($V{int_res}).add($F{dmd_for_mth_mc})]]></variableExpression>
	</variable>
	<variable name="int_res" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[new java.math.BigDecimal(  ($V{month_int_cal}.floatValue()) /100)]]></variableExpression>
	</variable>
	<variable name="net_bal" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[$F{month_bill_amt}.add( $V{int_res}.add($F{dmd_for_mth_mc} ))]]></variableExpression>
	</variable>
	<variable name="net_value" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[$V{BALANCE}.add($V{net_bal})]]></variableExpression>
	</variable>
	<variable name="var" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[$V{net_value}]]></variableExpression>
	</variable>
	<variable name="var2" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[$F{month_bill_amt}.add($V{balance_wc} )]]></variableExpression>
	</variable>
	<variable name="var3" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[$F{dmd_for_mth_mc}.add($V{balance_mc})]]></variableExpression>
	</variable>
	<variable name="var4" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[$V{bal_int}.add(  $V{int_res} )]]></variableExpression>
	</variable>
	<variable name="blk" class="java.lang.String" resetType="Report" calculation="Nothing">
	</variable>
	<variable name="row1_sum" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[$F{month_bill_amt}.add(  ( $F{dmd_for_mth_mc})).add(  $F{int_calc_wc})]]></variableExpression>
	</variable>
	<variable name="net_sum_1_new" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[$V{Balance_new_1}.add($V{row1_sum})]]></variableExpression>
		<initialValueExpression><![CDATA[new java.math.BigDecimal("0")]]></initialValueExpression>
	</variable>
	<variable name="Balance_new" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[$V{Balance_after_other_charge}.add($V{balance_mc}).add(  ($F{ob_int_amt_wc}))]]></variableExpression>
	</variable>
	<variable name="Balance_after_other_charge" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
