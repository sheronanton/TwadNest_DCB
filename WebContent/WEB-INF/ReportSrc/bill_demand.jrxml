	</variable>
	<variable name="int_display_2" class="java.lang.String" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[">>"+$F{int_calc_wc}]]></variableExpression>
	</variable>
	<variable name="netotheramt_int" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[($F{add_charges_int}).subtract( $F{minus_charges_int})]]></variableExpression>
	</variable>
	<variable name="net_tota_other_charges" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[$V{netotheramt}.add($V{netotheramt_int})]]></variableExpression>
	</variable>
