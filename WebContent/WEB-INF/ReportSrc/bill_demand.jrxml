	</variable>
	<variable name="Balance_new_1" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[$V{Balance_after_other_charge}.add($V{balance_mc}).add(  ( $V{bal_int}))]]></variableExpression>
	</variable>
	<variable name="net_sum_new" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
