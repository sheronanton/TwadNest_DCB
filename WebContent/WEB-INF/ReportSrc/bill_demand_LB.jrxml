		<defaultValueExpression ><![CDATA[$F{BILL_SNO}]]></defaultValueExpression>
	</parameter>
	<queryString><![CDATA[SELECT DMD.BILL_SNO,
  DMD.OFFICE_ID,
  DMD.BENEFICIARY_SNO,
  TO_CHAR(DMD.BILL_PERIOD_FROM,'dd/mm/yyyy') AS BILL_PERIOD_FROM,
  TO_CHAR(DMD.BILL_PERIOD_TO,'dd/mm/yyyy')   AS BILL_PERIOD_TO,
      CASE WHEN LENGTH(DMD.BILL_MONTH)<2
            THEN CONCAT('0',DMD.BILL_MONTH)
            ELSE CONCAT('',DMD.BILL_MONTH) END
            AS BILL_MONTH,
  DMD.BILL_YEAR,
      CASE WHEN LENGTH(DMD.DIV_BILL_NO)<2
            THEN CONCAT('0',DMD.DIV_BILL_NO)
            ELSE CONCAT('',DMD.DIV_BILL_NO) END
            AS DIV_BILL_NO,
  DMD.NET_CONSUMPTION,
  TO_CHAR(DMD.BILLING_DT,'dd/mm/yyyy') AS BILLING_DT,
  BEN.BENEFICIARY_NAME,
  BEN.BENEFICIARY_TYPE_ID,
  BEN.BILLING_ADDRESS1,
  BEN.BILLING_ADDRESS2,
  BEN.BILLING_ADDRESS3,
  BEN.BILLING_PIN_CODE,
  DIV.OFFICE_NAME ,
  DMD.COLN_INT_WC,
  DMD.OB_CUR_YR_WC,
  DMD.OB_MAINT_CHARGES,
  DMD.INT_CALC_WC,
  DMD.CB_CUR_YR_WC,
  DMD.COLN_CUR_YR_WC,
  DMD.COLN_MAINT,
  INT.INT_RATE,
  DMD.WC_MTH_TOTAL,
  DMD.MONTH_BILL_AMT,
  DMD.AMT_WORDS
FROM
  (SELECT BILL_SNO,
    OFFICE_ID,
    BENEFICIARY_SNO,
    BILL_PERIOD_FROM,
    BILL_PERIOD_TO,
    BILL_MONTH,
    BILL_YEAR,
    DIV_BILL_NO,
    NET_CONSUMPTION,
    BILLING_DT ,
    COLN_INT_WC,
    OB_CUR_YR_WC,
    CB_CUR_YR_WC,
    OB_MAINT_CHARGES,
    INT_CALC_WC,
    COLN_MAINT,
    WC_MTH_TOTAL,
    MONTH_BILL_AMT,
    AMT_WORDS,
    COLN_CUR_YR_WC
  FROM PMS_DCB_TRN_BILL_DMD
  WHERE BILL_SNO=1
  ) DMD
JOIN
  (SELECT BENEFICIARY_SNO,
    BENEFICIARY_NAME,
    BENEFICIARY_TYPE_ID,
    DISTRICT_CODE,
    BLOCK_SNO,
    BILLING_ADDRESS1,
    BILLING_ADDRESS2,
    BILLING_ADDRESS3,
    BILLING_PIN_CODE
  FROM PMS_DCB_MST_BENEFICIARY where status='L'
  ) BEN
ON BEN.BENEFICIARY_SNO=DMD.BENEFICIARY_SNO
JOIN
  (SELECT OFFICE_NAME, OFFICE_ID FROM com_mst_all_offices_view
  )div
ON div.OFFICE_ID=DMD.OFFICE_ID
JOIN
  ( SELECT BENEFICIARY_TYPE,INT_RATE FROM PMS_DCB_MST_INT
  ) INT
ON INT.BENEFICIARY_TYPE=BEN.BENEFICIARY_TYPE_ID]]></queryString>

