	<parameter name="billsno" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT 
  SCH_NAME,
  OFFICE_NAME,
  METRE_INITIAL_READING,
  METRE_CLOSING_READING,
  QTY_CONSUMED,
  TOTAL_AMT
FROM
  (
    SELECT 
      SUBDIV_OFFICE_ID,
      QTY_CONSUMED,
      TOTAL_AMT,
      SCHEME_SNO,
      PR_SNO
    FROM PMS_DCB_WC_BILLING
    WHERE WC_TRN_SNO=$P{billsno}
  )WC
  
  JOIN
  
  (
    SELECT PR_SNO,
      METRE_INITIAL_READING,
      METRE_CLOSING_READING,METRE_SNO
    FROM PMS_DCB_TRN_MONTHLY_PR where METRE_SNO in ( select METRE_SNO  from PMS_DCB_MST_BENEFICIARY_METRE where  METER_STATUS = 'L'  )
  )PR
  ON WC.PR_SNO = PR.PR_SNO
  
  JOIN
  
  (
    SELECT SCH_SNO,
      SCH_NAME
    FROM PMS_SCH_MASTER
  )SC
  ON WC.SCHEME_SNO = SC.SCH_SNO
  
  JOIN
  
  (
    SELECT OFFICE_ID,
      OFFICE_NAME
    FROM COM_MST_OFFICES
  )OFC
  ON WC.SUBDIV_OFFICE_ID = OFC.OFFICE_ID]]></queryString>

