	<variable name="BALANCE_WC" class="java.math.BigDecimal" resetType="Report" calculation="Nothing">
		<variableExpression><![CDATA[($F{OB_YESTER_YR_WC}).add(($F{OB_CUR_YR_WC} )).subtract(($F{COLN_CUR_YR_WC}).add(($F{COLN_YESTER_YR_WC})))]]></variableExpression>
