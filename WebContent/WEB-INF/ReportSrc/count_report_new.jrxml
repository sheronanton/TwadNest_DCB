	<parameter name="year" isForPrompting="true" class="java.lang.String"/>
	<parameter name="monthval" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT 

  Off1.OFFICE_NAME as reg,
  off2.OFFICE_NAME as div,pqty.qty,collection.col,
  pr.prvcount,
  DECODE( ben1.ben1count,NULL,0, ben1.ben1count) AS  ben1count,
  DECODE( ben2.ben2count,NULL,0, ben2.ben2count) AS  ben2count,
  DECODE( met1.met1count,NULL,0, met1.met1count) AS  met1count,
  DECODE( met2.met2count,NULL,0, met2.met2count) AS  met2count,  
  metfr.metfrcount,wc.amt,
  metnotset.metnotsetr,
  metnfr.metnfrcount,metsetdone.metsetdoner,
  rate.ratecount,
  ratenot.ratenotcount,
  off2.OFFICE_ID,ob.op,prr.prc,prrcr.prcr,prrv.prvc,prrf.prfc,dmd.dmdc,jr.st,act.ltcount,tb.TB_STATUS,dcbfr.DCB_FREEZE
FROM (
  (SELECT REGION_OFFICE_ID,
    OFFICE_LEVEL_ID,
    OFFICE_ID,
    OFFICE_NAME
  FROM COM_MST_ALL_OFFICES_VIEW
  WHERE OFFICE_LEVEL_ID='RN' 
  )Off1
JOIN
  (SELECT REGION_OFFICE_ID,
    OFFICE_LEVEL_ID,
    OFFICE_ID,
    OFFICE_NAME
  FROM COM_MST_ALL_OFFICES_VIEW
  WHERE OFFICE_LEVEL_ID='DN'  and OFFICE_ID in (select OFFICE_ID from PMS_DCB_DIV_DIST_MAP)
  )off2
ON off2.REGION_OFFICE_ID=off1.OFFICE_ID
left outer JOIN
  ( SELECT OFFICE_ID ,COUNT(*) as prvcount  FROM COM_MST_PRIVATE GROUP BY OFFICE_ID
  )pr
ON pr.OFFICE_ID=off2.OFFICE_ID

left outer JOIN
  ( SELECT OFFICE_ID ,COUNT(*) as ben1count  FROM PMS_DCB_MST_BENEFICIARY  where BENEFICIARY_TYPE_ID <=6 and BENEFICIARY_SNO   in ( select BENEFICIARY_SNO  from PMS_DCB_MST_BENEFICIARY_METRE where   METER_STATUS='L' )  GROUP BY OFFICE_ID
  )ben1
ON ben1.OFFICE_ID=off2.OFFICE_ID

left outer JOIN
  ( SELECT OFFICE_ID ,COUNT(*) as ben2count  FROM PMS_DCB_MST_BENEFICIARY  where status='L'  and BENEFICIARY_TYPE_ID >6 and BENEFICIARY_SNO   in ( select BENEFICIARY_SNO  from PMS_DCB_MST_BENEFICIARY_METRE where   METER_STATUS='L' )  GROUP BY OFFICE_ID
  )ben2
ON ben2.OFFICE_ID=off2.OFFICE_ID 

left outer JOIN
  ( SELECT OFFICE_ID ,COUNT(*) as met1count  FROM PMS_DCB_MST_BENEFICIARY_METRE  where METER_STATUS='L' and BENEFICIARY_TYPE_ID <=6 GROUP BY OFFICE_ID
  )met1
ON met1.OFFICE_ID=off2.OFFICE_ID   

left outer JOIN
  ( SELECT OFFICE_ID ,COUNT(*) as met2count  FROM PMS_DCB_MST_BENEFICIARY_METRE  where  METER_STATUS='L' and  BENEFICIARY_TYPE_ID >6 GROUP BY OFFICE_ID
  )met2
ON met2.OFFICE_ID=off2.OFFICE_ID   
left outer JOIN
  ( SELECT OFFICE_ID ,COUNT(*) as metnotsetr  FROM PMS_DCB_MST_BENEFICIARY_METRE  where METER_STATUS='L' and   SETTING_FLAG is null GROUP BY OFFICE_ID
  )metnotset
ON metnotset.OFFICE_ID=off2.OFFICE_ID
left outer  JOIN
  ( SELECT OFFICE_ID ,COUNT(*) as metsetdoner  FROM PMS_DCB_MST_BENEFICIARY_METRE  where  METER_STATUS='L' and  SETTING_FLAG is Not null GROUP BY OFFICE_ID
  )metsetdone
ON metsetdone.OFFICE_ID=off2.OFFICE_ID
left outer JOIN
  ( SELECT OFFICE_ID ,COUNT(*) as metfrcount  FROM PMS_DCB_MST_BENEFICIARY_METRE  where  METER_STATUS='L' and  SETTING_FLAG ='FR' GROUP BY OFFICE_ID
  )metfr
ON metfr.OFFICE_ID=off2.OFFICE_ID

left outer JOIN
  ( SELECT OFFICE_ID ,COUNT(*) as metnfrcount  FROM PMS_DCB_MST_BENEFICIARY_METRE  where  METER_STATUS='L' and   (SETTING_FLAG !='FR' or SETTING_FLAG is null) GROUP BY OFFICE_ID
  )metnfr
ON metnfr.OFFICE_ID=off2.OFFICE_ID

left outer JOIN
  ( SELECT OFFICE_ID ,COUNT(*) as ratecount  FROM PMS_DCB_MST_BENEFICIARY_METRE  where  METER_STATUS='L' and  TARIFF_RATE is not null GROUP BY OFFICE_ID
  )rate
ON rate.OFFICE_ID=off2.OFFICE_ID

left outer JOIN
  ( SELECT OFFICE_ID ,COUNT(*) as ratenotcount  FROM PMS_DCB_MST_BENEFICIARY_METRE  where  METER_STATUS='L' and  TARIFF_RATE is   null GROUP BY OFFICE_ID
  )ratenot
ON ratenot.OFFICE_ID=off2.OFFICE_ID
left outer join
(select count (*) as op,OFFICE_ID, FIN_YEAR,MONTH from PMS_DCB_OB_YEARLY where  beneficiary_sno IN (SELECT beneficiary_sno FROM PMS_DCB_MST_BENEFICIARY WHERE status='L'  )  and       fin_year=$P{year} and month=$P{month}  group by office_id, fin_year, month) ob
on ob.OFFICE_ID=off2.OFFICE_ID
left outer join
(select count (*) as prc,OFFICE_ID, YEAR,MONTH from PMS_DCB_TRN_MONTHLY_PR where   beneficiary_sno IN (SELECT beneficiary_sno FROM PMS_DCB_MST_BENEFICIARY WHERE status='L'  ) and beneficiary_sno   IN  (  SELECT beneficiary_sno FROM PMS_DCB_MST_BENEFICIARY_METRE  WHERE  METER_STATUS='L' )  and year=$P{year} and month=$P{month}  group by office_id, year, month) prr
on prr.OFFICE_ID=off2.OFFICE_ID
left outer join
(select count (*) as prcr,OFFICE_ID, YEAR,MONTH from PMS_DCB_TRN_MONTHLY_PR where  beneficiary_sno IN (SELECT beneficiary_sno FROM PMS_DCB_MST_BENEFICIARY WHERE status='L'  ) and beneficiary_sno   IN  (  SELECT beneficiary_sno FROM PMS_DCB_MST_BENEFICIARY_METRE  WHERE  METER_STATUS='L' )  and   year=$P{year}     and month=$P{month} and PROCESS_FLAG='CR'  group by office_id, year, month) prrcr
on prrcr.OFFICE_ID=off2.OFFICE_ID

left outer join
(select count (*) as prvc,OFFICE_ID, YEAR,MONTH from PMS_DCB_TRN_MONTHLY_PR where   beneficiary_sno IN (SELECT beneficiary_sno FROM PMS_DCB_MST_BENEFICIARY WHERE status='L'  ) and beneficiary_sno   IN  (  SELECT beneficiary_sno FROM PMS_DCB_MST_BENEFICIARY_METRE  WHERE  METER_STATUS='L' )  and   year=$P{year} and month=$P{month} and PROCESS_FLAG='V'  group by office_id, year, month) prrv
on prrv.OFFICE_ID=off2.OFFICE_ID
left outer join
(select count (*) as prfc,OFFICE_ID, YEAR,MONTH from PMS_DCB_TRN_MONTHLY_PR where   beneficiary_sno IN (SELECT beneficiary_sno FROM PMS_DCB_MST_BENEFICIARY WHERE status='L'  ) and beneficiary_sno   IN  (  SELECT beneficiary_sno FROM PMS_DCB_MST_BENEFICIARY_METRE  WHERE  METER_STATUS='L' )  and   year=$P{year} and month=$P{month} and PROCESS_FLAG='FR'  group by office_id, year, month) prrf
on prrf.OFFICE_ID=off2.OFFICE_ID
left outer join
(select count (*) as dmdc,OFFICE_ID,BILL_YEAR,BILL_MONTH from PMS_DCB_TRN_BILL_DMD where   beneficiary_sno IN (SELECT beneficiary_sno FROM PMS_DCB_MST_BENEFICIARY WHERE status='L'  ) and BILL_YEAR=$P{year} and BILL_MONTH=$P{month}  group by office_id, BILL_YEAR, BILL_MONTH) dmd
on dmd.OFFICE_ID=off2.OFFICE_ID 
)
 left  outer join 
(
   SELECT OFFICE_ID,
   case when JOURNAL_POSTED  is null then null else 'Yes' end   AS st
  FROM PMS_DCB_FREEZE_STATUS
  where   CASHBOOK_YEAR=$P{year} and CASHBOOK_MONTH=$P{month}
  )jr
ON jr.OFFICE_ID=off2.OFFICE_ID
 LEFT OUTER join 
(
  select 
   count(DISTINCT beneficiary_sno) as ltcount ,OFFICE_ID
  from   
     PMS_DCB_LEDGER_ACTUAL   
       where YEAR        =$P{year}
  AND MONTH       =$P{month}
  group by OFFICE_ID
)act
ON act.OFFICE_ID=off2.OFFICE_ID
left outer join
(
	select 
	ACCOUNTING_UNIT_ID,ACCOUNTING_UNIT_OFFICE_ID from 
	FAS_MST_ACCT_UNITS

)aunit
on aunit.ACCOUNTING_UNIT_OFFICE_ID=off2.OFFICE_ID
left outer join
(
	select case when TB_STATUS='Y' then 'Yes' else '-' end as  TB_STATUS  ,ACCOUNTING_UNIT_ID from FAS_TRIAL_BALANCE_STATUS where CASHBOOK_YEAR=$P{year} and CASHBOOK_MONTH=$P{month}
)tb
on tb.ACCOUNTING_UNIT_ID=aunit.ACCOUNTING_UNIT_ID

left outer  join 
 (
 select round(sum(TOTAL_AMT) / 100000,2) as amt ,office_id,month,year from PMS_DCB_WC_BILLING OFFICE_ID where MONTH=$P{month} and  YEAR=$P{year} group by office_id,year,month 
 ) wc 
on wc.OFFICE_ID=off2.OFFICE_ID

left outer join 
(
	select   sum(QTY_CONSUMED_NET) as qty,office_id,month,year    from PMS_DCB_TRN_MONTHLY_PR where MONTH=$P{month} and  YEAR=$P{year} and METRE_SNO	 IN
  (SELECT METRE_SNO	
   FROM pms_dcb_mst_beneficiary_metre where meter_status = 'L')  group by office_id,year,month 
)pqty
on pqty.OFFICE_ID=off2.OFFICE_ID

left outer join 
(
		select  round(sum(amount)/ 100000,2) as col ,ACCOUNTING_FOR_OFFICE_ID,cashbook_year,cashbook_month from FAS_RECEIPT_TRANSACTION a  where   ACCOUNT_HEAD_CODE in (SELECT ACCOUNT_HEAD_CODE  from PMS_DCB_RECEIPT_ACCOUNT_MAP where COLLECTION_TYPE in (6,7,8,9)) and   cashbook_month =$P{month} AND cashbook_year =$P{year}
                                          AND receipt_no IN 
                                            ( SELECT receipt_no FROM FAS_RECEIPT_MASTER b  WHERE SUB_LEDGER_TYPE_CODE  = 14 
                                                                and cashbook_month=$P{month}  AND cashbook_year=$P{year}
                                                                and RECEIPT_STATUS='L'   and a.ACCOUNTING_FOR_OFFICE_ID=b.ACCOUNTING_FOR_OFFICE_ID )
		group by ACCOUNTING_FOR_OFFICE_ID,cashbook_year,cashbook_month
)collection

on collection.ACCOUNTING_FOR_OFFICE_ID=off2.OFFICE_ID
left outer join
(
	select case when DCB_FREEZE='Y' then 'Yes' else '-' end as  DCB_FREEZE  ,ACCOUNTING_UNIT_ID from PMS_DCB_DATA_FREEZE where CASHBOOK_YEAR=$P{year} and CASHBOOK_MONTH=$P{month}
)dcbfr
on dcbfr.ACCOUNTING_UNIT_ID=aunit.ACCOUNTING_UNIT_ID]]></queryString>

