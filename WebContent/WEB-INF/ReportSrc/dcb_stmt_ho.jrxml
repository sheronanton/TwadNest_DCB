	<queryString><![CDATA[SELECT beneficiary_name,BEN_TYPE_DESC,
 SUM(ADD_DMD_UPTO)as dmd,
  SUM(opening_bal_wc),
  sum(DMD_UPTO_PRV_MNTH_WC)+sum(DMD_FOR_MTH_WC) as totaldemand,
  sum(COLN_INCLUDE_CHARGES) as COLN_FOR_MTH_WC  ,
  SUM(opening_bal_mc),
  SUM(col_uto_prv_mnh_mc),
  SUM(tot_coln_yes_yr_dmd) + SUM(tot_coln_for_yr_dmd) AS
totcol,
  SUM(net_due),BEN_TYPE_ID 	
FROM

 pms_dcb_ledger_actual

WHERE  office_id=$P{office_id} and 
   MONTH =$P{month}
and year = $P{year}
GROUP BY  BEN_TYPE_ID,BEN_TYPE_DESC,beneficiary_name 
order by   BEN_TYPE_ID,beneficiary_name]]></queryString>

