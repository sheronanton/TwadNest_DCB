	<queryString><![CDATA[SELECT DECODE(ac.OPENING_BAL_MC_3,null,0.0,ac.OPENING_BAL_MC_3)                         AS OPENING_BAL_MC_3,
  DECODE(ac.ADDNS_IF_ANY_3A ,NULL,0.0, ac.ADDNS_IF_ANY_3A)                              AS ADDNS_IF_ANY_3A ,
  DECODE(ac.COL_UTO_PRV_MNH_MC_4 ,NULL,0.0,ac.COL_UTO_PRV_MNH_MC_4)                     AS COL_UTO_PRV_MNH_MC_4 ,
  DECODE(ac.COLL_FOR_MTH_MAINT_5 ,NULL,0.0,ac.COLL_FOR_MTH_MAINT_5 )                    AS COLL_FOR_MTH_MAINT_5,
  DECODE(ac.TOTAL_COLLN_6 ,NULL,0.0, ac.TOTAL_COLLN_6 )                                 AS TOTAL_COLLN_6,
  DECODE(ac.BALANCE_7,NULL,0.0,ac.BALANCE_7)                                            AS BALANCE_7,
  DECODE(ac.OPENING_BAL_WC_8 ,NULL,0.0, ac.OPENING_BAL_WC_8)                            AS OPENING_BAL_WC_8,
  DECODE(ac.DMD_UPTO_PRV_MNTH_WC_9 ,NULL,0.0, ac.DMD_UPTO_PRV_MNTH_WC_9)                AS DMD_UPTO_PRV_MNTH_WC_9,
  DECODE( ac.DMD_FOR_MTH_WC_10,NULL,0.0,ac.DMD_FOR_MTH_WC_10)                           AS DMD_FOR_MTH_WC_10,
  DECODE( ac.TOTAL_DUES_11,NULL,0.0,ac.TOTAL_DUES_11)                                   AS TOTAL_DUES_11,
  DECODE( ac.COLN_UPTO_PRV_MTH_YES_YR_WC_12,NULL,0.0,ac.COLN_UPTO_PRV_MTH_YES_YR_WC_12) AS COLN_UPTO_PRV_MTH_YES_YR_WC_12,
  DECODE( ac.COLN_UPTO_PRV_MTH_CR_YR_DMD_13,NULL,0.0,ac.COLN_UPTO_PRV_MTH_CR_YR_DMD_13) AS COLN_UPTO_PRV_MTH_CR_YR_DMD_13,
  DECODE( ac.COLN_FOR_MTH_YES_YR_WC_14,NULL,0.0,ac.COLN_FOR_MTH_YES_YR_WC_14)           AS COLN_FOR_MTH_YES_YR_WC_14,
  DECODE( ac.COLN_FOR_MTH_WC_15,NULL,0.0,ac.COLN_FOR_MTH_WC_15)                         AS COLN_FOR_MTH_WC_15,
  DECODE( ac.TOT_COLN_YES_YR_DMD_16,NULL,0.0, ac.TOT_COLN_YES_YR_DMD_16)                AS TOT_COLN_YES_YR_DMD_16,
  DECODE( ac.TOT_COLN_FOR_YR_DMD_17,NULL,0.0, ac.TOT_COLN_FOR_YR_DMD_17)                AS TOT_COLN_FOR_YR_DMD_17,
  DECODE( ac.BALANCE_18 ,NULL,0.0,ac.BALANCE_18)                                        AS BALANCE_18,
  DECODE( ac.NET_DUE_19 ,NULL,0.0,ac.NET_DUE_19)                                        AS NET_DUE_19,
  DECODE( ac.OPENING_BAL_YESTER_YR ,NULL,0.0,ac.OPENING_BAL_YESTER_YR)                  AS OPENING_BAL_YESTER_YR,
  ac.MONTH                                                                              AS MONTH,
  ac.YEAR                                                                               AS YEAR,
case when  BEN_TYPE_GROUP=8 then 'Others (' || ty.BEN_TYPE_GROUP_NAME || ')' else  ty.BEN_TYPE_GROUP_NAME end as BEN_TYPE_GROUP_NAME ,
  --ROUND( ( DECODE(ac.COLN_FOR_MTH_YES_YR_WC_14,NULL,0.0,ac.COLN_FOR_MTH_YES_YR_WC_14)+DECODE(ac.COLN_FOR_MTH_WC_15,NULL,0.0,ac.COLN_FOR_MTH_WC_15) -DECODE(prv.DMD_FOR_MTH_WC_10,NULL,0.0,prv.DMD_FOR_MTH_WC_10) ),2)
COLN_ARR_MTH_WC AS column4 ,
  apr.openingbalacne ,
--ROUND( ( DECODE(ac.COLN_FOR_MTH_YES_YR_WC_14,NULL,0.0,ac.COLN_FOR_MTH_YES_YR_WC_14)+DECODE(ac.COLN_FOR_MTH_WC_15,NULL,0.0,ac.COLN_FOR_MTH_WC_15) -DECODE(prv.DMD_FOR_MTH_WC_10,NULL,0.0,prv.DMD_FOR_MTH_WC_10) ),2)
  ROUND( COLN_ARR_MTH_WC/apr.openingbalacne *100,2) AS avg1 ,
  CASE
    when (   round( (COLN_FOR_MTH_YES_YR_WC_14+COLN_FOR_MTH_WC_15),2)=0.00 ) then  0
    WHEN (   ( COLN_FOR_MTH_YES_YR_WC_14+COLN_FOR_MTH_WC_15) > (ac.DMD_FOR_MTH_WC_10)   )
    THEN 100
    ELSE ROUND( ( (ROUND((COLN_FOR_MTH_YES_YR_WC_14+COLN_FOR_MTH_WC_15),2)) / ROUND (ac.DMD_FOR_MTH_WC_10,2) )*100,2)
  END AS avg2,
  CASE
    WHEN DMD_UPTO_PRV_MNTH_WC_9 > 0
    THEN ROUND(((COLN_UPTO_PRV_MTH_YES_YR_WC_12+COLN_UPTO_PRV_MTH_CR_YR_DMD_13)/DMD_UPTO_PRV_MNTH_WC_9) * 100,2 )
  END AS avg3,
 CASE
   WHEN (OPENING_BAL_MC_3+DMD_UPTO_PRV_MNTH_WC_9+DMD_FOR_MTH_WC_10) > 0
   THEN ROUND ( ((  COLN_UPTO_PRV_MTH_YES_YR_WC_12+COLN_UPTO_PRV_MTH_CR_YR_DMD_13+COLN_FOR_MTH_YES_YR_WC_14) / (OPENING_BAL_MC_3+DMD_UPTO_PRV_MNTH_WC_9+ DMD_FOR_MTH_WC_10) ) / 100,4) * 100
  END AS avg4
  


FROM
  (SELECT 
    DECODE(SUM(OPENING_BAL_MC),null,0.0,SUM(OPENING_BAL_MC))      / 100000               AS OPENING_BAL_MC_3,
    DECODE(SUM(ADDNS_IF_ANY),NULL,0.0,SUM(ADDNS_IF_ANY))     / 100000                            AS ADDNS_IF_ANY_3A,
    DECODE(SUM(COL_UTO_PRV_MNH_MC),NULL,0.0,SUM(COL_UTO_PRV_MNH_MC))   / 100000                  AS COL_UTO_PRV_MNH_MC_4,
    DECODE(SUM(COLL_FOR_MTH_MAINT),NULL,0.0,SUM(COLL_FOR_MTH_MAINT))   / 100000                  AS COLL_FOR_MTH_MAINT_5,
    DECODE(SUM(TOTAL_COLLN),NULL,0.0,SUM(TOTAL_COLLN))    / 100000                               AS TOTAL_COLLN_6,
    DECODE(SUM(BALANCE),NULL,0.0,SUM(BALANCE))   / 100000                                        AS BALANCE_7,
    DECODE(SUM(OPENING_BAL_WC),NULL,0.0,SUM(OPENING_BAL_WC))     / 100000                        AS OPENING_BAL_WC_8,
    DECODE(SUM(DMD_UPTO_PRV_MNTH_WC),NULL,0.0,SUM(DMD_UPTO_PRV_MNTH_WC))    / 100000             AS DMD_UPTO_PRV_MNTH_WC_9,
    DECODE(SUM(DMD_FOR_MTH_WC),NULL,0.0,SUM(DMD_FOR_MTH_WC))   / 100000                          AS DMD_FOR_MTH_WC_10,
    DECODE(SUM(TOTAL_DUES),NULL,0.0,SUM(TOTAL_DUES))   / 100000                                  AS TOTAL_DUES_11,
    DECODE(SUM(COLN_UPTO_PRV_MTH_YES_YR_WC),NULL,0.0,SUM(COLN_UPTO_PRV_MTH_YES_YR_WC))/ 100000   AS COLN_UPTO_PRV_MTH_YES_YR_WC_12,
    DECODE(SUM(COLN_UPTO_PRV_MTH_CR_YR_DMD),NULL,0.0,SUM(COLN_UPTO_PRV_MTH_CR_YR_DMD)) / 100000  AS COLN_UPTO_PRV_MTH_CR_YR_DMD_13,
    DECODE(SUM(COLN_FOR_MTH_YES_YR_WC),NULL,0.0,SUM(COLN_FOR_MTH_YES_YR_WC))  / 100000           AS COLN_FOR_MTH_YES_YR_WC_14,
    DECODE(SUM(COLN_INCLUDE_CHARGES),NULL,0.0,SUM(COLN_INCLUDE_CHARGES))    / 100000                       AS COLN_FOR_MTH_WC_15,
    DECODE(SUM(TOT_COLN_YES_YR_DMD),NULL,0.0,SUM(TOT_COLN_YES_YR_DMD))  / 100000                 AS TOT_COLN_YES_YR_DMD_16,
    DECODE(SUM(TOT_COLN_FOR_YR_DMD),NULL,0.0,SUM(TOT_COLN_FOR_YR_DMD))  / 100000                 AS TOT_COLN_FOR_YR_DMD_17,
    DECODE(SUM(OPENING_BAL_YESTER_YR),NULL,0.0,SUM(OPENING_BAL_YESTER_YR))  / 100000             AS BALANCE_18,
    DECODE(SUM(BALANCE_18),NULL,0.0,SUM(BALANCE_18))        / 100000                             AS OPENING_BAL_YESTER_YR,
    DECODE(SUM(NET_DUE),NULL,0.0,SUM(NET_DUE))   / 100000                                        AS NET_DUE_19,
    DECODE(SUM(COLN_ARR_MTH_WC),NULL,0.0,SUM(COLN_ARR_MTH_WC))   / 100000                                        AS COLN_ARR_MTH_WC,
    BEN_TYPE_GROUP,
    DECODE(MONTH,1,'JANUARY', 2,'February', 3,'March', 4,'April', 5,'May', 6,'June', 7,'July', 8,'August', 9,'September', 10,'October', 11,'November', 12,'December', 'N/A') MONTH,
    YEAR,
    MONTH AS m1
  FROM PMS_DCB_LEDGER_ACTUAL
  WHERE YEAR   =$P{year}
  AND MONTH    =$P{month}
and office_id=$P{office_id}
  GROUP BY BEN_TYPE_GROUP ,
    MONTH,
    YEAR
  )ac
LEFT JOIN
  (SELECT BEN_TYPE_GROUP_NAME ,
    BEN_TYPE_GROUP
  FROM PMS_DCB_BEN_TYPE
  GROUP BY BEN_TYPE_GROUP_NAME,
    BEN_TYPE_GROUP
  )ty
ON ty.BEN_TYPE_GROUP=ac.BEN_TYPE_GROUP
JOIN
  (SELECT MONTH,
    BEN_TYPE_GROUP,
    YEAR,
    DECODE(SUM(DMD_FOR_MTH_WC),NULL,0.0,SUM(DMD_FOR_MTH_WC)) / 100000 AS DMD_FOR_MTH_WC_10
  FROM PMS_DCB_LEDGER_ACTUAL
  WHERE YEAR   =$P{pyear}
  AND MONTH    =$P{pmonth}
 and office_id=$P{office_id}
  GROUP BY MONTH,
    BEN_TYPE_GROUP,
    YEAR
  )prv
ON prv.YEAR           =ac.YEAR
AND prv.BEN_TYPE_GROUP=ac. BEN_TYPE_GROUP
LEFT OUTER JOIN
  (SELECT MONTH,
    BEN_TYPE_GROUP,
    YEAR,
    ( DECODE(SUM(OPENING_BAL_WC),NULL,0.0,SUM(OPENING_BAL_WC)) + DECODE(SUM(OPENING_BAL_YESTER_YR),NULL,0.0,SUM(OPENING_BAL_YESTER_YR)) ) / 100000  AS openingbalacne
  FROM PMS_DCB_LEDGER_ACTUAL
  WHERE YEAR   =$P{pyear}
  AND MONTH    =$P{pmonth}
  and office_id=$P{office_id}
  GROUP BY MONTH,
    BEN_TYPE_GROUP,
    YEAR
  )apr
ON apr.YEAR           =ac.YEAR
AND apr.BEN_TYPE_GROUP=ac. BEN_TYPE_GROUP
AND apr.YEAR          =prv.YEAR
ORDER BY ac.BEN_TYPE_GROUP]]></queryString>

