	<parameter name="sch_name" isForPrompting="false" class="java.lang.String"/>
	<parameter name="office_id" isForPrompting="true" class="java.lang.String"/>
	<parameter name="fin_year" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[Select Ac.*,
  Mitem.Main_Item_Desc || '-' ||
  sitem.SUB_ITEM_DESC as description
From
  (Select Main_Item_Sno,
  Sub_Item_Sno,
  Sum(ACTUAL_EXP) As Amount
From PMS_AME_TRN_SCH_ACT_EXP_FINYR
where  FIN_YEAR=$P{fin_year}
And Sch_Sno              =$P{sch_sno} and Office_Id      =$P{office_id}
GROUP BY Main_Item_Sno, Sub_Item_Sno
  ) ac
LEFT JOIN
  ( SELECT MAIN_ITEM_SNO, MAIN_ITEM_DESC FROM PMS_AME_MST_MAIN_ITEM
  )mitem
ON mitem.MAIN_ITEM_SNO=ac.MAIN_ITEM_SNO
LEFT JOIN
  ( SELECT SUB_ITEM_SNO,SUB_ITEM_DESC FROM PMS_AME_MST_SUB_ITEM
  )sitem
ON sitem.SUB_ITEM_SNO=ac.SUB_ITEM_SNO
Order By Ac.Main_Item_Sno ,

  ac.SUB_ITEM_SNO]]></queryString>

