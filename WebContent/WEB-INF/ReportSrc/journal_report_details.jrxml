	<queryString><![CDATA[SELECT JOU.ACCOUNT_HEAD_CODE as  "ACCOUNT_HEAD_CODE",
  JOU.CR_DR_INDICATOR as "CR_DR_INDICATOR",
  SCH.SCH_NAME as "SCH_NAME",
  AMAS.ACCOUNT_HEAD_DESC as "ACCOUNT_HEAD_DESC",
  JOU.SCHEME_TYPE_ID,
  JOU.OFFICE_ID,
  JOU.AMOUNT as "AMOUNT",
  JOU.SUB_LEDGER_CODE,
  JOU.REMARKS as "REMARKS"
FROM (
  (SELECT AMOUNT,
    OFFICE_ID,
    SCHEME_TYPE_ID,
    ACCOUNT_HEAD_CODE,
    CR_DR_INDICATOR,
    SUB_LEDGER_CODE,
    REMARKS
  FROM PMS_DCB_JOURNAL_DETAILS
  WHERE OFFICE_ID    = $P{office_id}::int
  AND CASHBOOK_MONTH = $P{month}::int
  AND CASHBOOK_YEAR  = $P{year}::int 
  AND SCHEME_TYPE_ID  =$P{schtype}::int and AMOUNT <> 0
  ORDER BY CR_DR_INDICATOR DESC,
    OFFICE_ID,
    SCHEME_TYPE_ID,
    CASHBOOK_MONTH,
    CASHBOOK_YEAR
  ) JOU
LEFT JOIN
  ( SELECT PROJECT_ID, SCH_SNO FROM PMS_MST_PROJECTS_VIEW
  ) prv
ON prv.PROJECT_ID=JOU.SUB_LEDGER_CODE
LEFT JOIN
  ( SELECT SCH_NAME, SCH_SNO FROM PMS_SCH_MASTER
  )SCH
ON SCH.SCH_SNO=prv.SCH_SNO
JOIN
  ( SELECT ACCOUNT_HEAD_CODE, ACCOUNT_HEAD_DESC FROM COM_MST_ACCOUNT_HEADS
  ) AMAS
ON AMAS.ACCOUNT_HEAD_CODE=JOU.ACCOUNT_HEAD_CODE )]]></queryString>

