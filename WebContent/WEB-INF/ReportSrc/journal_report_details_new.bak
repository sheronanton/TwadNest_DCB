	<queryString><![CDATA[SELECT JOU.ACCOUNT_HEAD_CODE,
  JOU.CR_DR_INDICATOR,
  SCH.SCH_NAME,
  AMAS.ACCOUNT_HEAD_DESC,
  JOU.SCHEME_TYPE_ID,
  JOU.OFFICE_ID,
  JOU.AMOUNT,
  JOU.SUB_LEDGER_CODE,
  JOU.REMARKS,JOU.SCHEME_TYPE_ID,stype.SCH_TYPE_DESC
FROM (
  (SELECT AMOUNT,
    OFFICE_ID,
    SCHEME_TYPE_ID,
    ACCOUNT_HEAD_CODE,
    CR_DR_INDICATOR,
    SUB_LEDGER_CODE,
    REMARKS
  FROM PMS_DCB_JOURNAL_DETAILS
  WHERE OFFICE_ID    = $P{office_id}::numeric
  AND CASHBOOK_MONTH = $P{month}::numeric
  AND CASHBOOK_YEAR  = $P{year}::numeric
   
  and AMOUNT <> 0
  ORDER BY CR_DR_INDICATOR DESC,
    OFFICE_ID,
    SCHEME_TYPE_ID,
    CASHBOOK_MONTH,
    CASHBOOK_YEAR
  ) JOU
LEFT JOIN
  ( SELECT PROJECT_ID, SCH_SNO FROM PMS_MST_PROJECTS_VIEW
  ) prv
ON prv.PROJECT_ID=JOU.SUB_LEDGER_CODE
LEFT JOIN
  ( SELECT SCH_NAME, SCH_SNO FROM PMS_SCH_MASTER
  )SCH
ON SCH.SCH_SNO=prv.SCH_SNO
JOIN
  ( SELECT ACCOUNT_HEAD_CODE, ACCOUNT_HEAD_DESC FROM COM_MST_ACCOUNT_HEADS
  ) AMAS
ON AMAS.ACCOUNT_HEAD_CODE=JOU.ACCOUNT_HEAD_CODE 

join 
 (
	select 
		SCH_TYPE_ID,
		SCH_TYPE_DESC
	from 

	PMS_SCH_LKP_TYPE

 )stype
on stype.SCH_TYPE_ID=JOU.SCHEME_TYPE_ID

)
 order by JOU.SCHEME_TYPE_ID,JOU.CR_DR_INDICATOR desc]]></queryString>

