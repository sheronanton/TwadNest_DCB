    WHERE MONTH          =$P{month}
    AND YEAR             =$P{year}
    AND BEN_TYPE_ID_SUB IN (6)
    group by  YEAR,MONTH,OFFICE_ID,OFFICE_NAME,SCH_TYPE_DESC,BEN_TYPE_DESC_SUB,BEN_TYPE_ID_SUB,block_name    
