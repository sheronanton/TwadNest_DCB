	<queryString><![CDATA[select 
  office_name,off.office_id,BENEFICIARY_NAME  ,BENEFICIARY_TYPE_ID_SUB ,ben.BENEFICIARY_SNO,ben_type_desc
  ,decode(mfct,null,0,mfct)  as mfct
 ,decode(mnfct,null,0,mnfct) as mnfct
 ,decode(mwct,null,0,mwct) as mwct
 ,decode(mnwct,null,0,mnwct) as mnwct
 from  
   ( select office_name,office_id from COM_MST_ALL_OFFICES_VIEW where office_level_id='DN'  and office_id in (select office_id from pms_dcb_div_dist_map) )off
  join
   ( select  BENEFICIARY_SNO,BENEFICIARY_NAME,OFFICE_ID,BENEFICIARY_TYPE_ID_SUB , (select a.BEN_TYPE_DESC from PMS_DCB_BEN_TYPE a where a.BEN_TYPE_ID=BENEFICIARY_TYPE_ID_SUB) as ben_type_desc from PMS_DCB_MST_BENEFICIARY where status='L' ) ben 
   on  ben.OFFICE_ID=off.office_id
    left outer join  
  ( select count(*) as mfct ,office_id,BENEFICIARY_SNO from PMS_DCB_MST_BENEFICIARY_METRE where meter_status='L' and upper(METRE_FIXED)=upper('y') group by office_id,BENEFICIARY_SNO )meter_fixed 
  on  meter_fixed.OFFICE_ID=off.office_id and ben.BENEFICIARY_SNO=meter_fixed.BENEFICIARY_SNO
  left outer join 
  ( select count(*) as mnfct ,office_id,BENEFICIARY_SNO from PMS_DCB_MST_BENEFICIARY_METRE where meter_status='L' and upper(METRE_FIXED)=upper('n') group by office_id,BENEFICIARY_SNO )meter_not_fixed 
  on  meter_not_fixed.OFFICE_ID=off.office_id and ben.BENEFICIARY_SNO=meter_not_fixed.BENEFICIARY_SNO
  left outer join 
  ( select count(*) as mwct ,office_id,BENEFICIARY_SNO from PMS_DCB_MST_BENEFICIARY_METRE where meter_status='L' and (upper(METRE_FIXED)=upper('y') and upper(METRE_WORKING)=upper('y')) group by office_id,BENEFICIARY_SNO )meter_worked 
  on  meter_worked.OFFICE_ID=off.office_id and ben.BENEFICIARY_SNO=meter_worked.BENEFICIARY_SNO
  left outer join 
  ( select count(*) as mnwct ,office_id,BENEFICIARY_SNO from PMS_DCB_MST_BENEFICIARY_METRE where meter_status='L' and (upper(METRE_FIXED)=upper('y') and upper(METRE_WORKING)=upper('n')) group by office_id,BENEFICIARY_SNO )meter_not_worked 
  on  meter_not_worked.OFFICE_ID=off.office_id and ben.BENEFICIARY_SNO=meter_not_worked.BENEFICIARY_SNO
 order by office_id,BENEFICIARY_TYPE_ID_SUB,BENEFICIARY_NAME]]></queryString>

