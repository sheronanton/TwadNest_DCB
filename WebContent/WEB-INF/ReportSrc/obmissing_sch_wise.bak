	<queryString><![CDATA[SELECT DISTINCT b.scheme_sno,
  a.beneficiary_sno,
  a.beneficiary_name,c.SCH_NAME,c.SCH_SNO,a.office_id,d.BEN_TYPE_DESC,a.BENEFICIARY_TYPE_ID
FROM pms_dcb_mst_beneficiary a,
  pms_dcb_mst_beneficiary_metre b ,PMS_SCH_MASTER c,PMS_DCB_BEN_TYPE d
WHERE a.beneficiary_sno = b.beneficiary_sno 
 AND a.office_id = b.office_id 
 AND b.meter_status = 'L'
 AND a.status = 'L'
 and c.SCH_SNO=b.scheme_sno
 and d.BEN_TYPE_ID=a.BENEFICIARY_TYPE_ID
 and a.office_id = $P{office_id}::numeric
  AND a.beneficiary_sno not IN
  (SELECT b.beneficiary_sno
   FROM pms_dcb_ob_yearly b
   WHERE b.office_id = $P{office_id}::numeric
   AND b.MONTH = $P{month}::numeric
   AND b.fin_year = $P{year}::numeric
   AND a.beneficiary_sno = b.beneficiary_sno)
 order by beneficiary_sno,scheme_sno]]></queryString>

