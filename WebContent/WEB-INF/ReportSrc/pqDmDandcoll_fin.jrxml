	<parameter name="year2" isForPrompting="true" class="java.lang.Integer"/>
	<parameter name="ftype" isForPrompting="true" class="java.lang.Integer"/>
	<parameter name="month_label" isForPrompting="true" class="java.lang.String"/>
	<parameter name="Lyear" isForPrompting="true" class="java.lang.Integer"/>
	<queryString><![CDATA[select bentype.BEN_TYPE_ID,bentype.BEN_TYPE_DESC, sum(c.Quantity)/ $P{ftype} as Quan,sum(b.Collection)/ $P{ftype} as COLLECTION ,sum(b.dmd) / $P{ftype} AS DEMAND from 
(select BENEFICIARY_SNO,BENEFICIARY_TYPE_ID_sub
 from PMS_DCB_MST_BENEFICIARY
)a
left outer join 
(
select BENEFICIARY_SNO,
 (sum(COLN_CUR_YR_WC) + sum(COLN_MAINT) + sum(COLN_YESTER_YR_WC) + sum(MINUS_CHARGES_WC) + sum(MINUS_CHARGES_MAINT) - sum(ADD_CHARGES_MAINT) - sum(ADD_CHARGES_WC) ) AS Collection,
      sum(MONTH_BILL_AMT) as dmd
      from PMS_DCB_TRN_BILL_DMD
   WHERE   (to_date((01||'-'||BILL_MONTH||'-'||BILL_YEAR),'dd-MM-yyyy') >=to_date('1-4-'||$P{year1},'dd-mm-yyyy'))
  AND (to_date((01||'-'||BILL_MONTH||'-'||BILL_YEAR),'dd-MM-yyyy') <=to_date(31||'-'||3||'-'||$P{year2},'dd-mm-yyyy')) 
  group by BENEFICIARY_SNO
  )b
  on a.BENEFICIARY_SNO=b.BENEFICIARY_SNO
left outer join
(
select BENEFICIARY_SNO ,  sum (QTY_CONSUMED) as Quantity
  FROM PMS_DCB_WC_BILLING
   WHERE   (to_date((01||'-'||MONTH||'-'||YEAR),'dd-MM-yyyy') >=to_date('1-4-'||$P{year1},'dd-mm-yyyy'))
  AND (to_date((01||'-'||MONTH||'-'||YEAR),'dd-MM-yyyy') <=to_date(31||'-'||3||'-'||$P{year2},'dd-mm-yyyy')) 
  group by BENEFICIARY_SNO
  )c
  on a.BENEFICIARY_SNO=c.BENEFICIARY_SNO 
  LEFT JOIN
    ( SELECT BEN_TYPE_ID, BEN_TYPE_DESC FROM PMS_DCB_BEN_TYPE
    ) bentype
  ON bentype.BEN_TYPE_ID = a.BENEFICIARY_TYPE_ID_sub group by bentype.BEN_TYPE_ID, bentype.BEN_TYPE_DESC 
  order by bentype.BEN_TYPE_ID]]></queryString>

