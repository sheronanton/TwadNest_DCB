	<parameter name="mvalue" isForPrompting="true" class="java.lang.String"/>
	<queryString><![CDATA[SELECT  
   sch.SCH_NAME  as SCH_NAME ,
  sch.sch_sno,
  schtype.SCH_TYPE_DESC ,
  pr.net AS net,
  dv.OFFICE_NAME
FROM
   
  (SELECT SUM(QTY_CONSUMED_NET) AS net ,
    SCH_SNO,
    OFFICE_ID,
    MONTH,
    YEAR
  FROM PMS_DCB_TRN_MONTHLY_PR
  WHERE MONTH    =$P{month}
  AND YEAR       =$P{year}
  AND office_id IN
    ( SELECT OFFICE_ID FROM PMS_DCB_DIV_DIST_MAP
    )
  GROUP BY OFFICE_ID,
    MONTH,
    YEAR,
    SCH_SNO
  )pr
 
 
JOIN
  ( SELECT SCH_SNO, SCH_NAME, SCH_TYPE_ID FROM PMS_SCH_MASTER order by SCH_NAME
  )sch
ON sch.SCH_SNO=pr.SCH_SNO
JOIN
  ( SELECT SCH_TYPE_ID, SCH_TYPE_DESC FROM PMS_SCH_LKP_TYPE
  )schtype
ON schtype.SCH_TYPE_ID=sch.SCH_TYPE_ID
JOIN
  (SELECT OFFICE_ID,
    OFFICE_NAME,
    OFFICE_LEVEL_ID
  FROM COM_MST_ALL_OFFICES_VIEW
  WHERE OFFICE_LEVEL_ID='DN'
  )dv
ON dv.OFFICE_ID=pr.office_id
ORDER BY OFFICE_ID, UPPER(SCH_NAME )]]></queryString>

