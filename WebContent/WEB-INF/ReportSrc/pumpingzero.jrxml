	<queryString><![CDATA[SELECT b.BENEFICIARY_NAME,
  T.BEN_TYPE_DESC,
  met.METRE_LOCATION,
  sch.SCH_NAME,BEN_TYPE_ID
FROM
  (SELECT BENEFICIARY_SNO,
    METRE_SNO,
    SCH_SNO,
    OFFICE_ID
  FROM PMS_DCB_TRN_MONTHLY_PR
  WHERE QTY_CONSUMED_NET=0
  AND office_id         = $P{office_id}
  AND MONTH             =$P{month}
  AND YEAR              =$P{year}
  AND PROCESS_FLAG     ='FR'
  ) pum
JOIN
  (SELECT BENEFICIARY_SNO,
    BLOCK_SNO,
    BENEFICIARY_NAME,
    BENEFICIARY_TYPE_ID
  FROM PMS_DCB_MST_BENEFICIARY
  )b
ON b.BENEFICIARY_SNO=pum.BENEFICIARY_SNO
JOIN
  (SELECT SCHEME_SNO,
    METRE_LOCATION,
    OFFICE_ID,
    METRE_SNO,
    BENEFICIARY_SNO
  FROM PMS_DCB_MST_BENEFICIARY_METRE
  WHERE office_id = $P{office_id}
  AND METER_STATUS='L'
  )met
ON met.METRE_SNO       =pum.METRE_SNO
AND met.BENEFICIARY_SNO=b.BENEFICIARY_SNO
AND met.OFFICE_ID      =pum.OFFICE_ID
JOIN
  (SELECT BEN_TYPE_DESC,
    BEN_TYPE_ID,
    ADDRESS_TO
  FROM PMS_DCB_BEN_TYPE
  ORDER BY BEN_TYPE_ID
  ) T
ON T.BEN_TYPE_ID=b.BENEFICIARY_TYPE_ID
JOIN
  (SELECT SCH_SNO,SCH_NAME FROM PMS_SCH_MASTER
  )sch
ON sch.SCH_SNO =met.SCHEME_SNO
AND sch.SCH_SNO=pum.SCH_SNO 

order by BEN_TYPE_ID]]></queryString>

